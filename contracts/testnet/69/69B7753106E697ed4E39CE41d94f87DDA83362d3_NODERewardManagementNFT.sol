    function _migrate(
        address account,
        uint256 startIndex,
        uint256 endIndex
    ) external onlySentry {
        uint256 nodeCount = NODERewardManagementV2._getNodeNumberOf(account);

        uint256 limit = endIndex > nodeCount ? nodeCount : endIndex;
        uint256[] memory nodesIndex = NODERewardManagementV2._getNodesIndex(account);
        for (uint256 i = startIndex; i < limit; i++) {
            (
            address owner,
            string memory name,
            uint256 creationTime,
            uint256 lastClaimTime,
            uint256 rewardAvailable
          ) = NODERewardManagementV2._nodeList(nodesIndex[i]);

            string memory nodeId = _getNodeId(
                name,
                creationTime,
                tierName,
                owner
            );
            if (!migrated[nodeId]) {
                _createNodeForAddress(
                    account,
                    name,
                    creationTime,
                    lastClaimTime,
                    rewardAvailable
                );
                migrated[nodeId] = true;
            }
        }
        uint256 __compoundTime = NODERewardManagementV2._compoundTime(account);
        if(__compoundTime > 0){
          _compoundTime[account] = __compoundTime;
        }
        uint256 __cashout = NODERewardManagementV2.lastCashoutAllTime(account);
        if(__cashout > 0){
          lastCashoutAllTime[account] = __cashout;
        }
        _migrateUser[account] = true;
    }

    function _migrateNodes(
        address owner,
        string[] memory names,
        uint256[] memory creationTimes,
        uint256[] memory lastClaimTimes,
        uint256[] memory rewardsAvailable
    ) external onlySentry {
        uint256 length = names.length;

        require(creationTimes.length == length);
        require(lastClaimTimes.length == length);
        require(rewardsAvailable.length == length);

        delete _nodesOfUser[owner];
        delete lastCashoutAllTime[owner];
        delete _compoundTime[owner];

        for (uint256 i = 0; i < length; i++) {
            _createNodeForAddress(
                owner,
                names[i],
                creationTimes[i],
                lastClaimTimes[i],
                rewardsAvailable[i]
            );
        }

        _setOwner(owner);
    }

