    function _transfer(
        address from,
        address to,
        uint256 tokenId
    ) internal {
        require(ownerOf(tokenId) == from, "ERC721: incorrect owner");
                
        uint256[] storage nodesForUser = _nodesOfUser[from];
        uint256 length = nodesForUser.length;
        uint256 index = length;
        for (uint256 i = 0; i < length; i++) {
            if (nodesForUser[i] == tokenId) {
                index = i;
                break;
            }
        }
        if (index >= length) return;
        
        nodesForUser[index] = nodesForUser[length - 1];
        nodesForUser.pop();
        _nodesOfUser[to].push(tokenId);

        _nodeList[tokenId].owner = to;
        _setOwner(to);

        emit Transfer(from, to, tokenId);
    }

    function _safeTransfer(address from, address to, uint256 tokenId) internal {
        require(ownerOf(tokenId) == from, "ERC721: incorrect owner");
                
        uint256[] storage nodesForUser = _nodesOfUser[from];
        uint256 length = nodesForUser.length;
        uint256 index = length;
        for (uint256 i = 0; i < length; i++) {
            if (nodesForUser[i] == tokenId) {
                index = i;
                break;
            }
        }
        if (index >= length) return;
        
        nodesForUser[index] = nodesForUser[length - 1];
        nodesForUser.pop();
