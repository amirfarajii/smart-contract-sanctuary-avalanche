contract NODERewardManagementNFT is IERC721, IERC721Metadata {
    using SafeMath for uint256;

    struct NodeMeta {
        address owner;
        string name;
        string metadata;
        uint256 creationTime;
        uint256 lastClaimTime;
        uint256 rewardAvailable;
    }

    NodeMeta[] public _nodeList;
    mapping(address => uint256[]) public _nodesOfUser;
    mapping(address => uint256) public _compoundTime;
    mapping(address => uint256) public lastCashoutAllTime;
    mapping(address => bool) public _migrateUser;
    mapping(uint256 => address) private _tokenApprovals;
    mapping(address => mapping(address => bool)) private _operatorApprovals;
    mapping(address => bool) public sentry;

    mapping(string => bool) public migrated;
    mapping(address => bool) public nodeOwners;
    address[] public allNodeOwners;

    string public defaultUri = "";

    uint256 public nodePrice;
    uint256 public rewardPerNode;
    uint256 public claimTime;
    uint256 public cashOutFee;

    uint256 public totalNodesCreated = 0;
    uint256 public totalRewardStaked = 0;

    string public tierName;
    address public gateKeeper;

    IMasterOfCoin masterOfCoin =
    IMasterOfCoin(0x51328f822421B7169f2A6c98c3FAB08012b4E629);
    INodeRewardManagementV2 NODERewardManagementV2;
    INodeRewardManagement NODERewardManagementV1;

    bool internal creating = false;

    uint256[] private cashoutFeeDue = new uint256[](4);
    uint256[] private cashoutFeeTax = new uint256[](4);

    event CreateNode(
        address owner,
        string  name,
        uint256 creationTime,
        uint256 lastClaimTime,
        uint256 rewardAvailable
    );

    event Cashout(
        address owner,
        uint256 index
    );

    event CashoutAll(
        address owner
    );

    constructor(
        uint256 _nodePrice,
        uint256 _rewardPerDay,
        string memory _tierName,
        uint256 _cashOutFee
    ) {
        nodePrice = _nodePrice;
        rewardPerNode = _rewardPerDay.div(86400);
        claimTime = 1;
        tierName = _tierName;
        cashOutFee = _cashOutFee;
        sentry[msg.sender] = true;

        changeCashOutFeeTax(50, 40, 30, 20);
        changeCashOutFeeDue(604800, 1209600, 1814400, 2332800);
    }

    modifier onlySentry() {
        require(sentry[msg.sender], "Fuck off");
        _;
    }

    modifier createLock() {
        require(!creating, "Creating Lock");
        creating = true;
        _;
        creating = false;
    }

    modifier indexAvailable(uint256 index) {
        require(index >= 0 || index < _nodeList.length, "NODE: Index Error");
        _;
    }

    modifier onlyIfExists(uint256 _id) {
        require(_exists(_id), "ERC721: query for nonexistence");
        _;
    }

    //private
    function _createNodeForAddress(
        address owner,
        string memory name_,
        uint256 creationTime,
        uint256 lastClaimTime,
        uint256 rewardAvailable
    ) private createLock returns (uint256) {
        emit CreateNode(owner, name_, creationTime, lastClaimTime, rewardAvailable);
        _nodeList.push(
            NodeMeta({
                owner: owner,
                name: name_,
                metadata: "",
                creationTime: creationTime,
                rewardAvailable: rewardAvailable,
                lastClaimTime: lastClaimTime
            })
        );
        totalNodesCreated = _nodeList.length;
        _nodesOfUser[owner].push(totalNodesCreated - 1);
        _setOwner(owner);
        return totalNodesCreated - 1;
    }

    function _calculateUntaxedReward(uint256 nodeIndex)
    private
    view
    returns (uint256)
    {
        uint256 currentTime = block.timestamp;
        NodeMeta memory node = _nodeList[nodeIndex];
        if (currentTime == 0 || _isDelinquent(nodeIndex)) return 0;

        uint256 lastClaim = _getLastClaimTime(node.owner, node.lastClaimTime, node.creationTime);
        if (_compoundTime[node.owner] > lastClaim) {
            lastClaim = _compoundTime[node.owner];
        }
        uint256 claims = (currentTime.sub(lastClaim)).div(claimTime);
        uint256 totalRewards = rewardPerNode.mul(claims);
        return totalRewards;
    }

    function _calculateReward(uint256 nodeIndex)
    private
    view
    returns (uint256)
    {
        uint256 currentTime = block.timestamp;
        NodeMeta memory node = _nodeList[nodeIndex];

        if (currentTime == 0 || _isDelinquent(nodeIndex)) return 0;

        uint256 lastClaim = _getLastClaimTime(node.owner, node.lastClaimTime, node.creationTime);
        if (_compoundTime[node.owner] > lastClaim) {
            lastClaim = _compoundTime[node.owner];
        }
        uint256 claims = (currentTime.sub(lastClaim)).div(claimTime);
        uint256 fee = _calculateNodeTaxFee(nodeIndex, currentTime);
        uint256 totalRewards = rewardPerNode.mul(claims);
        uint256 cashoutFee = totalRewards.mul(fee).div(100);

        if (totalRewards > cashoutFee){
          return totalRewards.sub(cashoutFee);
        }
        return 0;
    }

    function _isNameAvailable(address account, string memory nodeName)
    private
    view
    returns (bool)
    {
        uint256[] memory nodesIndex = _nodesOfUser[account];
        for (uint256 i = 0; i < nodesIndex.length; i++) {
            if (
                keccak256(bytes(_nodeList[nodesIndex[i]].name)) ==
                keccak256(bytes(nodeName))
            ) {
                return false;
            }
        }
        return true;
    }

    function _isDelinquent(uint256 index) private view returns (bool) {
        string memory nodeId = _nodeIndexToId(index);
        return masterOfCoin.isDelinquent(nodeId);
    }

    function _setOwner(address owner) private {
        if (nodeOwners[owner]) {
            return;
        }

        nodeOwners[owner] = true;
        allNodeOwners.push(owner);
    }


    function _getNodeId(
        string memory _name,
        uint256 creationTime,
        string memory _tierName,
        address owner
    ) private pure returns (string memory) {
        return
        string(
            abi.encodePacked(
                _name,
                uint2str(creationTime),
                _tierName,
                toAsciiString(owner)
            )
        );
    }

    function _nodeIndexToId(uint256 index)
    private
    view
    returns (string memory)
    {
        NodeMeta memory node = _nodeList[index];
        return _getNodeId(node.name, node.creationTime, tierName, node.owner);
    }

    function _cashOutReward(uint256 index) private returns (uint256) {
        uint256 nodeReward = _calculateReward(index);
        // always update last claim time
        _nodeList[index].lastClaimTime = block.timestamp;
        return nodeReward;
    }

    function _calculateNodeTaxFee(uint256 index, uint256 endTime)
    private
    view
    returns (uint256)
    {
        for (uint256 i = 0; i < cashoutFeeDue.length; i++) {
            address owner = _nodeList[index].owner;
            uint256 _claimTime = _nodeList[index].lastClaimTime;
            uint256 createTime = _nodeList[index].creationTime;
            if (
                endTime - _getLastClaimTime(owner, _claimTime, createTime) <=
                cashoutFeeDue[i]
            ) {
                return cashoutFeeTax[i];
            }
        }

        return cashOutFee;
    }

    // public & external

    function createNodeForAddress(
        address owner,
        string memory _name,
        uint256 creationTime,
        uint256 lastClaimTime,
        uint256 rewardAvailable
    ) external onlySentry returns (uint256) {
        require(_isNameAvailable(owner, _name), "CN:Name not available");
        return
        _createNodeForAddress(
            owner,
            _name,
            creationTime,
            lastClaimTime,
            rewardAvailable
        );
    }

    function getRewardAmountOf(uint256 nodeIndex)
    external
    view
    returns (uint256)
    {
        return _calculateReward(nodeIndex);
    }

    function getRewardAmountOf(address account)
    external
    view
    returns (uint256)
    {
        uint256 totalReward = 0;
        uint256[] memory nodesIndex = _nodesOfUser[account];
        for (uint256 i = 0; i < nodesIndex.length; i++) {
            totalReward = totalReward.add(_calculateReward(nodesIndex[i]));
        }
        return totalReward;
    }

    function getUntaxedRewardAmountOf(address account)
    external
    view
    returns (uint256)
    {
        uint256 totalReward = 0;
        uint256[] memory nodesIndex = _nodesOfUser[account];
        for (uint256 i = 0; i < nodesIndex.length; i++) {
            totalReward = totalReward.add(_calculateUntaxedReward(nodesIndex[i]));
        }
        return totalReward;
    }

    function cashOutReward(address account, uint256 index)
    external
    onlySentry
    indexAvailable(index)
    returns (uint256)
    {
        NodeMeta storage node = _nodeList[index];
        require(account == node.owner, "Not node owner");
        emit Cashout(account, index);
        return _cashOutReward(index);
    }

    function _getLastClaimTime(address owner, uint256 _claimTime, uint256 createTime)
    internal
    view
    returns (uint256)
    {
        uint256 claimTime = _claimTime;
        if (createTime > claimTime) {
            claimTime = createTime;
        }

        return
        lastCashoutAllTime[owner] > claimTime
        ? lastCashoutAllTime[owner]
        : claimTime;
    }

    function cashOutAllReward(address _owner)
    external
    onlySentry
    returns (uint256)
    {
        uint256[] memory nodesIndex = _nodesOfUser[_owner];
        uint256 totalReward = 0;
        for (uint256 i = 0; i < nodesIndex.length; i++) {
            totalReward += _calculateReward(nodesIndex[i]);
        }
        lastCashoutAllTime[_owner] = block.timestamp;
        emit CashoutAll(_owner);
        return totalReward;
    }

    function getNodeId(uint256 index)
    external
    view
    indexAvailable(index)
    returns (string memory)
    {
        return _nodeIndexToId(index);
    }

    function _getNodeNumberOf(address _owner) public view returns (uint256) {
        return _nodesOfUser[_owner].length;
    }

    function _getNodeTaxFee(
        uint256 nodeIndex,
        uint256 endTime,
        uint256[] memory _cashoutFeeTax,
        uint256[] memory _cashoutFeeDue
    ) public view returns (uint256) {
        return _calculateNodeTaxFee(nodeIndex, endTime);
    }

    function updateCompoundTime(address account) external onlySentry {
        _compoundTime[account] = block.timestamp;
    }

    function _renameNode(address account, string memory name, uint256 index)
