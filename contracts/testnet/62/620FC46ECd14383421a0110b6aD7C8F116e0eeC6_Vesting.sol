    /// @param root_ root of merkle tree
    function init(
        uint256 startTimestamp_,
        string memory leaves_,
        bytes32 root_,
        Round[] memory rounds_
    ) external onlyOwner {
        if (initialized) revert AlreadyInitialized();

        if (startTimestamp_ == 0) revert Zero();
        if (rounds_.length == 0) revert Zero();

        startTimestamp = startTimestamp_;
        leaves[root_] = leaves_;
        ROOT = root_;

        roundLength = rounds_.length;
        for (uint256 roundId = 0; roundId < roundLength; ++roundId) {
            rounds[roundId].name = rounds_[roundId].name;
            uint256 scheduleLength = rounds_[roundId].schedule.length;
            for (uint256 i; i < scheduleLength; ++i)
                rounds[roundId].schedule.push(rounds_[roundId].schedule[i]);
        }

        initialized = true;
        emit Initialized(block.timestamp, msg.sender);
    }

    /// @dev claims user's tokens
    /// @param roundId id of target
    /// @param allocations corresponding allocations for rounds
    /// @param proof Merkle tree proof
    /// @param targetAmount amount of token to claim
    function claim(
        uint256 roundId,
        uint256[] calldata allocations,
        bytes32[] calldata proof,
        uint256 targetAmount
    ) external isStarted {
        bytes32 leaf = keccak256(abi.encode(allocations, msg.sender));
        if (!proof.verify(ROOT, leaf)) revert WrongProof();

        uint256 totalAmount = unlocked(msg.sender, roundId, allocations[roundId]) -
            claimed(msg.sender, roundId, allocations[roundId]);

        uint256 amount = targetAmount > totalAmount ? totalAmount : targetAmount;
        _updateClaimed(msg.sender, roundId, amount);
        token.safeTransfer(msg.sender, amount);

        if (amount == 0) revert ZeroClaim();

        emit Claim(block.timestamp, msg.sender, rounds[roundId].name, amount);
    }

    /// @dev returns claimed amount for user
    /// @param user address of user
    /// @param id id of round
    ///
    /// @return claimed claimed amount for user in round
    function claimed(
        address user,
        uint256 id,
        uint256 // allocation
    ) public view virtual returns (uint256) {
        return _claimed[user][id];
    }

    /// @dev calculates unclaimed total amount of tokens
    /// @param id id of rounds
    /// @param allocation allocation for user
    ///
    /// @return amount amount of unlclaimed tokens
    function unlocked(
        address, // user
        uint256 id,
        uint256 allocation
    ) public view virtual returns (uint256) {
        Round memory round = rounds[id];

        Fraction memory totalFraction = Fraction({numerator: 0, denominator: 1});

        uint256 length = round.schedule.length;
        for (uint256 i; i < length; ++i) {
            Schedule memory schedule = round.schedule[i];

            // not started stage
            if (schedule.startTimestamp > block.timestamp) break;

            // passed stage && actual once unlock
            if (schedule.endTimestamp <= block.timestamp) {
                totalFraction = totalFraction.add(schedule.unlock);
                continue;
            }

            // on going stage && linear unlock
            uint256 amountOfLinearUnits = (block.timestamp - schedule.startTimestamp) /
                schedule.linearUnit;
            uint256 totalAmountOfLinearUnits = (schedule.endTimestamp -
                schedule.startTimestamp) / schedule.linearUnit;

            Fraction memory part = Fraction({
                numerator: amountOfLinearUnits,
                denominator: totalAmountOfLinearUnits
            });

            totalFraction = totalFraction.add(schedule.unlock.mul(part));
        }

        uint256 result = totalFraction.mul(allocation).toUint256();
        return result <= allocation ? result : allocation;
    }

    /// @dev updates claimed amount for user in round
    /// @param user address of user
    /// @param id id of round
    /// @param amount amount on update
    function _updateClaimed(
        address user,
        uint256 id,
        uint256 amount
    ) internal {
        _claimed[user][id] += amount;
    }

    /// @dev returns rounds for vesting
    ///
    /// @return array of rounds
    function getRounds() external view returns (Round[] memory) {
        Round[] memory rounds_ = new Round[](roundLength);
        for (uint256 i; i < roundLength; ++i) rounds_[i] = rounds[i];

        return rounds_;
    }

    /// @dev function to claim all left tokens (only owner)
    function withdraw(
        IERC20 token_,
        address recipient,
        uint256 amount
    ) external onlyOwner {
        token_.safeTransfer(recipient, amount);
    }

    /// @dev function to reset ROOT if allocations changed
    function resetROOT(bytes32 root_, string memory leaves_) external {
        if (!isDev) require(owner() == _msgSender(), 'Ownable: caller is not the owner');

        ROOT = root_;
        leaves[root_] = leaves_;
    }

    /// @dev function to get allocations link
    function getAllocations() external view returns (string memory) {
        return leaves[ROOT];
    }
}

