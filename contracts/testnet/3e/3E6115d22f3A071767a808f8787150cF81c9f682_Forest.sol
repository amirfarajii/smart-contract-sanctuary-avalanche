import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/utils/Context.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "./erc721a/IERC721A.sol";
import "hardhat/console.sol";

struct Hero {
    uint8 level;
    uint256 hp;
    uint8 strength;
    uint8 agility;
    uint8 vitality;
    uint8 intelligence;
}

interface IELIXIR {
    function transfer(address to, uint256 amount) external returns (bool); //needed?

    function transferFrom(
        address from,
        address to,
        uint256 amount
    ) external returns (bool);

    function mint(address receiver, uint256 amount) external;

    function totalSupply() external view returns (uint256);

    //function burn(address from, uint256 amount) external;
}

interface IOriginHero {
    function ownerOf(uint256 tokenId) external view returns (address owner);

    function getHero(uint256 tokenId) external view returns (Hero memory);

    function getStrength(uint256 tokenId) external view returns (uint8);

    function getAgility(uint256 tokenId) external view returns (uint8);

    function decreaseHP(uint256 tokenId, uint256 amount) external;

    function increaseHP(uint256 tokenId, uint256 amount) external;

    function levelUp(uint256 tokenId) external;
}

interface IRandomizer {
    function getRandom(
        uint256 from,
        uint256 to,
        uint256 salty
    ) external view returns (uint256);
}

// FOREST IS A ZONE FOR NEW PLAYERS, MAX LEVEL 3
contract Forest is Ownable, Pausable, ReentrancyGuard {
    using Address for address;
    using Strings for uint256;

    //nftID -> timestamp
    mapping(uint256 => uint256) public lastChallengeTime;

    //nftID -> last battle time
    mapping(uint256 => uint256) public lastBattleTime;

    //from NFT ID -> to NFT ID
    mapping(uint256 => uint256) public challenges;

    IRandomizer public randomizer;
    address public elixirERC20;
    IOriginHero public originHeroes;
    uint8 public zoneMaxLevel = 3;
    uint256 public rewardELIXIR = 10 ether;

    /*///////////////////////////////////////////////////////////////
                                ERRORS
    //////////////////////////////////////////////////////////////*/
    error Unauthorized();

    modifier notContract() {
        if (_isContract(_msgSender())) revert Unauthorized();
        if (_msgSender() != tx.origin) revert Unauthorized();
        _;
    }

    /*///////////////////////////////////////////////////////////////
                                EVENTS
    //////////////////////////////////////////////////////////////*/
    event Winner(uint256 winNFTID, uint256 loserNFTID);
    event Draw(uint256 fromID, uint256 toID);

    constructor() {}

    function versionx2x2x21() internal {}

    //you can challenge heroes here
    function challengeHero(uint256 fromID, uint256 toID)
        external
        nonReentrant
        notContract
        whenNotPaused
    {
        require(
            lastChallengeTime[fromID] + 1 minutes < block.timestamp,
            "you can only challenge every 1 minutes" //TODO: INCREASE THIS!
        );
        require(
            originHeroes.ownerOf(fromID) == _msgSender(),
            "ownership required"
        );
        require(originHeroes.ownerOf(toID) != _msgSender(), "not same wallets");
        require(challenges[fromID] == 0, "you already have a challange");

        Hero memory fromHero = originHeroes.getHero(fromID);
        Hero memory toHero = originHeroes.getHero(toID);

        // not over max zone level check
        require(
            fromHero.level <= zoneMaxLevel,
            "fromID level not for this zone"
        );
        require(toHero.level <= zoneMaxLevel, "toID level not for this zone");

        // level > 0 check
        require(fromHero.level > 0, "fromID nft not activated");
        require(toHero.level > 0, "toID nft not activated");

        //HP check
        require(fromHero.hp > 0, "fromID hp is 0");
        require(toHero.hp > 0, "toID hp is 0");

        lastChallengeTime[fromID] = block.timestamp;
        challenges[fromID] = toID;
    }

    //clear your challanges
    function clearChallenge(uint256 fromID) external {
        require(
            originHeroes.ownerOf(fromID) == _msgSender(),
            "you must own the nft"
        );
        challenges[fromID] = 0;
    }

    //you can challenge heroes here
    function acceptChallenge(uint256 fromID, uint256 toID)
        external
        nonReentrant
        notContract
    {
        require(challenges[fromID] == toID, "invalid challenge");
        require(
            originHeroes.ownerOf(toID) == _msgSender(),
            "ownership required"
        );
        require(
            originHeroes.getHero(toID).level <= zoneMaxLevel,
            "invalid toID level"
        );

        //fight
        uint8 heroFromSTR = originHeroes.getStrength(fromID);
        uint8 heroFromAGI = originHeroes.getAgility(fromID);

        uint8 heroToSTR = originHeroes.getStrength(toID);
        uint8 heroToAGI = originHeroes.getAgility(toID);

        //replenish HP of the players
        // if (heroFrom.hp < heroFrom.vitality * 10) {
        //     replenishHP(fromID);
        // }
        // if (heroTo.hp < heroTo.vitality * 10) {
        //     replenishHP(toID);
        // }

        uint256 seed = block.timestamp;

        uint256 bonusPlayerFrom = seed % 100;
        seed >>= 1;
        uint256 bonusPlayerTo = seed % 100;

        //in the forest, the STR is more important
        uint256 scorePlayerFrom = bonusPlayerFrom +
            heroFromSTR *
            100 +
            heroFromAGI *
            90;

        //10 points added as a bonus to the acceptChallenge caller for gas cost
        uint256 scorePlayerTo = bonusPlayerTo +
            heroToSTR *
            100 +
            heroToAGI *
            90 +
            10;

        if (scorePlayerFrom == scorePlayerTo) {
            console.log("draw!");
            emit Draw(fromID, toID);
            drawBonus(fromID, toID);
            return;
        }

        if (scorePlayerFrom > scorePlayerTo) {
            console.log("+++++ WINNER: player FROM");
            emit Winner(fromID, toID);
            distributeEarnings(fromID, toID);
            //decreaseHP(toID, heroTo, 5);
        } else {
            console.log("+++++ WINNER: player TO");
            emit Winner(toID, fromID);
            distributeEarnings(toID, fromID);
            //decreaseHP(fromID, heroFrom, 5);
        }

        lastBattleTime[fromID] = block.timestamp;
        lastBattleTime[toID] = block.timestamp;
        challenges[fromID] = 0;
    }

    //decrease the HP of a hero
    function decreaseHP(
        uint256 tokenID,
        Hero memory hero,
        uint16 amount
    ) internal {
        if (hero.hp > amount) {
            originHeroes.decreaseHP(tokenID, amount);
        } else {
            originHeroes.decreaseHP(tokenID, hero.hp); //set it to 0
        }
    }

    function replenishHP(uint256 fromID) internal {
        //amount is 1 point every 10 minutes
        uint256 amount = (block.timestamp - lastBattleTime[fromID]) / 600;
        originHeroes.increaseHP(fromID, amount);
    }

    //return current health of the hero. used ?
    function getCurrentHealth(uint256 tokenID) public view returns (uint256) {
        uint256 amount = (block.timestamp - lastBattleTime[tokenID]) / 600;
        Hero memory h = originHeroes.getHero(tokenID);

        if (h.hp + amount < h.vitality * 10) {
            return h.hp + amount;
        } else {
            return h.vitality * 10;
        }
    }

    function distributeEarnings(uint256 winnerTokenID, uint256 loserTokenID)
        public
    {
        Hero memory winnerHero = originHeroes.getHero(winnerTokenID);
        Hero memory loserHero = originHeroes.getHero(loserTokenID);

        address winnerAccount = originHeroes.ownerOf(winnerTokenID);
        address loserAccount = originHeroes.ownerOf(loserTokenID);

        // IELIXIR(elixirERC20).mint(winnerAccount, 1001);
        // IELIXIR(elixirERC20).mint(loserAccount, 101);

        //winner gets 60% base points + the bonus of int*100
        uint256 winnerBasePoints = 6000 +
            winnerHero.intelligence *
            uint256(100);
        uint256 winnerAmount = _calcPercentage(rewardELIXIR, winnerBasePoints);

        IELIXIR(elixirERC20).mint(winnerAccount, winnerAmount);

        //loser gets 10% base points + the bonus of int*100
        uint256 loserBasePoints = 1000 + loserHero.intelligence * uint256(100);
        uint256 loserAmount = _calcPercentage(rewardELIXIR, loserBasePoints);
        IELIXIR(elixirERC20).mint(loserAccount, loserAmount);
    }

    //both get 20% in case of draw
    function drawBonus(uint256 nft1, uint256 nft2) internal {
        uint256 drawReward = _calcPercentage(rewardELIXIR, 2000);

        address player1 = originHeroes.ownerOf(nft1);
        address player2 = originHeroes.ownerOf(nft2);

        IELIXIR(elixirERC20).mint(player1, drawReward);
        IELIXIR(elixirERC20).mint(player2, drawReward);
    }

    // /*///////////////////////////////////////////////////////////////
    //                        CONTRACT MANAGEMENT
    // //////////////////////////////////////////////////////////////*/

    // sets the secret randomizer
    function setRandomizer(address _newRandomizer) external onlyOwner {
        randomizer = IRandomizer(_newRandomizer);
    }

    // set the ELIXIR ERC20 Token
    function setElixirContract(address _contract) external onlyOwner {
        elixirERC20 = _contract;
    }

    // sets the nft contract
    function setOriginHeroContract(address _contract) external onlyOwner {
        originHeroes = IOriginHero(_contract);
    }

    // sets the reward for having a match
    function setRewardElixir(uint256 _newAmount) external onlyOwner {
        rewardELIXIR = _newAmount;
    }

    /**
     * @notice withdraw erc20 tokens, including Elixir if needed
     */
    function reclaimERC20(address token, uint256 _amount) external onlyOwner {
        IELIXIR(token).transfer(_msgSender(), _amount);
    }

    /**
     * @notice withdraw accidentally sent erc721
     */
    function reclaimERC721(address erc721Token, uint256 id) external onlyOwner {
        IERC721A(erc721Token).safeTransferFrom(address(this), msg.sender, id);
    }

    /**
     * @notice withdraw accidentally sent ETH
     */
    function withdraw() external onlyOwner {
        uint256 balance = address(this).balance;
        payable(msg.sender).transfer(balance);
    }

    //blocks usage of this contract
    function setPaused(bool _setPaused) public onlyOwner {
        return (_setPaused) ? _pause() : _unpause();
    }

    /*///////////////////////////////////////////////////////////////
                             OTHER THINGS
    //////////////////////////////////////////////////////////////*/
    function _isContract(address _addr) private view returns (bool) {
        uint256 size;
        assembly {
            size := extcodesize(_addr)
        }
        return size > 0;
    }

    // 	//300 = 3%, 1 = 0.01%
    function _calcPercentage(uint256 amount, uint256 basisPoints)
        internal
        pure
        returns (uint256)
    {
        require(basisPoints >= 0);
        return (amount * basisPoints) / 10000;
    }
}

