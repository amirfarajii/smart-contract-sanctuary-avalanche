import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

import "hardhat/console.sol";


interface ZOOGZ {
    struct Zoog {
        string name;
        uint8 aggression;
        uint8 toughness;
        uint8 smarts;
        uint8 vitality;
    }
    // this is a public variable
    function MAX_LEVEL() external view returns(uint8);
    // from erc721
    function ownerOf(uint256 _id) external returns(address);
    // custom functions
    function setAggression(uint256 _id, uint8 _val) external;
    function setToughness(uint256 _id, uint8 _val) external;
    function setSmarts(uint256 _id, uint8 _val) external;
    function setVitality(uint256 _id, uint8 _val) external;
    function getZoogFromToken(uint256 tokenId) external returns (Zoog memory zoog);
}

interface ZungleItemShop {
    struct ZungleItem {
        uint256 id;
        uint256 price;
        uint256 xpPoints;
        bool aggression;
        bool toughness;
        bool smarts;
        bool vitality;
    }
    // from erc1155
    function balanceOf(address account, uint256 id) external view returns (uint256);
    // custom functions
    function getZungleItem(uint256 typeId) external returns(ZungleItem memory item);
    function batchBurnItems(address from, uint256[] calldata ids, uint256[] calldata amounts) external;
    
}

contract ZoogLeveler is AccessControl, ReentrancyGuard {
    using Strings for uint256;
    using SafeMath for uint256;

    // struct holding xp values for each stat
    struct XpStruct {
        uint24 aggressionXp;
        uint24 toughnessXp;
        uint24 smartsXp;
        uint24 vitalityXp;
        uint32 lastUpgradedTs;
        uint8 itemCount;
    }

    ZOOGZ public zoogz;
    ZungleItemShop public itemShop;

    bool public levelingOpen;
    uint256 public maxItemsPerDay;

    // needs to be preloaded
    mapping(uint8 => uint24) public levelToXp;
    mapping(uint256 => XpStruct) public idToXpStruct;
    mapping(uint256 => bool) public tokenInitialized;

     // roles
    bytes32 public constant SUPPORT_ROLE = keccak256("SUPPORT");

    // events
    event LevelSet(uint8[] levels, uint24[] values);
    event ZoogInitialized(uint256 tokenId);


    // CONSTRUCTOR

    constructor(address _zoogAddress, address _shopAddress){
        zoogz = ZOOGZ(_zoogAddress);
        itemShop = ZungleItemShop(_shopAddress);

        // intial role assignment to the owner
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _setupRole(SUPPORT_ROLE, _msgSender());

        levelingOpen = true;
        maxItemsPerDay = 10;

        // TODO: change these
        levelToXp[1] = 10;
        levelToXp[2] = 30;
        levelToXp[3] = 70;
        levelToXp[4] = 150;
        levelToXp[5] = 275;
        levelToXp[6] = 450;
    }

    // MODIFIERS

    modifier levelingIsOpen() {
        require(
            levelingOpen,
            "ZL: Leveling is not open"
        );
        _;
    }

    modifier initialized(uint256 tokenId){
        require(tokenInitialized[tokenId], "ZL: Zoog base levels have not been initialized");
        _;
    }

    // SETTERS

    function setZoogzAddress(address _addr) external onlyRole(SUPPORT_ROLE){
        zoogz = ZOOGZ(_addr);
    }

    function setItemShopAddress(address _addr) external onlyRole(SUPPORT_ROLE){
        itemShop = ZungleItemShop(_addr);
    }

    function openLeveling() external onlyRole(SUPPORT_ROLE){
        levelingOpen = true;
    }

    function closeLeveling() external onlyRole(SUPPORT_ROLE){
        levelingOpen = false;
    }

    function setMaxItemsPerDay(uint256 itemCount) external onlyRole(SUPPORT_ROLE){
        maxItemsPerDay = itemCount;
    }

    // populate xp thresholds saving calculations later
    function loadXpValues(uint8[] calldata levels, uint24[] calldata values) external onlyRole(SUPPORT_ROLE){
        require(levels.length == values.length, "ZL: array length mismatch");
        uint256 l = levels.length;
        for(uint8 i = 0; i < l; i++){
            levelToXp[levels[i]] = values[i];
        }
        emit LevelSet(levels, values);
    }

    function setLevelXp(uint8 level, uint24 xp) external onlyRole(SUPPORT_ROLE){
        levelToXp[level] = xp;
    }

    // initialize zoog stats
    function _initialize(uint256 tokenId) internal {
        require(zoogz.ownerOf(tokenId) == msg.sender, "ZL: Not zoog owner");
        if(!tokenInitialized[tokenId]){
            ZOOGZ.Zoog memory zoog = zoogz.getZoogFromToken(tokenId);

            idToXpStruct[tokenId] = XpStruct({
                aggressionXp: levelToXp[zoog.aggression],
                toughnessXp: levelToXp[zoog.toughness],
                smartsXp: levelToXp[zoog.smarts],
                vitalityXp: levelToXp[zoog.vitality],
                lastUpgradedTs: 0,
                itemCount: 0
            });

            tokenInitialized[tokenId] = true;
            emit ZoogInitialized(tokenId);
        }
    }

    function initialize(uint256[] calldata ids) external {
        uint256 l = ids.length;
        for(uint256 i = 0; i < l; i++){
            _initialize(ids[i]);
        }
    }



    // LEVELERS

    /**
     * make sure that zoog has initialized stats for level, only set once
     * reset item count and timestamp
     * get total xp from items to be used
     * add the total xp to the level's current xp
     * 
     */

    function _incrementAggressionLevel(uint256 tokenId, uint256 xpLevel) internal {
        uint8 currentLvl = zoogz.getZoogFromToken(tokenId).aggression;
        uint8 finalLvl;
        uint8 maxLvl = zoogz.MAX_LEVEL();
        for(uint8 i = currentLvl + 1; i <= maxLvl; i++){
            if(levelToXp[i] > xpLevel){
                break;
            }
            finalLvl = i;
        }
        zoogz.setAggression(tokenId, finalLvl);
    }

    function _incrementToughnessLevel(uint256 tokenId, uint256 xpLevel) internal {
        uint8 currentLvl = zoogz.getZoogFromToken(tokenId).toughness;
        uint8 finalLvl;
        uint8 maxLvl = zoogz.MAX_LEVEL();
        for(uint8 i = currentLvl + 1; i <= maxLvl; i++){
            if(levelToXp[i] > xpLevel){
                break;
            }
            finalLvl = i;
        }
        zoogz.setToughness(tokenId, finalLvl);
    }

    function _incrementSmartsLevel(uint256 tokenId, uint256 xpLevel) internal {
        uint8 currentLvl = zoogz.getZoogFromToken(tokenId).smarts;
        uint8 finalLvl;
        uint8 maxLvl = zoogz.MAX_LEVEL();
        for(uint8 i = currentLvl + 1; i <= maxLvl; i++){
            if(levelToXp[i] > xpLevel){
                break;
            }
            finalLvl = i;
        }
        zoogz.setSmarts(tokenId, finalLvl);
    }

    function _incrementVitalityLevel(uint256 tokenId, uint256 xpLevel) internal {
        uint8 currentLvl = zoogz.getZoogFromToken(tokenId).vitality;
        uint8 finalLvl;
        uint8 maxLvl = zoogz.MAX_LEVEL();
        for(uint8 i = currentLvl + 1; i <= maxLvl; i++){
            if(levelToXp[i] > xpLevel){
                break;
            }
            finalLvl = i;
        }
        zoogz.setVitality(tokenId, finalLvl);
    }

    function levelAggression(uint256 tokenId, uint256[] calldata itemIds, uint256[] calldata itemAmounts) external levelingIsOpen initialized(tokenId) nonReentrant{
        require(zoogz.ownerOf(tokenId) == msg.sender, "ZL: Not zoog owner");
       
        uint32 totalXp = 0;
        uint32 totalAmount = 0;

        // get total xp for items
        uint8 l = uint8(itemIds.length);
        for(uint256 i = 0; i < l; i++){
            uint256 amount = itemAmounts[i];
            require(amount > 0, "ZL: Must have an amount");
            require(itemShop.balanceOf(msg.sender, itemIds[i]) >= amount, "ZL: Not enough item balance");
            ZungleItemShop.ZungleItem memory item = itemShop.getZungleItem(itemIds[i]);
            // make sure it can be used for stat upgrade
            require(item.aggression, "ZL: Item cannot be used to level stat");
            totalAmount += uint32(amount);
            totalXp += uint32(amount * item.xpPoints);
        }

        // cooldown -- max x items per 24h
        // reset item count and timestamp if last timstamp was more than a day ago
        uint256 daysElapsed = (block.timestamp - idToXpStruct[tokenId].lastUpgradedTs) / 1 days;
        if(daysElapsed >= 1){
            idToXpStruct[tokenId].itemCount = 0;
            idToXpStruct[tokenId].lastUpgradedTs = uint32(block.timestamp);
        }

        idToXpStruct[tokenId].itemCount += uint8(totalAmount);
        require(idToXpStruct[tokenId].itemCount <= maxItemsPerDay, "ZL: Too many items");

        if(totalXp > 0){
            idToXpStruct[tokenId].aggressionXp += uint24(totalXp);
            _incrementAggressionLevel(tokenId, idToXpStruct[tokenId].aggressionXp);
        }

        // burn items
        itemShop.batchBurnItems(msg.sender, itemIds, itemAmounts);
    }

    function levelToughness(uint256 tokenId, uint256[] calldata itemIds, uint256[] calldata itemAmounts) external levelingIsOpen initialized(tokenId) nonReentrant{
        require(zoogz.ownerOf(tokenId) == msg.sender, "ZL: Not zoog owner");
       
