import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/token/common/ERC2981.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "hardhat/console.sol";

// token interface
interface MNKZ {
    function balanceOf(address account) external view returns (uint256);
    function burn(address _from, uint256 _amount) external;
}


contract ZungleItemShop is ERC1155Supply, AccessControl, ERC2981 {
    using Strings for uint256;
    using SafeMath for uint256;

    /**
     * types of items
     * base items -- lvl up any stat
     * advanved items -- lvl up certain stats, but more xp
     * cratez -- opened to give variety of stats
     * keez - open cratez
     */

    struct ZungleItem {
        uint256 id; // type id -- do we need this? i guess if we dont want to check valid types TODO
        uint256 price; // token cost in mnkz without decimals
        uint256 xpPoints; // xp given when burned, not all will have this, but most will
        bool aggression; // bool for each representing upgrades
        bool toughness;
        bool smarts;
        bool vitality;
    }

    // so markets know us by name
    string public name = "ZungleItemShop";
    string public symbol = "ZIS";

    bool public shopOpen = false;

    address public tokenAddress; // mnkz address for token burn
    string private _baseURIExtended;

    uint96 public tokenRoyalties = 500; // 5% royalty
    address public royaltyPayout = 0xB090fc5D0C06804dfbf06efb59Ce1F291f13162a; // royalty address

    // types must start at 1
    mapping(uint256 => bool) public validItemTypes;
    mapping(uint256 => ZungleItem) public typeToItem;
    mapping(uint256 => bool) public userMintTypes; // items users can mint

    // used in querying
    uint256[] public shopItems; // array of storefront items
    uint256[] public allItems; // array of all items
    mapping(uint256 => uint256) public shopItemToIndex;
    mapping(uint256 => uint256) public allItemToIndex;

    // roles
    bytes32 public constant SUPPORT_ROLE = keccak256("SUPPORT");
    bytes32 public constant BURNER_ROLE = keccak256("BURNER");
    bytes32 public constant SPECIAL_MINTER = keccak256("SPECIAL_MINTER"); // oooh special

    // events
    event ItemAdded(uint256 indexed id);
    event ItemRemoved(uint256 indexed id);
    event ItemMinted(uint256 indexed id, uint256 amount);
    event ItemsMinted(uint256[] ids, uint256[] amounts);
    event ItemBurned(uint256 indexed id, uint256 amount);

    constructor(string memory baseUri, address _tokenAddress) ERC1155(baseUri){
        _baseURIExtended = baseUri;
        tokenAddress = _tokenAddress;

        // intial role assignment to the owner
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _setupRole(SUPPORT_ROLE, _msgSender());
    }

    // MODIFIERS

    modifier shopIsOpen() {
        require(
            shopOpen,
            "ZS: Shop is not open"
        );
        _;
    }

    modifier validItem(uint256 typeId){
        require(validItemTypes[typeId], "ZS: Type is invalid");
        _;
    }

    modifier callerIsUser() {
        require(
            tx.origin == msg.sender,
            "ZS: The caller is another contract"
        );
        _;
    }

    // GETTERS

    function getZungleItem(uint256 typeId) external view validItem(typeId) returns(ZungleItem memory){
        return typeToItem[typeId];
    }

    // SETTERS

    function openShop() external onlyRole(SUPPORT_ROLE){
        shopOpen = true;
    }

    function closeShop() external onlyRole(SUPPORT_ROLE){
        shopOpen = false;
    }

    function setTokenAddress(address _tokenAddress) external onlyRole(SUPPORT_ROLE) {
        tokenAddress = _tokenAddress;
    }

    function updateBaseUri(string memory baseUri) external onlyRole(SUPPORT_ROLE) {
        _baseURIExtended = baseUri;
    }

    function setVaidItemType(uint256 _typeId) external onlyRole(SUPPORT_ROLE){
        validItemTypes[_typeId] = true;
    }

    // emergency functions
    function setAllItems(uint256[] calldata items) external onlyRole(SUPPORT_ROLE){
        allItems = items;
    }

    function setShopItems(uint256[] calldata items) external onlyRole(SUPPORT_ROLE){
        shopItems = items;
    }

    function setAllItemIndex(uint256 typeId, uint256 index) external validItem(typeId) onlyRole(SUPPORT_ROLE){
        allItemToIndex[typeId] = index;
    }

    function setShopItemIndex(uint256 typeId, uint256 index) external validItem(typeId) onlyRole(SUPPORT_ROLE){
        shopItemToIndex[typeId] = index;
    }

    // ideally never called, but just in case
    function removeItem(uint256 typeId) external validItem(typeId) onlyRole(DEFAULT_ADMIN_ROLE){
        require(totalSupply(typeId) == 0, "ZS: Type already has tokens minted");
        validItemTypes[typeId] = false;

       if(userMintTypes[typeId]){
            // remove from storefront
            for(uint256 i = shopItemToIndex[typeId]; i < shopItems.length - 1; i++){
                shopItems[i] = shopItems[i+1];
            }
            shopItemToIndex[typeId] = 0;
            delete shopItems[shopItems.length - 1];
        }

        // remove from all items
        for(uint256 i = allItemToIndex[typeId]; i < allItems.length - 1; i++){
            allItems[i] = allItems[i+1];
        }
        allItemToIndex[typeId] = 0;
        delete allItems[allItems.length - 1];
        delete typeToItem[typeId];
        emit ItemRemoved(typeId);
    }

    function addItem(uint256 typeId, uint256 price, uint256 xpPoints, bool aggression, bool toughness, bool smarts, bool vitality, bool userMint) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(!validItemTypes[typeId], "ZS: Item already exists"); // TODO: change
        typeToItem[typeId] = ZungleItem({
                id: typeId,
                price: price,
                xpPoints: xpPoints,
                aggression: aggression,
                toughness: toughness,
                smarts: smarts,
                vitality: vitality
        });
        validItemTypes[typeId] = true; // do we need?

        // allow users to mint
        userMintTypes[typeId] = userMint;
        if(userMint){
            shopItems.push(typeId);
            shopItemToIndex[typeId] = shopItems.length - 1; // set index for lookup
        }
        allItems.push(typeId);
        allItemToIndex[typeId] = allItems.length - 1; // set index for lookup
        emit ItemAdded(typeId);
    }

    // set item parameters

    function setItemPrice(uint256 typeId, uint256 price) external validItem(typeId) onlyRole(SUPPORT_ROLE) {
        typeToItem[typeId].price = price;
    }

    function setItemXp(uint256 typeId, uint256 xp) external validItem(typeId) onlyRole(SUPPORT_ROLE) {
        typeToItem[typeId].xpPoints = xp;
    }

    function setItemAggression(uint256 typeId, bool value) external validItem(typeId) onlyRole(SUPPORT_ROLE) {
        typeToItem[typeId].aggression = value;
    }

    function setItemToughness(uint256 typeId, bool value) external validItem(typeId) onlyRole(SUPPORT_ROLE) {
        typeToItem[typeId].toughness = value;
    }

    function setItemSmarts(uint256 typeId, bool value) external validItem(typeId) onlyRole(SUPPORT_ROLE) {
        typeToItem[typeId].smarts = value;
    }

    function setItemVitality(uint256 typeId, bool value) external validItem(typeId) onlyRole(SUPPORT_ROLE) {
        typeToItem[typeId].vitality = value;
    }

    function setItemMintableByUser(uint256 typeId, bool value) external validItem(typeId) onlyRole(SUPPORT_ROLE) {
        userMintTypes[typeId] = value;
    }


    // URI

    function uri(uint256 typeId)
        public
        view                
        override
        validItem(typeId)
        returns (string memory)
    {
        return
            bytes(_baseURIExtended).length > 0
                ? string(abi.encodePacked(_baseURIExtended, typeId.toString()))
                : _baseURIExtended;
    }

    // MINT

    // internal mint function
    function _mintItem(address user, uint256 typeId, uint256 amount) internal validItem(typeId) {
        _mint(user, typeId, amount, "");
        emit ItemMinted(typeId, amount);
    }

    function _mintBatch(address user, uint256[] calldata typeIds, uint256[] calldata amounts) internal {
        _mintBatch(user, typeIds, amounts, "");
        emit ItemsMinted(typeIds, amounts);
    }

    function mintSpecial(address user, uint256 typeId, uint256 amount) external onlyRole(SPECIAL_MINTER){
        _mintItem(user, typeId, amount);
    }
    
    function buyItemFromShop(uint256 typeId, uint256 amount) external shopIsOpen callerIsUser {
        MNKZ mnkz = MNKZ(tokenAddress);
        uint256 totalCost = typeToItem[typeId].price * amount;
        require(userMintTypes[typeId], "ZS: Item cannot be bought from store");
        // balance check with decimals accounted for
        require(mnkz.balanceOf(msg.sender) / 10**18 >= totalCost, "ZS: Not enough tokens to purchase");

        // mint items
        _mintItem(msg.sender, typeId, amount);

        // burn mnkz -- token burn handles decimals
        mnkz.burn(msg.sender, totalCost);
    }

    function batchBuyItemsFromShop(uint256[] calldata typeIds, uint256[] calldata amounts) external shopIsOpen callerIsUser {
        MNKZ mnkz = MNKZ(tokenAddress);
        uint256 totalCost = 0;
        for(uint256 i = 0; i < typeIds.length; i++){
            require(amounts[i] > 0, "ZS: Must buy at least 1 item");
            uint256 typeId = typeIds[i];
            require(validItemTypes[typeId], "ZS: Item is not valid");
            require(userMintTypes[typeId], "ZS: Item cannot be bought from store");
            // add price to total cost
            totalCost += typeToItem[typeId].price * amounts[i];
        }
        // balance check with decimals accounted for
        require(mnkz.balanceOf(msg.sender) / 10**18 > totalCost, "ZS: Not enough tokens to purchase");
        _mintBatch(msg.sender, typeIds, amounts, "");

        // burn mnkz -- token burn handles decimals 
        mnkz.burn(msg.sender, totalCost);
    }


    // BURN

    // internal burn function
    function _burnItem(address user, uint256 typeId, uint256 amount) internal validItem(typeId) {
        _burn(user, typeId, amount);
    }

    // burn one item
    // only by allowed burners
    function burnItem(address from, uint256 typeId, uint256 amount) external onlyRole(BURNER_ROLE) {
        _burnItem(from, typeId, amount);
    }

    // burn multiple
    function batchBurnItems(address from, uint256[] calldata ids, uint256[] calldata amounts) external onlyRole(BURNER_ROLE){
        _burnBatch(from, ids, amounts);
    }



    // ROYALTIES

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC1155, ERC2981, AccessControl)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    function setTokenRoyalties(uint96 _royalties) external onlyRole(SUPPORT_ROLE) {
        tokenRoyalties = _royalties;
        _setDefaultRoyalty(royaltyPayout, tokenRoyalties);
    }

    function setRoyaltyPayoutAddress(address _payoutAddress)
        external
        onlyRole(SUPPORT_ROLE)
    {
        royaltyPayout = _payoutAddress;
        _setDefaultRoyalty(royaltyPayout, tokenRoyalties);
    }

}

