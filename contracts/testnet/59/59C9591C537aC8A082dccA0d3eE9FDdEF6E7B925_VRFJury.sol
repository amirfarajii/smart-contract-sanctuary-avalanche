    constructor(
        address _vrfCoordinatorV2,
        bytes32 _keyHash,
        uint64 subId,
        uint32 _callbackGasLimit
    ) VRFConsumerBaseV2(_vrfCoordinatorV2) {
        i_VrfCoordinatorV2 = VRFCoordinatorV2Interface(_vrfCoordinatorV2);
        i_keyHash = _keyHash;
        _subId = subId;
        i_callbackGasLimit = _callbackGasLimit;
