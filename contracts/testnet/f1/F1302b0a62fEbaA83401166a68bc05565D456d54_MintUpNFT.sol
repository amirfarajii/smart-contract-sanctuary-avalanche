//   MIT
contract MintUpNFT is
    ERC721Enumerable,
    Ownable,
    ERC2981PerTokenRoyalties,
    Initializable
{
    using Strings for uint256;
    using SafeMath for uint256;

    uint256 public constant ROYALTY_VALUE = 500; // 100 = 1%, 200 = 2% etc...  5% for this launchpad
    uint256 public constant MAX_PER_CLAIM = 10;

    //
    // SALE
    //

    IAdmin public admin;

    struct Participation {
        uint256 numberNFTsBought;
        uint256 amountAVAXPaid;
        uint256 timeParticipated;
    }

    struct Sale {
        bool isCreated;
        address saleOwner;
        uint256 saleTimeStarts;
        uint256 saleTimeEnds;
        uint256 launchPublicPrice;
        uint256 numberOfNFTsToSell;
        uint256 totalNFTsSold;
        uint256 totalAVAXRaised;
        uint256 mintupPart;
    }

    struct WhitelistRights {
        uint256 quantity;
        uint256 timeBeforeStart;
        uint256 discountPrice;
    }

    Sale public sale;

    // whitelist (address, quantity) of project owned adresses who can mint any time
    mapping(address => WhitelistRights) public whitelist;

    // whitelist (address, quantity) adresses that can mint at {premintTime}
    mapping(address => uint256) premints;

    // premint timestamp, usually few hours before {startingPriceMint}
    // number of participants
    uint256 public numberOfParticipants;

    // royalties (address, percentage)
    mapping(address => uint256) royalties;

    // User participated
    mapping(address => bool) public isParticipated;

    // Mapping user to his participation
    mapping(address => Participation) public userToParticipation;

    // tokenID mapping
    mapping(uint256 => uint256) indexer;

    mapping(address => uint256) mainMintCount;
    uint256 indexerLength;
    mapping(uint256 => uint256) tokenIDMap;
    mapping(uint256 => uint256) takenImages;
    string baseUri = "";
    uint256 public minted = 0;

    // if false then mint will be sequential;
    bool random = true;
    uint256 gross = 0;
    mapping(address => bool) admins;
    address royaltyRecipient;

    uint256 public mintUpFees;

    event Claim(uint256 indexed _id);
    event Premint(uint256 indexed _id);

    constructor(
        string memory nftName,
        string memory nftSymbol,
        string memory baseTokenURI,
        address _royaltyRecipient,
        bool _random
    ) ERC721(nftName, nftSymbol) {
        royaltyRecipient = _royaltyRecipient;
        baseUri = baseTokenURI;
        random = _random;
    }

    modifier onlySaleOwner() {
        require(msg.sender == sale.saleOwner, "Restricted to sale owner.");
        _;
    }

    modifier onlyAdmin() {
        require(admins[msg.sender], "Restricted to admins.");
        _;
    }

    function initialize(address _admin) public initializer {
        require(_admin != address(0));
        admin = IAdmin(_admin);
        admins[_admin] = true;
    }

    function setSaleParams(
        address _saleOwner,
        uint256 _saleTimeStarts,
        uint256 _saleTimeEnds,
        uint256 _launchPublicPrice,
        uint256 _numberOfNFTsToSell,
        uint256 _mintupPart
    ) public onlyAdmin {
        require(
            _saleTimeStarts > block.timestamp,
            "saleTimeStarts has to be in the future"
        );
        require(
            _saleTimeEnds > block.timestamp,
            "saleTimeEnds has to be in the future"
        );
        require(
            _saleTimeEnds > _saleTimeStarts,
            "saleTimeEnds has to be in the future"
        );
        require(
            _launchPublicPrice > 0.1 ether,
            "LaunchPublicPrice has to be in the future"
        );
        require(sale.isCreated == false, "Sale is already created");
        require(_numberOfNFTsToSell > 0, "_numberOfNFTsToSell has to be > 0");
        require(_mintupPart > 0, "_mintupPart has to be > 0 ");
        sale.saleOwner = _saleOwner;
        sale.saleTimeStarts = _saleTimeStarts;
        sale.saleTimeEnds = _saleTimeEnds;
        sale.launchPublicPrice = _launchPublicPrice;
        sale.numberOfNFTsToSell = _numberOfNFTsToSell;
        sale.totalNFTsSold = 0;
        sale.totalAVAXRaised = 0;
        sale.mintupPart = _mintupPart;
        sale.isCreated = true;
        indexerLength = _numberOfNFTsToSell;
    }

    function participate() external payable {
        require(msg.sender == tx.origin, "Only direct calls.");

        // Check user haven't participated before
        require(!isParticipated[msg.sender], "Already participated.");

        // Compute the number of nfts msg.value is buying
        uint256 numberOfNFTsBuying = (msg.value).div(sale.launchPublicPrice);

        // Must buy more than 0 tokens
        require(numberOfNFTsBuying > 0, "Can't buy 0 tokens");

        // Require that amountOfTokensBuying is less than sale token leftover cap
        require(
            numberOfNFTsBuying <=
                sale.numberOfNFTsToSell.sub(sale.totalNFTsSold),
            "Not enough tokens to sell."
        );

        // Increase amount of sold tokens
        sale.totalNFTsSold = sale.totalNFTsSold.add(numberOfNFTsBuying);

        // Increase amount of AVAX raised
        //sale.nft_collection.earlyClaim{value: amountAVAX}(numberOfNFTsBuying);

        uint256 total_cost = (sale.launchPublicPrice * numberOfNFTsBuying);
        uint256 fee = total_cost.mul(sale.mintupPart).div(100);
        mintUpFees = mintUpFees.add(fee);
        total_cost = total_cost.sub(fee);
        gross += total_cost;
        sale.totalAVAXRaised = sale.totalAVAXRaised.add(total_cost + fee);

        uint256 excess = msg.value - (total_cost + fee);
        payable(address(this)).transfer(total_cost + fee);

        claim(numberOfNFTsBuying, msg.value);
        if (excess > 0) {
            payable(_msgSender()).transfer(excess);
        }

        // Create participation object
        Participation memory p = Participation({
            numberNFTsBought: numberOfNFTsBuying,
            amountAVAXPaid: msg.value,
            timeParticipated: block.timestamp
        });

        // Add participation for msg.value.
        userToParticipation[msg.sender] = p;
        // Mark user is participated
        isParticipated[msg.sender] = true;
        // Increment number of participants in the Sale.
        numberOfParticipants++;
    }

    //
    // END SALE
    //

    /////////////////////////////////////////////////////////
    //
    //
    //                      RANDOM MINT PROCESS
    //
    //
    //

    // Think of it as an array of 100 elements, where we take
    //    a random index, and then we want to make sure we don't
    //    pick it again.
    // If it hasn't been picked, the mapping points to 0, otherwise
    //    it will point to the index which took its place
    function getNextImageID(uint256 index) internal returns (uint256) {
        uint256 nextImageID = indexer[index];

        // if it's 0, means it hasn't been picked yet
        if (nextImageID == 0) {
            nextImageID = index;
        }
        // Swap last one with the picked one.
        // Last one can be a previously picked one as well, thats why we check
        if (indexer[indexerLength - 1] == 0) {
            indexer[index] = indexerLength - 1;
        } else {
            indexer[index] = indexer[indexerLength - 1];
        }
        indexerLength -= 1;
        return nextImageID;
    }

    function enoughRandom() internal view returns (uint256) {
        if (sale.numberOfNFTsToSell - minted == 0) return 0;
        return (uint256(
            keccak256(
                abi.encodePacked(
                    block.difficulty,
                    block.timestamp,
                    msg.sender,
                    blockhash(block.number)
                )
            )
        ) % (indexerLength));
    }

    function randomMint(address receiver, uint256 nextTokenIndex) internal {
        uint256 nextIndexerId = enoughRandom();
        uint256 nextImageID = getNextImageID(nextIndexerId);

        assert(takenImages[nextImageID] == 0);
        takenImages[nextImageID] = 1;
        tokenIDMap[nextTokenIndex] = nextImageID;
        _safeMint(receiver, nextTokenIndex);
    }

    function sequentialMint(address receiver, uint256 id) internal {
        takenImages[id] = 1;
        tokenIDMap[id] = id;
        _safeMint(receiver, id);
    }

    /////////////////////////////////////////////////////////
    //
    //
    //                      WHITELIST MINT
    //
    //
    //

    function whitelistMint(uint256 n) public payable {
        require(
            whitelist[msg.sender].quantity > 0,
            "You have no permission to mint"
        );
        require(
            (sale.saleTimeStarts - whitelist[msg.sender].timeBeforeStart) <=
                block.timestamp,
            "Too early to mint"
        );
        require(n > 0, "Number need to be higher than 0");
        require(n <= MAX_PER_CLAIM, "Max per claim is 10");

        require(
            msg.value >= (whitelist[msg.sender].discountPrice * n),
            "Ether value sent is below the price."
        );
        require(
            block.timestamp >= sale.saleTimeStarts,
            "Sale has not started yet"
        );
        require(sale.saleTimeEnds > block.timestamp, "Sale has ended");

        require(msg.sender == tx.origin, "Only direct calls.");
        // Check user haven't participated before

        // Compute the number of nfts msg.value is buying

        // Must buy more than 0 tokens
        require(n > 0, "Can't buy 0 tokens");

        // Increase amount of sold tokens
        sale.totalNFTsSold = sale.totalNFTsSold.add(n);

        // Increase amount of AVAX raised
        mainMintCount[msg.sender] += n;
        uint256 total_cost = (whitelist[msg.sender].discountPrice * n);
        uint256 fee = total_cost.mul(sale.mintupPart).div(100);
        mintUpFees = mintUpFees.add(fee);
        total_cost = total_cost.sub(fee);
        gross += total_cost;

        require(
            msg.value >= total_cost + fee,
            "The sent value (+fee) is to low "
        );
        uint256 excess = msg.value - (total_cost + fee);

        sale.totalAVAXRaised = sale.totalAVAXRaised.add((total_cost + fee));

        payable(address(this)).transfer((total_cost + fee));
        for (uint256 i = 0; i < n; i++) {
            if (random) {
                randomMint(tx.origin, minted);
            } else {
                sequentialMint(tx.origin, minted);
            }
            _setTokenRoyalty(minted, royaltyRecipient, ROYALTY_VALUE);
            minted += 1;
            emit Premint(minted);
        }
        if (excess > 0) {
            payable(_msgSender()).transfer(excess);
        }
        whitelist[msg.sender].quantity -= n;
    }

    /////////////////////////////////////////////////////////
    //
    //
    //                      PREMINT
    //
    //
    //
    function timestamp() public view returns (uint256) {
        return block.timestamp;
    }

    //MM: erc721 non adaptée pr mint en masse
    function premint(uint256 n) external {
        require(premints[msg.sender] > 0, "You have no preminting rihgts");
        require(n > 0, "Number need to be higher than 0");
        require(n <= MAX_PER_CLAIM, "Max per claim is 10");

        for (uint256 i = 0; i < n; i++) {
            if (random) {
                randomMint(tx.origin, minted);
            } else {
                sequentialMint(tx.origin, minted);
            }
            minted += 1;
            emit Premint(minted);
        }
        premints[msg.sender] -= n;
    }

    /////////////////////////////////////////////////////////
    //
    //
    //                      PUBLIC MINT
    //
    //
    //
    function claim(uint256 n, uint256 amountAvax) internal {
        require(
            n + minted <= sale.numberOfNFTsToSell,
            "Not enough left to mint."
        );
        require(n > 0, "Number need to be higher than 0");

        require(n <= MAX_PER_CLAIM, "Max per claim is 10");
        require(
            amountAvax >= (sale.launchPublicPrice * n),
            "Ether value sent is below the price"
        );

        require(
            block.timestamp >= sale.saleTimeStarts,
            "Sale has not started yet"
        );
        require(sale.saleTimeEnds > block.timestamp, "Sale has ended");

        mainMintCount[msg.sender] += n;

        for (uint256 i = 0; i < n; i++) {
            if (random) {
                randomMint(tx.origin, minted);
            } else {
                sequentialMint(tx.origin, minted);
            }
            _setTokenRoyalty(minted, royaltyRecipient, ROYALTY_VALUE);

            minted += 1;
            emit Claim(minted);
        }
    }

    /////////////////////////////////////////////////////////
    //
    //
    //                      Transfers
    //
    //
    //

    function transferFrom(
        address from,
        address to,
        uint256 tokenId
    ) public virtual override {
        require(
            _isApprovedOrOwner(_msgSender(), tokenId),
            "ERC721: transfer caller is not owner nor approved"
        );

        _transfer(from, to, tokenId);
    }

    function safeTransferFrom(
        address from,
        address to,
        uint256 tokenId
    ) public virtual override {
        safeTransferFrom(from, to, tokenId, "");
    }

    function safeTransferFrom(
        address from,
        address to,
        uint256 tokenId,
        bytes memory _data
    ) public virtual override {
        require(
            _isApprovedOrOwner(_msgSender(), tokenId),
            "ERC721: transfer caller is not owner nor approved"
        );

        _safeTransfer(from, to, tokenId, _data);
    }

    /////////////////////////////////////////////////////////
    //
    //
    //                       ERC
    //
    //
    //
    function tokenURI(uint256 tokenId)
        public
        view
        virtual
        override
        returns (string memory)
    {
        require(
            _exists(tokenId),
            "ERC721Metadata: URI query for nonexistent token"
        );
        string memory baseURI = _baseURI();
        uint256 imageID = tokenIDMap[tokenId];
        return
            bytes(baseURI).length > 0
                ? string(abi.encodePacked(baseURI, imageID.toString()))
                : "";
    }

    function _baseURI() internal view override returns (string memory) {
        return baseUri;
    }

    /// @inheritdoc	ERC165
    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(ERC2981Base, ERC721Enumerable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    /////////////////////////////////////////////////////////
    //
    //
    //                      UTILITIES
    //
    //
    //
    function withdraw() external {
        //admin has to be added by mintup stuff to allow withdraw
        require(_msgSender() == sale.saleOwner, "Your are not the sale owner");
        require(
            address(this).balance >= (sale.totalAVAXRaised - mintUpFees),
            "Nothing to withdraw"
        );
        payable(_msgSender()).transfer((sale.totalAVAXRaised - mintUpFees));
    }

    function withdrawMintupFees() external {
        require(admins[_msgSender()] == true, "Your are not an admin");
        require(mintUpFees > 0, "Nothing to withdraw");
        require(address(this).balance >= mintUpFees, "Balance is insufficient");
        payable(_msgSender()).transfer(mintUpFees);
        sale.totalAVAXRaised -= mintUpFees;
        mintUpFees = 0;
    }

    /////////////////////////////////////////////////////////
    //
    //
    //                      SETTERS
    //
    //
    //

    function setAdmins(address[] calldata _addr) external onlyAdmin {
        for (uint256 i = 0; i < _addr.length; i++) {
            admins[_addr[i]] = true;
        }
    }

    function setRoyaltyAddress(address _royaltyRecipient)
        external
        onlySaleOwner
    {
        royaltyRecipient = _royaltyRecipient;
    }

    function setBaseUri(string memory uri) external onlySaleOwner {
        baseUri = uri;
    }

    function setPremintAddress(address _address, uint256 allowedToMint)
        external
        onlySaleOwner
    {
        premints[_address] = allowedToMint;
    }

    function setWhiteListAddress(
        address[] calldata _address,
        uint256[] calldata allowedToMint,
        uint256[] calldata timeBeforeStart,
        uint256[] calldata discountPrices
    ) external onlySaleOwner {
        require(
            _address.length == allowedToMint.length &&
                allowedToMint.length == timeBeforeStart.length &&
                timeBeforeStart.length == discountPrices.length,
            "Arrays have to have same size"
        );
        for (uint256 i = 0; i < _address.length; i++) {
            whitelist[_address[i]] = WhitelistRights(
                allowedToMint[i],
                timeBeforeStart[i],
                discountPrices[i]
            );
        }
    }

    //MM: doublon avec setPremintAddress
    function removePremintAddress(address _address) external onlySaleOwner {
        premints[_address] = 0;
    }

    /////////////////////////////////////////////////////////
    //
    //
    //                      GETTERS
    //
    //
    //

    function getAdmins(address _addr) public view onlyOwner returns (bool) {
        return admins[_addr];
    }

    function isAllowedToPremint(address _premintAddress)
        public
        view
        returns (uint256)
    {
        uint256 userPremintAllowed = premints[_premintAddress];
        return userPremintAllowed;
    }

    function mainMintCountResult(address addr) public view returns (uint256) {
        uint256 count = mainMintCount[addr];
        return count;
    }

    function balance() public view returns (uint256) {
        return address(this).balance;
    }

    receive() external payable {}

    fallback() external payable {}
}