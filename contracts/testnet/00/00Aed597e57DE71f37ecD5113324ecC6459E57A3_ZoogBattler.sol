pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";
import "@chainlink/contracts/src/v0.8/interfaces/LinkTokenInterface.sol";

import "./MNKZ.sol";
import "./Zoogz.sol";
import "./ZoogEnergyStore.sol";

import "hardhat/console.sol";


contract ZoogBattler is AccessControl, ReentrancyGuard, VRFConsumerBaseV2 {
    using Strings for uint256;
    using SafeMath for uint256;

    // contracts to call
    Zoogz public zoogz;
    MNKZ public mnkz;
    ZoogEnergyStore public zoogEnergyStore;

    // how much energy each battle costs
    uint8 public energyPerBattle;
    // dice size
    uint8 public diceSize;
    // step for lvl cutoffs
    uint8 public stepSize;
    // bonus for type advantage
    uint8 public typeAdvantageBonus;
    // is battling open
    bool public battleOpen;
    // vrf cost
    uint256 public vrfCost;
    // last random num
    uint256 private _lastRandom;
    // min token amount in mnkz
    uint256 public minTokenAmount = 5 ether;
    // max token amount in mnkz
    uint256 public maxTokenAmount = 1000 ether;
    // current battle index
    uint256 public currentBattleIndex;
    // tax percentage
    uint256 public tax;
    // tax reciever
    address public taxReceiver;

    // info struct
    struct BattleInfo {
        uint256 tokenId; // zoog that started battle
        uint256 tokenAmt; // tokens set by creator
        uint256 battleIndex; // battle index
        uint32 startTs; // start time
        bool isVRF; // does battle have to use Chainlink VRF - hybrid model
    }

    // stats struct
    struct BattleStats {
        uint32 winCount;
        uint32 drawCount;
        uint32 matchCount;
    }

    // VRF stuff
    LinkTokenInterface public LINKTOKEN;
    VRFCoordinatorV2Interface public COORDINATOR;
    bytes32 public keyHash;
    uint32 public callbackGasLimit = 2500000;
    uint16 public requestConfirmations = 1;
    uint32 public numWords = 1;
    uint64 public subscriptionId;

    // mapping of battle index to battle info
    mapping(uint256 => BattleInfo) public battleIndexToInfo;
    // mapping of zoog token id to battle stats
    mapping(uint256 => BattleStats) public tokenToBattleStats;
    // mapping of zoog token id to upgradeable stats
    mapping(uint256 => mapping(uint8 => uint8)) public tokenToZoogStats;
    // mapping of zoog ids to battle index
    mapping(uint256 => uint256) public zoogToBattleIndex;
    // mapping of type advantages
    mapping(uint8 => mapping(uint8 => uint8)) private typeAdvantage;
    // mapping of zoog id to type
    mapping(uint256 => uint8) public zoogType;
    // mapping of request id to battle index for vrf calls
    mapping(uint256 => uint256) public requestIdToBattleIndex;
    // mapping of request id to challenger zoog - used for vrf calls
    mapping(uint256 => uint256) public requestIdToZoogId;

    // events TODO: fix
    event BattleCreated(uint256 indexed battleIndex, uint256 indexed hostingId, uint256 indexed tokenCount, bool isVRF, uint32 startTs);
    event BattleWin(uint256 indexed battleIndex, uint256 indexed winnerId, uint256 tokenCount, uint256 firstRoll, uint256 secondRoll, uint8 stat);
    event BattleDraw(uint256 indexed battleIndex, uint256 indexed challengerId, uint256 indexed opponentId, uint256 tokenCount, uint256 firstRoll, uint256 secondRoll, uint8 stat);
    event BattleCancelled(uint256 indexed battleIndex, uint256 indexed tokenId, uint256 tokenCount);
    event BattleAccept(uint256 indexed battleIndex, uint256 indexed acceptingId);

    // emergency events
    event EmergencyTransfer(address from, address to, uint256 tokenAmount);

    // roles
    bytes32 public constant SUPPORT_ROLE = keccak256("SUPPORT");

    constructor(
        address _zoogzAddress, 
        address _mnkzAddress, 
        address _energyAddress, 
        address _linkToken,
        address _vrfCoordinator, 
        bytes32 _keyHash, 
        uint64 _subscriptionId) VRFConsumerBaseV2(_vrfCoordinator){
        zoogz = Zoogz(_zoogzAddress);
        mnkz = MNKZ(_mnkzAddress);
        zoogEnergyStore = ZoogEnergyStore(_energyAddress);

        // vrf initialization
        LINKTOKEN = LinkTokenInterface(_linkToken);
        COORDINATOR = VRFCoordinatorV2Interface(_vrfCoordinator);
        keyHash = _keyHash;
        subscriptionId = _subscriptionId;
        vrfCost = 0.02 ether; // 0.02 LINK

        // initialize vars
        energyPerBattle = 2;
        diceSize = 20;
        stepSize = 5;
        typeAdvantageBonus = 1;
        tax = 2;
        // taxReceiver = _taxReceiver;

        // setup roles
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _setupRole(SUPPORT_ROLE, _msgSender());

        // approve mnkz for transfer
        mnkz.approve(address(this), mnkz.MAX_SUPPLY());
    }

    // MODIFIERS

    modifier zoogOwner(uint256 id) {
        require(zoogz.ownerOf(id) == _msgSender(), "ZB: Not zoog owner");
        _;
    }

    modifier battlingActive(){
        require(battleOpen, "ZB: Battles not active");
        _;
    }

    modifier callerIsUser() {
        require(
            tx.origin == msg.sender,
            "ZB: The caller is another contract"
        );
        _;
    }

    function getTypeAdvantage(uint256 challengerId, uint256 opponentId) external view returns (uint8) {
        require(zoogz.ownerOf(challengerId) != address(0), "ZB: Zoog does not exist");
        require(zoogz.ownerOf(opponentId) != address(0), "ZB: Zoog does not exist");
       
        return typeAdvantage[zoogType[challengerId]][zoogType[opponentId]];
    }

    // SETTERS

    function setTax(uint256 fee) external onlyRole(SUPPORT_ROLE) {
        tax = fee;
    }

    function setTaxReceiver(address receiver) external onlyRole(SUPPORT_ROLE) {
        taxReceiver = receiver;
    }

    function openBattling() external onlyRole(SUPPORT_ROLE){
        battleOpen = true;
    }

    function closeBattling() external onlyRole(SUPPORT_ROLE){
        battleOpen = false;
    }

    function setTypeAdvantageBonus(uint8 bonus) external onlyRole(SUPPORT_ROLE){
        typeAdvantageBonus = bonus;
    }

    function setEnergyPerBattle(uint8 energy) external onlyRole(SUPPORT_ROLE){
        energyPerBattle = energy;
    }

    function setDiceSize(uint8 size) external onlyRole(SUPPORT_ROLE){
        diceSize = size;
    }

    function setStepSize(uint8 size) external onlyRole(SUPPORT_ROLE){
        stepSize = size;
    }

    function setZoogAddress(address zoogAddress) external onlyRole(SUPPORT_ROLE){
        zoogz = Zoogz(zoogAddress);
    }

    function setMnkzAddress(address mnkzAddress) external onlyRole(SUPPORT_ROLE){
        mnkz = MNKZ(mnkzAddress);
    }

    function setZoogEnergyStoreAddress(address energyAddress) external onlyRole(SUPPORT_ROLE){
        zoogEnergyStore = ZoogEnergyStore(energyAddress);
    }

    function setMinTokenAmount(uint256 amount) external onlyRole(SUPPORT_ROLE){
        minTokenAmount = amount;
    }

    function setMaxTokenAmount(uint256 amount) external onlyRole(SUPPORT_ROLE){
        maxTokenAmount = amount;
    }

    // approve mnkz in case we need to approve additional amount
    function approveMnkz(uint256 amount) external onlyRole(DEFAULT_ADMIN_ROLE){
        mnkz.approve(address(this), amount);
    }

    // set type advantage mappings for battle lookup
    function setTypeAdvantages(uint8[] calldata types, uint8[] calldata advantages) external onlyRole(SUPPORT_ROLE){
        uint8 l = uint8(types.length);
        for(uint8 i = 0; i < l; ++i){
            typeAdvantage[types[i]][advantages[i]] = 1;
        }  
    }

    /**
     * set zoog types per token id
     * 1 - glow
     * 2 - drip
     * 3 - lump
     * 4 - puff
     */
    function setZoogTypes(uint256[] calldata zoogIds, uint8[] calldata types) external onlyRole(SUPPORT_ROLE){
        uint256 l = zoogIds.length;
        for(uint256 i = 0; i < l; ++i){
            zoogType[zoogIds[i]] = types[i];
        }  
    }

    // map zoog stats to mapping for lookup later
    function _setStats(uint256 tokenId) internal {
        Zoogz.Zoog memory z = zoogz.getZoogFromToken(tokenId);
        // set stats for easy lookup - will overwrite if exists
        tokenToZoogStats[tokenId][1] = z.aggression;
        tokenToZoogStats[tokenId][2] = z.toughness;
        tokenToZoogStats[tokenId][3] = z.smarts;
        tokenToZoogStats[tokenId][4] = z.vitality;
    }

    // check if battle creation is valid
    function _validateCreate(uint256 tokenId, uint256 tokens) internal view {
        require(zoogToBattleIndex[tokenId] == 0, "ZB: Zoog already in battle");
        require(tokens >= minTokenAmount, "ZB: Too few tokens");
        require(tokens <= maxTokenAmount, "ZB: Too many tokens");
        require(zoogEnergyStore.isZoogUpForChallenge(tokenId, energyPerBattle), "ZB: Too tired to battle");
    }

    /**
     * function to start battles for others to accept
     * saves zoog stats at time of battle start
     */
    function createBattle(uint256 tokenId, uint256 tokens, bool vrfBattle) external zoogOwner(tokenId) nonReentrant callerIsUser battlingActive {
        // check parameters
        _validateCreate(tokenId, tokens);
        require(mnkz.balanceOf(msg.sender) >= tokens, "ZB: Not enough tokens to purchase");
        if(vrfBattle){
            // check LINK balance to fund subscription
            require(LINKTOKEN.balanceOf(msg.sender) >= vrfCost, "ZB: Not enough LINK for VRF battle");
            // fund subscription - anyone can fund
            require(LINKTOKEN.transferFrom(msg.sender, address(this), vrfCost), "ZB: Transfer failed");
            require(LINKTOKEN.transferAndCall(address(COORDINATOR), vrfCost, abi.encode(subscriptionId)), "ZB: Transfer failed");
        }
        // subtract energy even though it says add
        zoogEnergyStore.addZoogEnergy(tokenId, energyPerBattle);
        // transfer tokens to contract
        require(mnkz.transferFrom(msg.sender, address(this), tokens), "ZB: Transfer failed");
        // update current battle index
        currentBattleIndex++;
        // mark zoog as in battle
        zoogToBattleIndex[tokenId] = currentBattleIndex;
        // create battle info for current battle index
        uint32 ts = uint32(block.timestamp);
        battleIndexToInfo[currentBattleIndex] = BattleInfo({
            tokenId: tokenId,
            tokenAmt: tokens,
            battleIndex: currentBattleIndex,
            startTs: ts,
            isVRF: vrfBattle
        });

        // set for ease of lookup
        _setStats(tokenId);
        // blast comms
        emit BattleCreated(currentBattleIndex, tokenId, tokens, vrfBattle, ts);
    }

    function acceptBattleWithVRF(uint256 challengerId, uint256 battleIndex) external zoogOwner(challengerId) nonReentrant callerIsUser battlingActive {
        require(zoogToBattleIndex[challengerId] == 0, "ZB: Already in a battle");
        require(zoogEnergyStore.isZoogUpForChallenge(challengerId, energyPerBattle), "ZB: Too tired to battle");
        BattleInfo memory b = battleIndexToInfo[battleIndex];
        require(b.isVRF, "ZB: Battle is not a VRF battle");
        require(b.startTs > 0, "ZB: Invalid battle instance");
        require(mnkz.balanceOf(msg.sender) >= b.tokenAmt, "ZB: Not enough tokens held");
        require(zoogz.ownerOf(challengerId) != zoogz.ownerOf(b.tokenId), "ZB: Cannot battle self");
        require(LINKTOKEN.balanceOf(msg.sender) >= vrfCost, "ZB: Not enough LINK for VRF battle");
        // fund subscription - anyone can fund
        require(LINKTOKEN.transferFrom(msg.sender, address(this), vrfCost), "ZB: Transfer failed");
        require(LINKTOKEN.transferAndCall(address(COORDINATOR), vrfCost, abi.encode(subscriptionId)), "ZB: Transfer failed");
        // transfer tokens to contract
        require(mnkz.transferFrom(msg.sender, address(this), b.tokenAmt), "ZB: Transfer failed");
        // set zoog stats for challenger
        _setStats(challengerId);
        // put challenger in battle since VRF
        zoogToBattleIndex[challengerId] = battleIndex;
        // set battle to 0 start time to prevent other accepts
        battleIndexToInfo[battleIndex].startTs = uint32(0);

        emit BattleAccept(battleIndex, challengerId);

        // kick of battle via VRF with requestRandomWords
        uint256 reqId = COORDINATOR.requestRandomWords(
            keyHash,
            subscriptionId,
            requestConfirmations,
            callbackGasLimit,
            numWords
        );

        // save battle index for lookup
        requestIdToBattleIndex[reqId] = battleIndex;
        // save challenger id for lookup
        requestIdToZoogId[reqId] = challengerId;
    } 

    function acceptBattle(uint256 challengerId, uint256 battleIndex) external zoogOwner(challengerId) nonReentrant callerIsUser battlingActive {
        require(zoogToBattleIndex[challengerId] == 0, "ZB: Already in a battle");
        require(zoogEnergyStore.isZoogUpForChallenge(challengerId, energyPerBattle), "ZB: Too tired to battle");
        BattleInfo memory b = battleIndexToInfo[battleIndex];
        require(!b.isVRF, "ZB: Battle must be accepted with VRF");
        require(b.startTs > 0, "ZB: Invalid battle instance");
        require(mnkz.balanceOf(msg.sender) >= b.tokenAmt, "ZB: Not enough tokens held");
        require(zoogz.ownerOf(challengerId) != zoogz.ownerOf(b.tokenId), "ZB: Cannot battle self");
        // transfer tokens to contract
        require(mnkz.transferFrom(msg.sender, address(this), b.tokenAmt), "ZB: Transfer failed");
        // set zoog stats for challenger
        _setStats(challengerId);

        emit BattleAccept(battleIndex, challengerId);

        // battle zoogz
        _standardBattle(challengerId, b.tokenId, b.tokenAmt, battleIndex);
        
        // set time to 0 to signal battle is over
        battleIndexToInfo[battleIndex].startTs = uint32(0);
        // pull zoogz out of battle
        zoogToBattleIndex[challengerId] = 0;
        zoogToBattleIndex[b.tokenId] = 0;
    }

    function cancelBattle(uint256 tokenId) external zoogOwner(tokenId) nonReentrant callerIsUser {
        require(zoogToBattleIndex[tokenId] != 0, "ZB: Zoog not in battle");
        _cancelBattle(tokenId);
    } 

    function _getRandom() internal view returns (uint256){
        return uint256(keccak256(abi.encodePacked(block.difficulty, block.timestamp, currentBattleIndex, _lastRandom)));
    }
    
    function _getStatFromZoog(uint256 tokenId, uint8 stat) internal view returns (uint256){
        return (tokenToZoogStats[tokenId][stat] / stepSize);
    }

    function _getTypeAdvantage(uint256 challengerId, uint256 opponentId) internal view returns (uint8){
        if(typeAdvantage[zoogType[challengerId]][zoogType[opponentId]] == 1){
            return typeAdvantageBonus;
        } else {
            return 0;
        }
    }

    function _getDiceRolls() internal view returns (uint8, uint256, uint256){
        uint256 random = _getRandom();
        // between 1-4
        uint8 statToCompare = uint8(((random >> 2) % 4) + 1);
        // between 1-dice size
        uint256 firstRoll = ((random) % diceSize) + 1;
        uint256 secondRoll = ((random >> statToCompare) % diceSize) + 1;

        return(statToCompare, firstRoll, secondRoll);
    }

    
    function _standardBattle(uint256 challengerId, uint256 opponentId, uint256 tokenAmt, uint256 battleIndex) internal {
        uint256 firstRoll;
        uint256 secondRoll;
        uint8 statToCompare;

        (statToCompare, firstRoll, secondRoll) = _getDiceRolls();

        _battleZoogz(challengerId, opponentId, firstRoll, secondRoll, statToCompare, tokenAmt, battleIndex);
    }

    function _battleZoogz(uint256 challengerId, uint256 opponentId, uint256 firstRoll, uint256 secondRoll, uint8 statToCompare, uint256 tokenAmt, uint256 battleIndex) internal {
        // challenger -> second roll
        // opponent -> first roll
        uint256 challengerTotal = _getStatFromZoog(challengerId, statToCompare) + secondRoll + _getTypeAdvantage(challengerId, opponentId);
        uint256 opponentTotal = _getStatFromZoog(opponentId, statToCompare) + firstRoll + _getTypeAdvantage(opponentId, challengerId);

        // decide winner
        if(challengerTotal == opponentTotal){
            // draw -- refund both sides
            require(mnkz.transferFrom(address(this), zoogz.ownerOf(challengerId), tokenAmt), "ZB: Transfer failed");
            require(mnkz.transferFrom(address(this), zoogz.ownerOf(opponentId), tokenAmt), "ZB: Tranfer failed");
            // increment draws
            tokenToBattleStats[challengerId].drawCount++;
            tokenToBattleStats[opponentId].drawCount++;
            emit BattleDraw(battleIndex, challengerId, opponentId, tokenAmt, firstRoll, secondRoll, statToCompare);
        } else {
            uint256 total = tokenAmt * 2;
            uint256 battleTax = total.mul(tax).div(100);
            uint256 winnings = total - battleTax;
            uint256 winner = challengerTotal > opponentTotal ? challengerId : opponentId;
            // send tax to reciever
            require(mnkz.transferFrom(address(this), taxReceiver, battleTax), "ZB: Transfer failed");
            // 2x the tokens sent to winner to account for both sides
            require(mnkz.transferFrom(address(this), zoogz.ownerOf(winner), winnings), "ZB: Transfer failed");
            // increment wins
            tokenToBattleStats[winner].winCount++;
            emit BattleWin(battleIndex, winner, winnings, firstRoll, secondRoll, statToCompare);
        }

        // increment matches
        tokenToBattleStats[challengerId].matchCount++;
        tokenToBattleStats[opponentId].matchCount++;

        // increment energy for challenger
        zoogEnergyStore.addZoogEnergy(challengerId, energyPerBattle);
    }

    // VRF SETTERS AND FUNCTIONS

    function setLinkTokenAddres(address linkTokenAddress) external onlyRole(SUPPORT_ROLE) {
        LINKTOKEN = LinkTokenInterface(linkTokenAddress);
    }

    function setKeyHash(bytes32 _keyHash) external onlyRole(SUPPORT_ROLE) {
        keyHash = _keyHash;
    }

    function setCallbackGasLimit(uint32 _callbackGasLimit)
        external
        onlyRole(SUPPORT_ROLE)
    {
        callbackGasLimit = _callbackGasLimit;
    }

    function setRequestConfirmations(uint16 _reqConf)
        external
        onlyRole(SUPPORT_ROLE)
    {
        requestConfirmations = _reqConf;
    }

    function setNumberOfWords(uint32 _numOfWords)
        external
        onlyRole(SUPPORT_ROLE)
    {
        numWords = _numOfWords;
    }

    function setSubscriptionId(uint64 _subId) external onlyRole(SUPPORT_ROLE) {
        subscriptionId = _subId;
    }

    function setVrfCoordinator(address _addr) external onlyRole(SUPPORT_ROLE) {
        COORDINATOR = VRFCoordinatorV2Interface(_addr);
    }


    function fulfillRandomWords(uint256 requestId, uint256[] memory randomWords)
        internal
        override
    {
        // save for later
        _lastRandom = randomWords[0];
        // get two random dice rolls
        uint256 firstRoll = (randomWords[0] % diceSize) + 1;
        uint256 secondRoll = ((randomWords[0] >> 2) % diceSize) + 1;

        // get a stat to compare 1-4
        uint8 statToCompare = uint8(((randomWords[0] >> 3) % 4) + 1);

        // get battle info to process
        uint256 battleIndex = requestIdToBattleIndex[requestId];
        BattleInfo memory b = battleIndexToInfo[battleIndex];

        uint256 challengerId = requestIdToZoogId[requestId];

        // battle
        _battleZoogz(challengerId, b.tokenId, firstRoll, secondRoll, statToCompare, b.tokenAmt, battleIndex);

        // take zoogz out of battle
        zoogToBattleIndex[challengerId] = 0;
        zoogToBattleIndex[b.tokenId] = 0;
    }

     // EMERGENCY FUNCTIONS

    function fundWithTokens(uint256 amount) external onlyRole(SUPPORT_ROLE){
        require(mnkz.balanceOf(msg.sender) >= amount, "ZB: Not enough to fund");
        require(mnkz.transferFrom(msg.sender, address(this), amount), "ZB: Transfer failed");
    }

    function rescueTokens(uint256 amount) external onlyRole(DEFAULT_ADMIN_ROLE){
       require(mnkz.transferFrom(address(this), msg.sender, amount), "ZB: Transfer failed");
        emit EmergencyTransfer(address(this), msg.sender, amount);
    }

    function takeZoogzOutOfBattle(uint256[] calldata ids) external onlyRole(SUPPORT_ROLE){
        uint256 l = ids.length;
        for(uint256 i = 0; i < l; i++){
            zoogToBattleIndex[ids[i]] = 0;
        }
    }

    // emergency retry
    function retryBattleInstance(uint256 challengerId, uint256 battleIndex) external onlyRole(SUPPORT_ROLE){
        require(zoogToBattleIndex[challengerId] == battleIndex, "ZB: Cannot retry");
        // set zoog stats for challenger
        _setStats(challengerId);
        // put challenger in battle since VRF
        zoogToBattleIndex[challengerId] = battleIndex;

        emit BattleAccept(battleIndex, challengerId);

        // kick of battle via VRF with requestRandomWords
        uint256 reqId = COORDINATOR.requestRandomWords(
            keyHash,
            subscriptionId,
            requestConfirmations,
            callbackGasLimit,
            numWords
        );

        // save battle index for lookup
        requestIdToBattleIndex[reqId] = battleIndex;
        // save challenger id for lookup
        requestIdToZoogId[reqId] = challengerId;
    }

    function bulkCancelBattles(uint256[] calldata zoogIds) external onlyRole(SUPPORT_ROLE){
        uint256 l = zoogIds.length;
        for(uint256 i = 0; i < l; i++){
            uint256 tokenId = zoogIds[i];
            _cancelBattle(tokenId);
        }
    }

    function _cancelBattle(uint256 tokenId) internal {
        uint256 battleIndex = zoogToBattleIndex[tokenId];
        // get tokens
        uint256 tokens = battleIndexToInfo[battleIndex].tokenAmt;
        // reset status
        battleIndexToInfo[battleIndex].startTs = uint32(0);
        zoogToBattleIndex[tokenId] = 0;
        // refund tokens
        require(mnkz.transferFrom(address(this), zoogz.ownerOf(tokenId), tokens), "ZB: Transfer failed");
        // blask comms
        emit BattleCancelled(battleIndex, tokenId, tokens);
    }
}

