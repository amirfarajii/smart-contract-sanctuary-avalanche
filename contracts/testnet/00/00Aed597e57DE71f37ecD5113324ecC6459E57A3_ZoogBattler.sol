pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";

contract MNKZ is ERC20, Ownable {
    uint256 constant DECIMALS = 18;
    uint256 public constant MAX_SUPPLY = 130000000 * 10**DECIMALS; // 130 million
    uint256 public constant INITIAL = 10000000 * 10**DECIMALS; // 10 million

    mapping(address => bool) public callers;

    constructor() ERC20("Monkeez Token", "MNKZ") {
        callers[owner()] = true;
        _mint(owner(), INITIAL);
    }

    modifier approvedCaller() {
        require(callers[msg.sender], "MNKZ: not an approved caller");
        _;
    }

    function mintTokens(address _addr, uint256 _amount)
        external
        approvedCaller
    {
        require(
            totalSupply() + (_amount * 10**DECIMALS) <= MAX_SUPPLY,
            "MNKZ: Max tokens have already minted"
        );
        _mint(_addr, (_amount * 10**DECIMALS));
    }

    function mintBatch(address[] calldata _addrs, uint256 _amount)
        external
        approvedCaller
    {
        for (uint256 i = 0; i < _addrs.length; i++) {
            _mint(_addrs[i], (_amount * 10**DECIMALS));
        }

        require(totalSupply() <= MAX_SUPPLY, "MNKZ: Max tokens have already minted");
    }

    function burn(address _from, uint256 _amount) external approvedCaller {
        _burn(_from, (_amount * 10**DECIMALS));
    }

    // SETTERS

    function addCaller(address _addr) external onlyOwner {
        callers[_addr] = true;
    }

    function removeCaller(address _addr) external onlyOwner {
        callers[_addr] = false;
    }
}

