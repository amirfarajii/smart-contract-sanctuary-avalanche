pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

import "hardhat/console.sol";

contract ZoogEnergyStore is AccessControl, ReentrancyGuard {
    using Strings for uint256;
    using SafeMath for uint256;

    uint8 public energyCap;

    struct EnergyInfo {
        uint32 lastTs;
        uint8 energy;
    }

    uint256 public energyTimerReset = 1 days;

    mapping(uint256 => EnergyInfo) public zoogEnergy;

    constructor(){
        // initialize maxEnergy
        energyCap = 10;

        // intial role assignment to the owner
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _setupRole(ENERGY_WRITER_ROLE, _msgSender());
    }

    bytes32 public constant ENERGY_WRITER_ROLE = keccak256("ENERGY_WRITER");

    event EnergySet(uint256 tokenId, uint8 energy);
    event EnergyAdded(uint256 tokenId, uint8 energyAdded);
    event EnergyRemoved(uint256 tokenId, uint8 energyRemoved);

    function isZoogUpForChallenge(uint256 tokenId, uint8 energyCost) external view returns (bool){
        // energy cap resets every 24 hours
        return (block.timestamp - zoogEnergy[tokenId].lastTs) >= energyTimerReset || zoogEnergy[tokenId].energy + energyCost <= energyCap;
    }

    function getZoogEnergy(uint256 tokenId) external view returns (uint8){
        return zoogEnergy[tokenId].energy;
    }

    function setEnergyCap(uint8 energy) external onlyRole(DEFAULT_ADMIN_ROLE){
        energyCap = energy;
    }

    function setEnergyTimerReset(uint256 time) external onlyRole(DEFAULT_ADMIN_ROLE){
        energyTimerReset = time;
    }

    // function to set zoog energy without altering ts
    function setZoogEnergy(uint256 tokenId, uint8 energyCount) external onlyRole(ENERGY_WRITER_ROLE){
        zoogEnergy[tokenId].energy = energyCount;
        emit EnergySet(tokenId, energyCount);
    }

    // function to set zoog energy via processes (battles, etc)
    function addZoogEnergy(uint256 tokenId, uint8 energyCount) external onlyRole(ENERGY_WRITER_ROLE){
        uint256 timeElapsed = (block.timestamp - zoogEnergy[tokenId].lastTs);
        if(timeElapsed >= energyTimerReset){
            zoogEnergy[tokenId].energy = 0;
            zoogEnergy[tokenId].lastTs = uint32(block.timestamp);
        }
        require(zoogEnergy[tokenId].energy + energyCount <= energyCap, "ZES: Zoog needs rest");
        zoogEnergy[tokenId].energy += energyCount;
        emit EnergyAdded(tokenId, energyCount);
    }

    function removeZoogEnergy(uint256 tokenId, uint8 energyCount) external onlyRole(ENERGY_WRITER_ROLE){
        if(zoogEnergy[tokenId].energy - energyCount > 0){
            zoogEnergy[tokenId].energy -= energyCount;
        } else {
            zoogEnergy[tokenId].energy = 0;
        }
        emit EnergyRemoved(tokenId, energyCount);
    }
}

