pragma solidity ^0.8.7;

// Imports
import "./Funds.sol";

// Interfaces
import "./interfaces/ISeasons.sol";
import "./interfaces/IStats.sol";
import "@chainlink/contracts/src/v0.8/interfaces/AggregatorV3Interface.sol";

// Contract
contract Souls is Funds {
    using CountersUpgradeable for CountersUpgradeable.Counter;
    using StringsUpgradeable for uint;

    ////////////////////////
    ///     VARIABLES    ///
    ////////////////////////

    CountersUpgradeable.Counter public soulsIds;

    ////////////////////////
    ///      EVENTS      ///
    ////////////////////////

    event SoulsMint(uint soulId, string pattern);

    ////////////////////////
    ///      PRIVATE     ///
    ////////////////////////

    /**
     * @dev Triggered by chainlink VRF response, starts a new mint.
     */
    function fulfillRandomWords(
        uint256 _reqId,
        uint256[] memory _randWords
    ) internal override {
        // Get current soul ID
        uint currentSoulId = soulsIds.current();

        // Get current season infos
        ISeasons SEASONS = ISeasons(seasonsContract);
        (,,,,uint karmaCuts, uint animaCuts,,string memory baseURI,) = SEASONS.getSeasonsFromId(SEASONS.season());

        // VRF normal random karma & anima
        uint karma = _getNormalStat(_randWords[0], 100, 4);
        uint anima = _getNormalStat(_randWords[1], 50, 4);

        // Assign random stats to the soul
        IStats(statsContract).setInitialStats(
            currentSoulId,
            karma,
            anima
        );

        // Get pattern
        string memory pattern = _getPattern(
            karma,
            anima,
            karmaCuts,
            animaCuts
        );

        // Mint
        _safeMint(reqIdToAsker[_reqId], currentSoulId);
        _setTokenURI(
            currentSoulId,
            string(
                abi.encodePacked(
                    baseURI,
                    pattern,
                    ".json"
                )
            )
        );

        // Increment token count & season supply
        SEASONS.incrementSupply();
        soulsIds.increment();

        // Emit event
        emit SoulsMint(currentSoulId, pattern);
    }

    /**
     * @dev Generate a pseudo bell curve random number between 0 and _maxValue based on _bellFactor and provided _seed.
     */
    function _getNormalStat(
        uint256 seed,
        uint maxValue,
        uint bellFactor
    ) private pure returns (uint stat) {
        uint _maxValue = maxValue + 1;

        stat = 0;
        for (uint256 i = 0; i < bellFactor; i++) {
            stat += uint256(keccak256(abi.encode(seed + i))) % _maxValue;
        }

        return (stat / bellFactor);
    }

    /**
     * @dev Return pattern string based on _karma and _anima.
     */
    function _getPattern(
        uint _karma,
        uint _anima,
        uint _karmaCuts,
        uint _animaCuts
    ) private view returns (string memory pattern) {
        IStats STATS = IStats(statsContract);

        return string(
            abi.encodePacked(
                StringsUpgradeable.toString(
                    STATS.getStatCut(
                        _karma * 10 ** 18,
                        _karmaCuts,
                        100_00000_00000_00000_000
                    )
                ),
                "-",
                StringsUpgradeable.toString(
                    STATS.getStatCut(
                        _anima * 10 ** 18,
                        _animaCuts,
                        50_00000_00000_00000_000
                    )
                )
            )
        );
    }

    ////////////////////////
    ///      PUBLIC      ///
    ////////////////////////

    /**
     * @dev Return converted USDC price into AVAX.
     */
    function getAvaxPrice() public view returns (uint avaxPrice) {
        (,int avaxUsdcFeed,,,) = AggregatorV3Interface(avaxUsdcDatafeedContract).latestRoundData();
        uint compute = (price * 10 ** 18) / uint(avaxUsdcFeed);
        return compute;
    }

    /**
     * @dev Buy a new soul and request mint.
     */
    function mint(address _to) public payable {
        if(!hasRole(SOULS_ADMIN_ROLE, _msgSender())) {
            // Check if contract is paused
            require(!ISeasons(seasonsContract).pause());

            // Check sufficient value sent
            require(msg.value >= getAvaxPrice());

            // Swap $AVAX for $LINK
            _swapAvaxForLink((msg.value / 100) * linkPercent);

            // Refill subscription
            _refillSubscription();
        }

        // Request randomness
        _requestRandomWords(_to);
    }

    ////////////////////////
    ///      OWNER       ///
    ////////////////////////

    /**
     * @dev Replace stats contract address with provided _statsContract.
     * 
     * Only callable by SOULS_ADMIN_ROLE.
     */
    function editStatsContract(address _statsContract) public onlyRole(SOULS_ADMIN_ROLE) {
        statsContract = _statsContract;
    }

    /**
     * @dev Replace seasons contract address with provided _seasonsContract.
     * 
     * Only callable by SOULS_ADMIN_ROLE.
     */
    function editSeasonsContract(address _seasonsContract) public onlyRole(SOULS_ADMIN_ROLE) {
        seasonsContract = _seasonsContract;
    }

    /**
     * @dev Replace current USDC price with provided _price.
     * WARNING: Price must be specified in USDC, with 8 digits.
     * 
     * Only callable by SOULS_ADMIN_ROLE.
     */
    function editPrice(uint _price) public onlyRole(SOULS_ADMIN_ROLE) {
        price = _price;
    }
}

