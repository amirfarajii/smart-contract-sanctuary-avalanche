pragma solidity ^0.8.7;

// Imports
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721URIStorageUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/CountersUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/StringsUpgradeable.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2Upgradeable.sol";

// Interfaces
import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "@chainlink/contracts/src/v0.8/interfaces/LinkTokenInterface.sol";
import "./interfaces/IJoeRouter02.sol";

/**
 * @dev Funds: Manage VRF, token swaps and funds withdrawals for Tiny Bones Souls.
 * Written by s0wcy => https://twitter.com/s0wcy
 * 
 * ### Chainlink requests.
 * VRF is the main source of randomness for Tiny Bones Souls.
 * Souls statistics and apperance are generated from a seed provided by Chainlink VRF.
 * This contract manages subscription to VRF and it's dynamic refill.
 * 
 * ### Token swaps.
 * This contract aim to simplify end users experience by providing automated swap from AVAX to LINK token.
 * As the LINK token is the fuel we need to generate new souls, we would need to ask new players to approve LINK token.
 * From a UX perspective, this is bad. That's why we decided to implement Trader Joe Router.
 * 
 * ### Funds withdrawals.
 * As the Tiny Bones DAO needs to build up its treasury, this contract allow AVAX and ERC20 withdrawals.
 * Multisig address of Tiny Bones DAO can be found in our documentation: https://docs.tinybones.club/.
 */
abstract contract Funds is Initializable, AccessControlUpgradeable, VRFConsumerBaseV2Upgradeable, ERC721URIStorageUpgradeable {
    ////////////////////////
    ///     VARIABLES    ///
    ////////////////////////

    address public seasonsContract;
    address public statsContract;

    uint public price;
    uint internal linkPercent;

    // Coordinator mainnet = 0xd5D517aBE5cF79B7e95eC98dB0f0277788aFF634
    // Coordinator testnet = 0x2eD832Ba664535e5886b75D64C46EB9a228C2610
    address internal vrfCoordinator;
    // KeyHash mainnet = 0x83250c5584ffa93feb6ee082981c5ebe484c865196750b39835ad4f13780435d
    // KeyHash testnet = 0x354d2f95da55398f44b7cff77da56283d9c6c829a4bdf1bbcaf2ad6a4d081f61
    bytes32 internal keyHash;
    // subId mainnet = 69
    // subId testnet = 380
    uint64 public subId;

    // WAVAX mainnet = 0xB31f66AA3C1e785363F0875A1B74E27b85FD66c7
    // WAVAX testnet = 0xd00ae08403B9bbb9124bB305C09058E32C39A48c
    address private wavaxTokenContract;
    // LINK mainnet = 0x5947BB275c521040051D82396192181b413227A3
    // LINK testnet = 0x0b9d5D9136855f6FEc3c0993feE6E9CE8a297846
    address private linkTokenContract;
    // LINK/AVAX mainnet = 0x1b8a25F73c9420dD507406C3A3816A276b62f56a
    // LINK/AVAX testnet = 0x79c91fd4F8b3DaBEe17d286EB11cEE4D83521775
    address private avaxLinkDatafeedContract;
    // AVAX/USD mainnet = 0x0A77230d17318075983913bC2145DB16C7366156
    // AVAX/USD testnet = 0x5498BB86BC934c8D34FDA08E81D444153d0D06aD
    address internal avaxUsdcDatafeedContract;
    // JoeRouter mainnet = 0x60aE616a2155Ee3d9A68541Ba4544862310933d4
    // JoeRouter testnet = 0xd7f655E3376cE2D7A2b08fF01Eb3B1023191A901
    address private joeRouterContract;

    bytes32 public constant SOULS_ADMIN_ROLE = keccak256("SOULS_ADMIN_ROLE");

    ////////////////////////
    ///      MAPPING     ///
    ////////////////////////

    mapping (uint => address) internal reqIdToAsker;

    ////////////////////////
    ///    INITIALIZE    ///
    ////////////////////////

    function initialize(address _seasonsContract, address _statsContract) external initializer {
        __AccessControl_init();
        __ERC721_init("Tiny Bones Souls", "SOUL");
        seasonsContract = _seasonsContract;
        statsContract = _statsContract;
        price = 999000000;
        linkPercent = 10;

        vrfCoordinator = 0x2eD832Ba664535e5886b75D64C46EB9a228C2610;
        keyHash = 0x354d2f95da55398f44b7cff77da56283d9c6c829a4bdf1bbcaf2ad6a4d081f61;
        subId = 380;
        __VRFConsumerBaseV2_init(vrfCoordinator);

        wavaxTokenContract = 0xd00ae08403B9bbb9124bB305C09058E32C39A48c;
        linkTokenContract = 0x0b9d5D9136855f6FEc3c0993feE6E9CE8a297846;
        avaxLinkDatafeedContract = 0x79c91fd4F8b3DaBEe17d286EB11cEE4D83521775;
        avaxUsdcDatafeedContract = 0x5498BB86BC934c8D34FDA08E81D444153d0D06aD;
        joeRouterContract = 0xd7f655E3376cE2D7A2b08fF01Eb3B1023191A901;

        _grantRole(DEFAULT_ADMIN_ROLE, _msgSender());
    }

    ////////////////////////
    ///      PRIVATE     ///
    ////////////////////////

    /**
     * @dev Request chainlink VRF for _asker.
     */
    function _requestRandomWords(address _asker) internal {
        uint reqId = VRFCoordinatorV2Interface(vrfCoordinator).requestRandomWords(
            keyHash,
            subId,
            3,
            2500000,
            2
        );

        reqIdToAsker[reqId] = _asker;
    }

    /**
     * @dev Swap _avaxAmount from this contract for LINK.
     */
    function _swapAvaxForLink(uint _avaxAmount) internal {
        // Get router interface
        IJoeRouter02 ROUTER = IJoeRouter02(joeRouterContract);

        // Pair
        address[] memory pair = new address[](2);
        pair[0] = wavaxTokenContract;
        pair[1] = linkTokenContract;

        // Min amount
        uint[] memory amountsOut = ROUTER.getAmountsOut(
            _avaxAmount,
            pair
        );
        uint minAmount = amountsOut[1] - ((amountsOut[1] / 100) * 2); // 2% slippage

        // Router swap
        ROUTER.swapExactAVAXForTokens{value: _avaxAmount}(
            minAmount,
            pair,
            address(this),
            block.timestamp + 600
        );
    }

    /**
     * @dev Refill LINK subscription with all LINK owned by this contract.
     */
    function _refillSubscription() internal {
        // Get token interface
        LinkTokenInterface TOKEN = LinkTokenInterface(linkTokenContract);

        // Refill
        TOKEN.transferAndCall(
            address(VRFCoordinatorV2Interface(vrfCoordinator)),
            TOKEN.balanceOf(address(this)),
            abi.encode(subId)
        );
    }

    ////////////////////////
    ///       OWNER      ///
    ////////////////////////

    /**
     * @dev Replace current subscription ID with provided _subId.
     */
    function fundsEditSubId(uint64 _subId) public onlyRole(SOULS_ADMIN_ROLE) {
        subId = _subId;
    }

    /**
     * @dev Withdraw tokens or AVAX if _contract is 0x0 address, from this contract to provided address _to.
     */
    function fundsWithdraw(address _contract, address _to, uint _amount) public onlyRole(SOULS_ADMIN_ROLE) {
        if(_contract == address(0)) {
            payable(_to).transfer(_amount);
        } else {
            LinkTokenInterface(_contract).transfer(_to, _amount);
        }
    }

    ////////////////////////
    ///     OVERRIDE     ///
    ////////////////////////

    function _beforeTokenTransfer( address from, address to, uint tokenId) internal virtual override(ERC721Upgradeable) {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    function supportsInterface(bytes4 interfaceId) public view virtual override(ERC721Upgradeable, AccessControlUpgradeable) returns (bool) {
        return super.supportsInterface(interfaceId);
    }
}

