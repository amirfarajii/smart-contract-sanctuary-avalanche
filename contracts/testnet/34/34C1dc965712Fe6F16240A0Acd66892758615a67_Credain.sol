//SPDX-License-Identifier: MIT
pragma solidity ^0.8.0;

import "@chainlink/contracts/src/v0.8/ChainlinkClient.sol";
import "@chainlink/contracts/src/v0.8/ConfirmedOwner.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

import "hardhat/console.sol";

contract Credain is  ChainlinkClient, ConfirmedOwner, ReentrancyGuard {
  
  using Chainlink for Chainlink.Request;
  using Address for address;
  
  uint256 private counter = 1; // 0 signified null
  uint256 public constant growthPercentageDenominator = 100;
  uint256 public startPoint;
  uint256 public endPoint;
  uint256 public noOfEntries = 0;
  uint256 private chainlinkFee = (2 * LINK_DIVISIBILITY) / 10;

  bytes32 private jobId = 'fcf4140d696d44b687012232948bdd5d';

  string public apiEndpoint;

  struct Entry {
    uint256 id;
    uint256 prev;
    uint256 next;

    address from;
    address to;
    uint256 amount;
    uint256 clearedAmount;
    uint256 initiationDate;
    uint256 deadline;
    bool isActive;
  }

  mapping (address => uint256) public lockedAssets;
  mapping (uint256 => Entry) public entries;

  enum PaymentType {
    FULL,
    PARTIAL
  }

  event TransactionInitiated(
    uint256 id,
    address indexed from,
    address indexed to,
    uint256 amount,
    uint256 initiationDate,
    uint256 deadline
  );

  event ProcessedPayment(
    uint256 id,
    address indexed from,
    address indexed to,
    uint256 amount,
    uint256 totalAmount,
    PaymentType paymentType
  );

  constructor(address _linkTokenAddress, address _oracleAddress, string memory _apiEnpoint) ConfirmedOwner(msg.sender) {
    startPoint = 0;
    endPoint = 0;
    setChainlinkOracle(_oracleAddress);
    setChainlinkToken(_linkTokenAddress);
    apiEndpoint = _apiEnpoint;
  }

  // Public functions
  function createTransaction(address to, uint256 amount, uint256 deadline) public payable {
    require(msg.value == amount, "Amount sent must be equal to the amount specified");
    lockedAssets[msg.sender] += amount;
    uint256 entryId = counter;
    _addNewEntry(entryId, msg.sender, to, amount, deadline);
    counter += 1;
    emit TransactionInitiated(entryId, msg.sender, to, amount, block.timestamp, deadline);
  }

  function executedByChainlink() public {
    // require(msg.sender == chainlinkOracleAddress(), "Only Chainlink can call this function");
    if(noOfEntries == 0) return;
    _getMarketStatus();
  }

  function fullfill(bytes32 requestId, uint256 value) public recordChainlinkFulfillment(requestId) {
    if (value <= 1) {
      return;
    }
    uint256 marketGrowth = uint256(value);
    _processPayments(marketGrowth);
  }

  function testFullfill(uint256 value) public {
    if (value <= 1) {
      return;
    }
    uint256 marketGrowth = uint256(value);
    _processPayments(marketGrowth);
  }

  // Private functions
  function _addNewEntry(uint256 entryId, address from, address to, uint256 amount, uint256 deadline) internal {
    if(startPoint == 0 && startPoint == endPoint) {
      // No entry exists
      startPoint = entryId;
      endPoint = entryId;
      entries[entryId] = Entry(entryId, 0, 0, from, to, amount, 0, block.timestamp, deadline, true);
    } else if(startPoint != 0 && endPoint != 0) {
      // Add new entry at the end
      uint256 prev = endPoint;
      endPoint = entryId;
      entries[entryId] = Entry(entryId, prev, 0, from, to, amount, 0, block.timestamp, deadline, true);
      entries[prev].next = entryId;
    } else {
      require(false, "Credain: Error with start and end points");
    }
    noOfEntries += 1;
  }

  function _getMarketStatus() internal returns(bytes32 requestId) {
    Chainlink.Request memory request = buildChainlinkRequest(jobId, address(this), this.fullfill.selector);
    request.add("get", apiEndpoint);
    request.add("path", "market_status");
    request.addInt("times", 1);

    return sendChainlinkRequest(request, chainlinkFee);
  }

  function _processPayments(uint256 marketGrowth) internal {

    if(marketGrowth == 0) return;
    uint256 currentEntryId = startPoint;
    
    while(currentEntryId != 0) {
      if(entries[currentEntryId].isActive == false) break;

      (uint256 amountToPay, PaymentType paymentType) = _getAmountToPay(currentEntryId, marketGrowth);
      currentEntryId = _handlePayment(currentEntryId, amountToPay, paymentType);
    }
  }

  function _getAmountToPay(uint256 currentEntryId, uint256 marketGrowth) internal view returns(uint256, PaymentType) {
    Entry storage currentEntry = entries[currentEntryId];
    uint256 calculatedPercentage = (currentEntry.amount * marketGrowth) / growthPercentageDenominator;
    uint256 amtLeftToBePaid = currentEntry.amount - currentEntry.clearedAmount;
    
    if(calculatedPercentage == 0 || calculatedPercentage > amtLeftToBePaid) return(amtLeftToBePaid, PaymentType.FULL);
    else return(calculatedPercentage, PaymentType.PARTIAL);

  }

  function _handlePayment(uint256 currentEntryId, uint256 amountToPay, PaymentType paymentType) internal returns(uint256) {
    uint256 nextId = entries[currentEntryId].next;
    if(paymentType == PaymentType.FULL) {
      _payFull(currentEntryId);
    } else {
      _payPartial(currentEntryId, amountToPay);
    }
    return nextId;
  }

  function _payFull(uint256 currentEntryId) internal {
    Entry storage currentEntry = entries[currentEntryId];
    currentEntry.isActive = false;
    _removeEntry(currentEntryId);
    lockedAssets[currentEntry.from] -= currentEntry.amount;
    payable(currentEntry.to).transfer(currentEntry.amount);
    emit ProcessedPayment(currentEntry.id, currentEntry.from, currentEntry.to, currentEntry.amount, currentEntry.amount, PaymentType.FULL);
  }

  function _removeEntry(uint256 currentEntryId) internal {
    Entry storage currentEntry = entries[currentEntryId];
    if(currentEntry.prev == 0 && currentEntry.next == 0) {
      // Only one entry exists
      startPoint = 0;
      endPoint = 0;
    } else if(currentEntry.prev == 0 && currentEntry.next != 0) {
      // First entry
      startPoint = currentEntry.next;
      entries[currentEntry.next].prev = 0;
    } else if(currentEntry.prev != 0 && currentEntry.next == 0) {
      // Last entry
      endPoint = currentEntry.prev;
      entries[currentEntry.prev].next = 0;
    } else {
      // Middle entry
      entries[currentEntry.prev].next = currentEntry.next;
      entries[currentEntry.next].prev = currentEntry.prev;
    }
    delete entries[currentEntryId];
    noOfEntries -= 1;
  }

  function _payPartial(uint256 currentEntryId, uint256 amountToPay) internal {
    Entry storage currentEntry = entries[currentEntryId];
    currentEntry.clearedAmount += amountToPay;
    lockedAssets[currentEntry.from] -= amountToPay;
    payable(currentEntry.to).transfer(amountToPay);
    emit ProcessedPayment(currentEntry.id, currentEntry.from, currentEntry.to, amountToPay, currentEntry.amount, PaymentType.PARTIAL);
  }

}

