pragma solidity 0.8.9;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/IERC1155Upgradeable.sol";

import "./IMarketplace.sol";
import "./IMessageStructures.sol";
import "./VerifySignature.sol";

import "hardhat/console.sol";

contract Marketplace is
    Ownable,
    ReentrancyGuard,
    IMarketplace,
    IMessageStructures
{
    struct Sale {
        address seller;
        address tokenContractAddress;
        uint256 tokenId;
        uint256 floorPrice;
        uint256 startDate;
        uint256 endDate;
        mapping(address => uint256) offers;
        address highestBidder; // Do we need other bidders?
        uint256 highestBid; // Do we need other bids?
        // bool isSold; // if an item gets sold, it is deleted
        string saleType;
    }

    enum BuyType {
        Buy,
        AcceptOffer
    }

    event SaleCreated(
        address indexed seller,
        address indexed tokenContractAddress,
        uint256 tokenId,
        uint256 floorPrice,
        uint256 startDate,
        uint256 endDate,
        string saleType,
        uint256 nonce
    );

    event SaleCancelled(address indexed tokenContractAddress, uint256 tokenId);

    event SalePriceEdited(
        address indexed tokenContractAddress,
        uint256 tokenId,
        uint256 newPrice,
        uint256 nonce
    );

    event SaleSuccessful(
        address indexed tokenContractAddress,
        uint256 tokenId,
        uint256 salePrice,
        address indexed buyer,
        string saleType
    );

    event OfferCreated(
        address tokenContractAddress,
        uint256 tokenId,
        uint256 offerPrice,
        address offerer,
        address paymentToken,
        uint256 nonce
    );

    event OfferPriceEdited(
        address indexed tokenContractAddress,
        uint256 tokenId,
        address offerer,
        uint256 newOfferPrice,
        uint256 nonce
    );

    event OfferCancelled(
        address tokenContractAddress,
        uint256 tokenId,
        address offerer
    );

    event OfferAccepted(
        address indexed tokenContractAddress,
        uint256 tokenId,
        address indexed offerer,
        uint256 offer
    );

    // ToDo: should we support multiple payment tokens in the future?
    IERC20Upgradeable public paymentToken;
    // ToDo: listing, offering, bidding or buying fees??!
    DomainSeparator public domain;

    // saleByNFT: [contract address][tokenId] => Sale
    mapping(address => mapping(uint256 => Sale)) public saleByNFT;

    // nonces: [user address] => nonce
    mapping(address => uint256) public nonces;

    constructor(
        string memory _name,
        string memory _version,
        bytes32 salt,
        IERC20Upgradeable _paymentToken
    ) {
        domain.name = _name;
        domain.version = _version;
        domain.chainId = block.chainid;
        domain.verifyingContract = address(this);
        domain.salt = salt;
        paymentToken = _paymentToken;
    }

    // TODO: We have only single listing, batch listing may be added in the future
    function createSale(
        ListingMessage memory listingMessage,
        bytes memory signature
    ) external onlyOwner {
        // validations:
        require(
            listingMessage.startDate > block.timestamp,
            "Start date is in the past"
        );

        if (
            keccak256(abi.encode(listingMessage.saleType)) ==
            keccak256("Auction")
        ) {
            require(
                listingMessage.startDate < listingMessage.endDate,
                "Invalid end date"
            );
        }

        require(
            listingMessage.expirationDate >= block.timestamp,
            "Signature is expired"
        );

        require(
            listingMessage.nonce == nonces[listingMessage.seller] + 1,
            "Wrong nonce"
        );
        nonces[listingMessage.seller] = listingMessage.nonce;

        // verifying the signature
        require(
            VerifySignature.verifyListing(
                listingMessage.seller,
                listingMessage,
                domain,
                signature
            ),
            "Wrong signature provided"
        );

        if (
            IERC721Upgradeable(listingMessage.tokenContractAddress)
                .supportsInterface(type(IERC721Upgradeable).interfaceId)
        ) {
            require(
                IERC721Upgradeable(listingMessage.tokenContractAddress).ownerOf(
                    listingMessage.tokenId
                ) == listingMessage.seller,
                "Seller is not the owner"
            );

            require(
                IERC721Upgradeable(listingMessage.tokenContractAddress)
                    .isApprovedForAll(listingMessage.seller, address(this)) ||
                    IERC721Upgradeable(listingMessage.tokenContractAddress)
                        .getApproved(listingMessage.tokenId) ==
                    address(this),
                "NFT is not approved for the marketplace"
            );
        } else if (
            IERC1155Upgradeable(listingMessage.tokenContractAddress)
                .supportsInterface(type(IERC1155Upgradeable).interfaceId)
        ) {
            require(
                IERC1155Upgradeable(listingMessage.tokenContractAddress)
                    .balanceOf(listingMessage.seller, listingMessage.tokenId) !=
                    0,
                "Seller is not the owner"
            );
            require(
                IERC1155Upgradeable(listingMessage.tokenContractAddress)
                    .isApprovedForAll(listingMessage.seller, address(this)),
                "NFT is not approved for the marketplace"
            );
        } else {
            revert("The contract is not supported");
        }

        if (
            keccak256(abi.encode(listingMessage.saleType)) ==
            keccak256("FixedPrice")
        ) {
            require(
                listingMessage.endDate == 0,
                "End date is 0 in fixed price"
            );
        }

        // save on-chain data:

        // @dev: Struct containing a (nested) mapping cannot be constructed.
        saleByNFT[listingMessage.tokenContractAddress][listingMessage.tokenId]
            .seller = listingMessage.seller;
        saleByNFT[listingMessage.tokenContractAddress][listingMessage.tokenId]
            .tokenContractAddress = listingMessage.tokenContractAddress;
        saleByNFT[listingMessage.tokenContractAddress][listingMessage.tokenId]
            .tokenId = listingMessage.tokenId;
        saleByNFT[listingMessage.tokenContractAddress][listingMessage.tokenId]
            .floorPrice = listingMessage.floorPrice;
        saleByNFT[listingMessage.tokenContractAddress][listingMessage.tokenId]
            .startDate = listingMessage.startDate;
        saleByNFT[listingMessage.tokenContractAddress][listingMessage.tokenId]
            .endDate = listingMessage.endDate;
        saleByNFT[listingMessage.tokenContractAddress][listingMessage.tokenId]
            .highestBidder = address(0);

        saleByNFT[listingMessage.tokenContractAddress][listingMessage.tokenId]
            .highestBid = uint256(0);
        saleByNFT[listingMessage.tokenContractAddress][listingMessage.tokenId]
            .saleType = listingMessage.saleType;

        // emit the event
        emit SaleCreated(
            listingMessage.seller,
            listingMessage.tokenContractAddress,
            listingMessage.tokenId,
            listingMessage.floorPrice,
            listingMessage.startDate,
            listingMessage.endDate,
            listingMessage.saleType,
            listingMessage.nonce
        );
    }

    /**
     * @dev This function must be called upon user request or when a sale is ended or the NFT is transfered to other accounts
     */
    function cancelSale(address tokenContractAddress, uint256 tokenId)
        external
        onlyOwner
    {
        _cancelSale(tokenContractAddress, tokenId);
    }

    function _cancelSale(address tokenContractAddress, uint256 tokenId)
        internal
    {
        require(
            saleByNFT[tokenContractAddress][tokenId].seller != address(0),
            "The sale does not exist"
        );
        // delete the sale from mappings
        // @dev: Struct containing a (nested) mapping cannot be constructed.
        saleByNFT[tokenContractAddress][tokenId].seller = address(0);

        // ToDO: can only delete the seller
        saleByNFT[tokenContractAddress][tokenId].tokenContractAddress = address(
            0
        );
        saleByNFT[tokenContractAddress][tokenId].tokenId = uint256(0);
        saleByNFT[tokenContractAddress][tokenId].floorPrice = uint256(0);
        saleByNFT[tokenContractAddress][tokenId].startDate = uint256(0);
        saleByNFT[tokenContractAddress][tokenId].endDate = uint256(0);
        saleByNFT[tokenContractAddress][tokenId].highestBidder = address(0);
        saleByNFT[tokenContractAddress][tokenId].highestBid = uint256(0);
        saleByNFT[tokenContractAddress][tokenId].saleType = "";

        // emit the event
        emit SaleCancelled(tokenContractAddress, tokenId);
    }

    /// @dev olny for fixed price
    function editSalePrice(
        ListingMessage memory listingMessage,
        bytes memory signature
    ) external onlyOwner {
        require(
            saleByNFT[listingMessage.tokenContractAddress][
                listingMessage.tokenId
            ].seller != address(0),
            "The sale does not exist"
        );
        require(
            listingMessage.expirationDate >= block.timestamp,
            "Signature is expired"
        );

        require(
            listingMessage.nonce == nonces[listingMessage.seller] + 1,
            "Wrong nonce"
        );
        nonces[listingMessage.seller] = listingMessage.nonce;

        // verifying the signature
        require(
            VerifySignature.verifyListing(
                listingMessage.seller,
                listingMessage,
                domain,
                signature
            ),
            "Wrong signature provided"
        );

        saleByNFT[listingMessage.tokenContractAddress][listingMessage.tokenId]
            .floorPrice = listingMessage.floorPrice;

        emit SalePriceEdited(
            listingMessage.tokenContractAddress,
            listingMessage.tokenId,
            listingMessage.floorPrice,
            listingMessage.nonce
        );
    }

    function buy(
        address[] calldata tokenContractAddresses,
        uint256[] calldata tokenIds
    ) external nonReentrant {
        require(
            tokenContractAddresses.length == tokenIds.length,
            "Length mismatch"
        );

        for (uint256 i = 0; i < tokenContractAddresses.length; i++) {
            _buy(
                saleByNFT[tokenContractAddresses[i]][tokenIds[i]],
                BuyType.Buy,
                _msgSender(),
                address(0)
            );
        }
    }

    function makeOffer(OfferMessage memory offerMessage, bytes memory signature)
        external
        onlyOwner
    {
        require(
            saleByNFT[offerMessage.tokenContractAddress][offerMessage.tokenId]
                .seller != address(0),
            "The sale does not exist"
        );
        require(
            offerMessage.expirationDate >= block.timestamp,
            "Signature is expired"
        );

        require(
            offerMessage.nonce == nonces[offerMessage.offerer] + 1,
            "Wrong nonce"
        );
        nonces[offerMessage.offerer] = offerMessage.nonce;

        // verifying the signature
        require(
            VerifySignature.verifyOffer(
                offerMessage.offerer,
                offerMessage,
                domain,
                signature
            ),
            "Wrong signature provided"
        );

        require(
            offerMessage.paymentToken == address(paymentToken),
            "Wrong payment token"
        );

        require(
            paymentToken.allowance(offerMessage.offerer, address(this)) >=
                offerMessage.offerPrice,
            "Offer price not approved"
        );

        require(
            paymentToken.balanceOf(offerMessage.offerer) >=
                offerMessage.offerPrice,
            "Offerer does not own enough tokens"
        );

        require(offerMessage.offerPrice > 0, "Can not offer 0");

        saleByNFT[offerMessage.tokenContractAddress][offerMessage.tokenId]
            .offers[offerMessage.offerer] = offerMessage.offerPrice;

        emit OfferCreated(
            offerMessage.tokenContractAddress,
            offerMessage.tokenId,
            offerMessage.offerPrice,
            offerMessage.offerer,
            offerMessage.paymentToken,
            offerMessage.nonce
        );
    }

    function editOfferPrice(
        OfferMessage memory offerMessage,
        bytes memory signature
    ) external onlyOwner {
        require(
            saleByNFT[offerMessage.tokenContractAddress][offerMessage.tokenId]
                .seller != address(0),
            "The sale does not exist"
        );
        require(
            saleByNFT[offerMessage.tokenContractAddress][offerMessage.tokenId]
                .offers[offerMessage.offerer] != 0,
            "Offer does not exist"
        );
        require(
            offerMessage.expirationDate >= block.timestamp,
            "Signature is expired"
        );

        require(
            offerMessage.nonce == nonces[offerMessage.offerer] + 1,
            "Wrong nonce"
        );
        nonces[offerMessage.offerer] = offerMessage.nonce;

        // verifying the signature
        require(
            VerifySignature.verifyOffer(
                offerMessage.offerer,
                offerMessage,
                domain,
                signature
            ),
            "Wrong signature provided"
        );

        saleByNFT[offerMessage.tokenContractAddress][offerMessage.tokenId]
            .offers[offerMessage.offerer] = offerMessage.offerPrice;

        emit OfferPriceEdited(
            offerMessage.tokenContractAddress,
            offerMessage.tokenId,
            offerMessage.offerer,
            offerMessage.offerPrice,
            offerMessage.nonce
        );
    }

    function cancleOffer(
        address tokenContractAddress,
        uint256 tokenId,
        address offerer
    ) external onlyOwner {
        require(
            saleByNFT[tokenContractAddress][tokenId].seller != address(0),
            "The sale does not exist"
        );
        require(
            saleByNFT[tokenContractAddress][tokenId].offers[offerer] != 0,
            "Offer does not exist"
        );
        saleByNFT[tokenContractAddress][tokenId].offers[offerer] = uint256(0);

        emit OfferCancelled(tokenContractAddress, tokenId, offerer);
    }

    function acceptOffer(
        address tokenContractAddress,
        uint256 tokenId,
        address offerer,
        uint256 offer
    ) external nonReentrant {
        require(
            saleByNFT[tokenContractAddress][tokenId].seller != address(0),
            "The sale does not exist"
        );
        require(
            saleByNFT[tokenContractAddress][tokenId].offers[offerer] != 0,
            "Offer does not exist"
        );
        require(
            saleByNFT[tokenContractAddress][tokenId].seller == _msgSender(),
            "Caller is not the seller"
        );

        require(
            saleByNFT[tokenContractAddress][tokenId].offers[offerer] == offer,
            "Wrong offer price"
        );

        emit OfferAccepted(tokenContractAddress, tokenId, offerer, offer);

        _buy(
            saleByNFT[tokenContractAddress][tokenId],
            BuyType.AcceptOffer,
            address(0),
            offerer
        );
    }

    function _buy(
        Sale storage sale,
        BuyType _buyType,
        address _buyer,
        address offerer
    ) internal {
        require(sale.seller != address(0), "The sale does not exist");

        address buyer = (_buyType == BuyType.Buy) ? _buyer : offerer;
        uint256 price = (_buyType == BuyType.Buy)
            ? sale.floorPrice
            : sale.offers[buyer];
        string memory buyType = (_buyType == BuyType.Buy)
            ? "Buy"
            : "AcceptOffer";

        paymentToken.transferFrom(buyer, sale.seller, price);

        if (
            IERC721Upgradeable(sale.tokenContractAddress).supportsInterface(
                type(IERC721Upgradeable).interfaceId
            )
        ) {
            IERC721Upgradeable(sale.tokenContractAddress).safeTransferFrom(
                sale.seller,
                buyer,
                sale.tokenId,
                ""
            );
        } else if (
            IERC1155Upgradeable(sale.tokenContractAddress).supportsInterface(
                type(IERC1155Upgradeable).interfaceId
            )
        ) {
            IERC1155Upgradeable(sale.tokenContractAddress).safeTransferFrom(
                sale.seller,
                buyer,
                sale.tokenId,
                1,
                ""
            );
        }

        emit SaleSuccessful(
            sale.tokenContractAddress,
            sale.tokenId,
            price,
            buyer,
            buyType
        );

        _cancelSale(sale.tokenContractAddress, sale.tokenId);
    }
}

