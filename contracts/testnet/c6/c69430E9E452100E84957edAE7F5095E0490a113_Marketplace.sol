pragma solidity 0.8.9;

interface IMessageStructures {
    struct ListingMessage {
        address seller;
        address tokenContractAddress;
        uint256 tokenId;
        uint256 floorPrice;
        uint256 startDate;
        uint256 endDate; // end date for auctions
        uint256 expirationDate; // expiration of this signed message
        uint256 nonce;
        string saleType;
    }

    struct OfferMessage {
        address offerer;
        address tokenContractAddress;
        // bytes32 merkle_root; // what is this for? e.g. 0xf6f91122bcb014562cd86d9167739bd02785c3f314f3a7a89ce6017388a8168e
        address paymentToken; // WAVAX = 0xb31f66aa3c1e785363f0875a1b74e27b85fd66c7
        uint256 tokenId;
        uint256 offerPrice;
        uint256 expirationDate; // expiration of this signed message
        uint256 nonce;
    }

    struct DomainSeparator {
        string name;
        string version;
        uint256 chainId;
        address verifyingContract; // e.g. the Marketplace contract
        bytes32 salt;
    }
}

