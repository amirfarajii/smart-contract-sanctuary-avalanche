import "./IMessageStructures.sol";

interface IMarketplace {
    function createSale(
        IMessageStructures.ListingMessage memory listingMessage,
        bytes memory signature
    ) external;

    function cancelSale(address tokenContractAddress, uint256 tokenId) external;

    function editSalePrice(
        IMessageStructures.ListingMessage memory listingMessage,
        bytes memory signature
    ) external;

    function buy(
        address[] calldata tokenContractAddresses,
        uint256[] calldata tokenIds
    ) external;

    function makeOffer(
        IMessageStructures.OfferMessage memory offerMessage,
        bytes memory signature
    ) external;

    function editOfferPrice(
        IMessageStructures.OfferMessage memory offerMessage,
        bytes memory signature
    ) external;

    function cancleOffer(
        address tokenContractAddress,
        uint256 tokenId,
        address offerer
    ) external;

    function acceptOffer(
        address tokenContractAddress,
        uint256 tokenId,
        address offerer,
        uint256 offer
    ) external;
}

