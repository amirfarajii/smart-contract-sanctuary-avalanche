pragma solidity >=0.4.22 <0.9.0;


contract ReservedFundContract {
  
    uint256 public maxBalance;
    uint256 public balance;
    address public owner;
    address public token;
    uint256 public lastUnlockTime;
    IERC20 itoken;
    
    uint256 public unlockedTokens;
    uint public vestingCycles;


event TransferSent(address _from,address _destAddr,uint _amount);

       constructor(address _owner,IERC20 _itoken) {
            owner = _owner;
            token = address(_itoken);
            itoken = _itoken;
            lastUnlockTime = block.timestamp;
            unlockedTokens = 0;
            vestingCycles = 0;
       }

    function calc(uint x,uint256 y) internal{
            require(balance > 0,"no tokens");
            uint256 newTokens = maxBalance * x / 1000;
            if(newTokens > balance){
                newTokens = balance;
            }
            unlockedTokens += newTokens;
            lastUnlockTime += y;
            vestingCycles ++;
            balance -= newTokens;
    }

    //withdraw unlocked tokens to user address
    function WithdrawToAddress(address user,uint256 amount) public onlyOwner{
        require(amount <= unlockedTokens,"Not enough unlocked tokens");
        itoken.transfer(user,amount);
        unlockedTokens -= amount;
        emit TransferSent(msg.sender,user,amount);
    }

    //unlock vested tokens if ready
    function Unlock() public onlyOwner{
          if (maxBalance == 0){
            uint256 newBalance = itoken.balanceOf(address(this));
            maxBalance = newBalance;
            balance = newBalance;
        }

        //6 months cliff
        if (vestingCycles == 0){
            require(block.timestamp > lastUnlockTime + 180 days,"Too early for unlocking tokens");
            calc(0,180 days);
            return;
        }
        if (balance <= 0){
            uint256 newBalance = itoken.balanceOf(address(this));
            balance += newBalance;
        }
        //6 months linear vesting
        //unlock 16.7 each month (for 6 months)
        if (vestingCycles > 0){
            require(block.timestamp > lastUnlockTime + 30 days,"Too early for unlocking tokens");
            calc(167,30 days);
        }
    }

    function Status() public view returns(uint256 status){
        uint256 countdown = block.timestamp - lastUnlockTime;
       if (vestingCycles == 0){
           if (countdown >= 210 days){
               return 0;
           }else {
               return 210 days - countdown;
           }
        }
        else{
           if (countdown >= 30 days){
               return 0;
           }else {
               return 30 days - countdown;
           } 
        }
    }


    modifier onlyOwner() {
        require(msg.sender == owner, "Ownable: caller is not the owner");
        _;
    }

    function transferOwnership(address newOwner) public onlyOwner {
        require(newOwner != address(0), "can't have owner with zero address");
        owner = newOwner;
    }
}
