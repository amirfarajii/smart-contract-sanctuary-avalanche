pragma solidity >=0.4.22 <0.9.0;


contract AdvisorsContract {
    uint256 public maxBalance;
    uint256 public balance;
    address public owner;
    address public token;
    uint256 public lastUnlockTime;
    IERC20 itoken;
    
    uint8 public vestingCycles;
    mapping(address => uint256) public whiteList;

    mapping(address => uint256) public ownedBFG;
    mapping(address => uint256) public lockedBFG;

    event TransferSent(address _from,address _destAddr,uint _amount);

        constructor(address _owner, IERC20 _itoken) {
            owner = _owner;
            token = address(_itoken);
            itoken = _itoken;
            lastUnlockTime = 1665199800;
            vestingCycles = 0;
       }

    function addWhitelist(address user,uint256 amount) public onlyOwner{
        if (balance == 0){
                uint256 _balance = itoken.balanceOf(address(this));
                balance = _balance;
                maxBalance = _balance;
        }
        require(balance > 0,"no BFG available");
        require(amount > 10, "min amount to send");
        require(balance > amount,"not enough BFG available");


        //send TGE
        uint256 amountTGE = amount * 10 / 100;
        balance -= amount;
        itoken.transfer(user,amountTGE);
        
        ownedBFG[user] += amount;
        lockedBFG[user] += (amount - amountTGE);
        emit TransferSent(address(this),user,amountTGE);
    }

    function getDaysUnlocked(uint8 daysPast, address _receiver) internal{
		//tokens for 1 day
		uint256 newTokens = maxBalance * 136 / 10000;
		//tokens for daysPast days
		uint256 calTokens = newTokens * daysPast;

        //transfer
		itoken.transfer(_receiver,calTokens);
        lockedBFG[_receiver] -= calTokens;
		
		vestingCycles = vestingCycles + (daysPast-1);
		lastUnlockTime = block.timestamp;

        emit TransferSent(address(this),_receiver,calTokens);
	}

    function Withdraw() public {
        require(ownedBFG[msg.sender] > 0,"Not WhiteListed or no more tokens to Claim");
        require(lockedBFG[msg.sender] > 0,"no unlocked BFG");
      
        //unlock
         if (maxBalance == 0){
            uint256 newBalance = itoken.balanceOf(address(msg.sender));
            maxBalance = newBalance;
            balance = newBalance;
        }
        //12 months cliff
		if(vestingCycles == 0){
			require(block.timestamp > lastUnlockTime + 360 days, "Too early for unlocking tokens");
			lastUnlockTime = lastUnlockTime + 360 days;
            vestingCycles ++;
		}

        //after 12 months, for 24 Months 0.135% each day linaer
		if(vestingCycles > 0 && vestingCycles <= 821){
			require(block.timestamp > lastUnlockTime, "Too early for unlock");
			//need to calculate days
			uint8 daysPast = uint8((block.timestamp - lastUnlockTime) / 60 / 60 / 24);
			require(daysPast > 0, "Too early for unlock");
			getDaysUnlocked(daysPast,msg.sender);
		}
        //send the rest after last vesting
		if(vestingCycles > 821){
            require(lockedBFG[msg.sender] > 0, "No more tokens for unlock");
			//transfer
			itoken.transfer(msg.sender,lockedBFG[msg.sender]);
            lockedBFG[msg.sender] = 0;
            
            emit TransferSent(address(this),msg.sender,lockedBFG[msg.sender]);
		}
    }

    modifier onlyOwner() {
        require(msg.sender == owner, "Ownable: caller is not the owner");
        _;
    }

    function transferOwnership(address newOwner) public onlyOwner {
        require(newOwner != address(0), "can't have owner with zero address");
        owner = newOwner;
    }

}

