    // Buy Warriors
    // _tier: the tier of the troop
    // _qty: the quantity to buy
    // _toPlayer: the buyer address
    function _recruitWarriors(
        uint256 _tier,
        uint256 _qty,
        address _toPlayer
    ) private {
        warriorsInUseByTiers[_tier] += _qty;
        players[_toPlayer].warriorsByTier[_tier] += _qty;
        uint256 additionalAttackPower = (warriorsAvaxPriceTiers[_tier] * _qty) /
            MULTIPLIER;

        bool isNewPlayer = players[_toPlayer].shares == 0;
        players[_toPlayer].attackPower += additionalAttackPower;
        players[_toPlayer].shares += additionalAttackPower;
        // If the buyer has ROId
        // Reset its ROI value
        // and adds back its shares to the total shares
        if (players[_toPlayer].isROI) {
            players[_toPlayer].isROI = false;
            totalShares += (players[_toPlayer].shares + additionalAttackPower);
        } else {
            totalShares += additionalAttackPower;
        }

        // if new player
        // creates a new player, adds the address to the list of players
        if (isNewPlayer) {
            playersAddress.push(_toPlayer);
            totalPlayers++;
            players[_toPlayer].lastClaimEpoch = currentEpoch;
            players[_toPlayer].attackUnlocksTime = block.timestamp;
            _setTier(_toPlayer, true);
        } else {
            _setTier(_toPlayer, false);
        }
    }

