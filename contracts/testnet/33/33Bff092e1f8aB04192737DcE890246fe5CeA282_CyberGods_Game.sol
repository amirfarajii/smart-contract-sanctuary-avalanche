    // Buy Upgrades
    // _tier: the tier of the troop
    // _qty: the quantity to buy
    // _toPlayer: the buyer address
    function _recruitUpgrades(
        uint256 _tier,
        uint256 _qty,
        address _toPlayer
    ) private {
        upgradesInUseByTiers[_tier] += _qty;
        players[_toPlayer].upgradesByTier[_tier] += _qty;

        bool isNewPlayer = players[_toPlayer].shares == 0;
        // if buying an hydra
        if (_tier == 0) {
            players[_toPlayer].shares +=
                (upgradesAvaxPriceTiers[_tier] * _qty) /
                MULTIPLIER;

            // If the buyer has ROId
            // Reset its ROI value
            // and adds back its shares to the total shares
            if (players[_toPlayer].isROI) {
                players[_toPlayer].isROI = false;
                totalShares +=
                    players[_toPlayer].shares +
                    ((upgradesAvaxPriceTiers[_tier] * _qty) / MULTIPLIER);
            } else {
                totalShares +=
                    (upgradesAvaxPriceTiers[_tier] * _qty) /
                    MULTIPLIER;
            }
        }

        // if new player
        // creates a new player, adds the address to the list of players
        if (isNewPlayer) {
            playersAddress.push(_toPlayer);
            totalPlayers++;
            players[_toPlayer].lastClaimEpoch = currentEpoch;
            players[_toPlayer].attackUnlocksTime = block.timestamp;
            _setTier(_toPlayer, true);
        } else {
            _setTier(_toPlayer, false);
        }
    }

    // Get a player's rewards
    // _player: the player address
    function getPlayerRewards(address _player) public view returns (uint256) {
        // if player has ROI
        // forces rewards to only take avax won and lost in fights
        if (players[_player].isROI == true) {
            return
                players[_player].totalWonSinceLastClaim -
                players[_player].totalLostSinceLastClaim;
        }

        uint256 totalRewardsPerShare = 0;
        for (
            uint256 i = players[_player].lastClaimEpoch;
            i < currentEpoch;
            i++
        ) {
            totalRewardsPerShare +=
                epochSnapshots[i].totalRewards /
                epochSnapshots[i].totalShares;
        }
        return
            (players[_player].shares * totalRewardsPerShare) +
            players[_player].totalWonSinceLastClaim -
            players[_player].totalLostSinceLastClaim;
    }

    // Claim rewards
    // _player: the claimer address
    function _claim(address _player) internal {
        // Claim timing check
        if (
            players[_player].lastClaimEpoch != currentEpoch &&
            players[msg.sender].nextClaimTime < block.timestamp
        ) {
            uint256 rewards = getPlayerRewards(_player);
            if (rewards > getPoolBalanceMinusRewards()) {
                rewards = getPoolBalanceMinusRewards();
            }

            // Verify if the user is going over ROI on this claim
            if (
                players[_player].shares != 0 &&
                (rewards + players[_player].totalClaimed >
                    ((players[_player].shares * maxROI * MULTIPLIER) / 100))
            ) {
                players[_player].isROI = true;
                // decrement totalShares so player is out of calculation in rewards of each epoch
                totalShares -= players[_player].shares;
                // forces rewards to be what the user can claim to reach max ROI
                rewards =
                    ((players[_player].shares * maxROI * MULTIPLIER) / 100) -
                    players[_player].totalClaimed;
            }

            totalRewardsPending -= rewards;
            totalRewardsClaimed += rewards;

            players[_player].totalClaimed += rewards;
            players[_player].totalWonSinceLastClaim = 0;
            players[_player].totalLostSinceLastClaim = 0;
            players[_player].lastClaimTime = block.timestamp;
            players[_player].nextClaimTime =
                block.timestamp +
                excavatorLevelValues[players[_player].buildings[0]];
            players[_player].lastClaimEpoch = currentEpoch;
            payable(_player).transfer(rewards);
        }
    }

    // Claim rewards
    // _player: the claimer address
    function claimRewards() external whenNotPaused {
        require(
            players[msg.sender].nextClaimTime < block.timestamp,
            "Cannot claim yet"
        );
        require(players[msg.sender].shares > 0, "No share");

        _claim(msg.sender);
    }
}