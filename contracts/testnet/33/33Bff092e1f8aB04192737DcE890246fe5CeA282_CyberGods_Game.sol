    // Building Actions
    // Upgrades a building for the caller
    // _buildingId: the building Id
    function buildingUpgrade(uint256 _buildingId)
        external
        payable
        whenNotPaused
    {
        require(
            _buildingId >= EXCAVATOR_ID && _buildingId <= ROBBERY_SCHOOL_ID,
            "Invalid Building"
        );

        uint256 currentBuildingLevel = players[msg.sender].buildings[
            _buildingId
        ];

        if (_buildingId == EXCAVATOR_ID) {
            require(
                currentBuildingLevel + 1 <= excavatorLevelPrices.length,
                "Excavator Already Max Level"
            );
            require(
                msg.value >= excavatorLevelPrices[currentBuildingLevel],
                "Value below price"
            );
        } else if (_buildingId == GARAGE_ID) {
            require(
                currentBuildingLevel + 1 <= garageLevelPrices.length,
                "Garage Already Max Level"
            );
            require(
                msg.value >= garageLevelPrices[currentBuildingLevel],
                "Value below price"
            );
        } else if (_buildingId == RADAR_ID) {
            require(
                currentBuildingLevel + 1 <= radarLevelPrices.length,
                "Radar Already Max Level"
            );
            require(
                msg.value >= radarLevelPrices[currentBuildingLevel],
                "Value below price"
            );
        } else if (_buildingId == ROBBERY_SCHOOL_ID) {
            require(
                currentBuildingLevel + 1 <= robberySchoolLevelPrices.length,
                "Robbery School Already Max Level"
            );
            require(
                msg.value >= robberySchoolLevelPrices[currentBuildingLevel],
                "Value below price"
            );
        }

        players[msg.sender].buildings[_buildingId] = currentBuildingLevel + 1;
        // (bool sent, ) = payable(feesManagerAddress).call{value: msg.value}("");
        // if (!sent) revert FailedToSendEther();
    }

    struct PlayerInTier {
        address playerAddress;
        string username;
        uint256 rewards;
        uint256 defensePower;
        uint256 hydras;
    }

    // Get the list of players in a tier
    // _tier: the tier value
    function getPlayersInTier(uint256 _minRange, uint256 _maxRange)
        external
        view
        returns (PlayerInTier[] memory)
    {
        // Get the number of player in the tier
        uint256 playerCounts = 0;
        for (uint256 i = _minRange; i <= _maxRange; i++) {
            playerCounts += playersInTiers[i];
        }

        PlayerInTier[] memory playersInTier = new PlayerInTier[](playerCounts);
        uint256 y = 0;

        // Loop over all players
        for (uint256 i = 0; i < playersAddress.length; i++) {
            address playerAddress = playersAddress[i];
            // Verify that the player isn't the caller and that the player is in the same tier

            if (
                (players[playerAddress].tier >= _minRange &&
                    players[playerAddress].tier <= _maxRange)
            ) {
                PlayerInTier memory pit = PlayerInTier(
                    playerAddress,
                    playersToUsername[playerAddress],
                    getPlayerRewards(playerAddress),
                    players[playerAddress].defensePower,
                    players[msg.sender].buildings[2] > 0
                        ? players[playerAddress].upgradesByTier[0]
                        : 0
                );
                playersInTier[y] = pit;
                y++;
            }
        }
        return playersInTier;
    }

    // Attack a player
    // _playerAddress: the target address
    function attackPlayer(address _playerAddress) external whenNotPaused {
        require(
            players[msg.sender].attackUnlocksTime < block.timestamp,
            "Cannot attack yet"
        );

        require(msg.sender != _playerAddress, "Cannot attack this player");
        require(players[msg.sender].tier == players[_playerAddress].tier);

        uint256 attackPower = players[msg.sender].attackPower;
        uint256 defensePower = players[_playerAddress].defensePower;

        // Verify if the player has Hydras
        if (players[_playerAddress].upgradesByTier[0] > 0) {
            defensePower += (players[_playerAddress].upgradesByTier[0] *
                (upgradesAvaxPriceTiers[0] / MULTIPLIER));
        }

        players[msg.sender].attacksStats[0]++;
        // Set next attack timer based on caller's Garage level
        players[msg.sender].attackUnlocksTime =
            block.timestamp +
            garageLevelValues[players[msg.sender].buildings[1]];

        players[_playerAddress].defensesStats[0]++;
        if (attackPower > defensePower) {
            // Calculate how much rewards the attacker gets based on its Robbery School level
            uint256 defenserRewards = (getPlayerRewards(_playerAddress) *
                robberySchoolLevelValues[players[msg.sender].buildings[3]]) /
                100;

            players[msg.sender].totalWonSinceLastClaim += defenserRewards;
            players[msg.sender].attacksStats[1]++;
            players[msg.sender].attacksStats[3] += defenserRewards;

            players[_playerAddress].totalLostSinceLastClaim += defenserRewards;
            players[_playerAddress].defensesStats[2]++;
            players[_playerAddress].defensesStats[3] += defenserRewards;
            emit AttackResult(msg.sender, _playerAddress);
        } else {
            players[msg.sender].attacksStats[2]++;
            players[_playerAddress].defensesStats[1]++;
            emit AttackResult(_playerAddress, msg.sender);
        }
    }

    // Set the tier of a player
    // _toPlayer: the player address
    // isNewPlayer: defines if the player is new to the game
    function _setTier(address _toPlayer, bool isNewPlayer) private {
        // If the player isn't new
        // Decrement the count of players in the player's tier
        if (!isNewPlayer) {
            playersInTiers[players[_toPlayer].tier]--;
        }

        uint256 playerShare = players[_toPlayer].shares;
        for (uint256 i = 0; i < ranks.length; i++) {
            // Verify in which tier the player should be based on its shares
            if (
                i == ranks.length ||
                (playerShare >= ranks[i] && playerShare < ranks[i + 1])
            ) {
                players[_toPlayer].tier = i;
                playersInTiers[i]++;
                break;
            }
        }
    }

    // Verify all requirements before recruiting troops
    // _type: the type of troop (warrior, defense or upgrade)
    // _tier: the tier of troops
    // _qty: the quantity of troops being recruited
    // _toPlayer: the buyer address
    // _promotional: is it a gift
    function _recruitRequires(
        uint256 _type,
        uint256 _tier,
        uint256 _qty,
        address _toPlayer,
        bool _promotional
    ) private {
        require(_type >= 0 && _type <= 2, "Invalid type");
        require(_qty > 0, "Invalid count of troop to recruit");

        if (_type == 0) {
            require(
                _tier >= 0 && _tier <= 3,
                "Invalid tier. Valid range: 0 to 3"
            );
            require(
                warriorsInUseByTiers[_tier] + _qty <=
                    warriorsMaxSupplyTiers[_tier],
                "Max Supply reached for this tier"
            );
            if (!_promotional) {
                require(
                    msg.value >= warriorsAvaxPriceTiers[_tier] * _qty,
                    "Not enough AVAX"
                );
            }
        } else if (_type == 1) {
            require(
                _tier >= 0 && _tier <= 3,
                "Invalid tier. Valid range: 0 to 3"
            );
            require(
                defensesInUseByTiers[_tier] + _qty <=
                    defensesMaxSupplyTiers[_tier],
                "Max Supply reached for this tier"
            );
            if (!_promotional) {
                require(
                    msg.value >= defensesAvaxPriceTiers[_tier] * _qty,
                    "Not enough AVAX"
                );
            }
        } else if (_type == 2) {
            require(
                _tier >= 0 && _tier <= 1,
                "Invalid tier. Valid range: 0 to 1"
            );
            require(
                upgradesInUseByTiers[_tier] + _qty <=
                    upgradesMaxSupplyTiers[_tier],
                "Max Supply reached for this tier"
            );
            require(
                players[_toPlayer].upgradesByTier[_tier] + _qty <=
                    maxUpgradesPerTier[_tier],
                "Above limit for this upgrade tier"
            );
            if (!_promotional) {
                require(
                    msg.value >= upgradesAvaxPriceTiers[_tier] * _qty,
                    "Not enough AVAX"
                );
            }
        }
    }

    // Gift troops
    // _type: the type of troop (warrior, defense, upgrade)
    // _tier: the tier of the troop
    // _qty: the quantity to buy
    // _toPlayer: the buyer address
    function adminGiftRecruitTroops(
        uint256 _type,
        uint256 _tier,
        uint256 _qty,
        address _toPlayer
    ) external onlyOwner {
        _recruitRequires(_type, _tier, _qty, _toPlayer, true);

        if (_type == 0) {
            _recruitWarriors(_tier, _qty, _toPlayer);
        } else if (_type == 1) {
            _recruitDefenses(_tier, _qty, _toPlayer);
        } else if (_type == 2) {
            _recruitUpgrades(_tier, _qty, _toPlayer);
        }
    }

