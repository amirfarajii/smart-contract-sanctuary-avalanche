    /// @param monthlyRentWei The rent for 1 slot for 1 month in AVAX Wei
    function createValidatorRenting(
        bytes24 nodeIdBytes,
        uint16 subnetSlots,
        uint256 monthlyRentWei
    ) public whenNotPaused returns (address validatorRentingAddr) {
        validatorRentingAddr = IAshFactory(factoryAddr).createValidatorRenting(
            nodeIdBytes,
            subnetSlots,
            monthlyRentWei
        );
