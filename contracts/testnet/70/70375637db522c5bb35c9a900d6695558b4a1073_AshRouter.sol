    modifier onlyAshValidator(bytes24 nodeIdBytes) {
        require(
            IAshFactory(factoryAddr).validatorRentingContract(nodeIdBytes) !=
                address(0),
            "No ValidatorRenting contract exist for this NodeID."
        );
