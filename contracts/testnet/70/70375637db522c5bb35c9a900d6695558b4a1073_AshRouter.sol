    /// @param nodeIdBytes The NodeID of the validator for which to withdraw funds
    function withdrawAllValidatorFunds(bytes24 nodeIdBytes)
        public
        onlyOwner
        onlyAshValidator(nodeIdBytes)
    {
        IAshValidatorRentingUpgradeable(
            IAshFactory(factoryAddr).validatorRentingContract(nodeIdBytes)
        ).withdrawAllFunds(msg.sender);
    }

