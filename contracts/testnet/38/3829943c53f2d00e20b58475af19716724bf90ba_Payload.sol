pragma solidity ^0.8.0;

import 'forge-std/Script.sol';
import 'forge-std/console.sol';
import {IMandateProvider} from 'aave-crosschain-mandates/contracts/interfaces/IMandateProvider.sol';
import {CrossChainMandateUtils} from 'aave-crosschain-mandates/contracts/CrossChainMandateUtils.sol';

interface IPayload {
  event PayloadExecuted(string executed);

  function executed() external view returns (bool);

  function execute() external;
}

contract Payload is IPayload {
  bool public executed;

  function execute() external {
    executed = true;
    emit PayloadExecuted('Payload executed correctly');
  }
}

contract CreatePayloadOnExecChain is Script {
  address public constant MANDATE_PROVIDER =
    0x2B69a42761644184F9e29eEc5ea5bE2617c3982d;

  function run() public {
    vm.startBroadcast();

    Payload payload = new Payload();

    bytes32 ipfsHash = keccak256(bytes('This is the first test payload'));

    IMandateProvider.ExecutionAction[]
      memory actions = new IMandateProvider.ExecutionAction[](1);
    actions[0] = IMandateProvider.ExecutionAction({
      target: address(payload),
      withDelegateCall: true,
      accessLevel: CrossChainMandateUtils.AccessControl.Level_1,
      value: 0,
      signature: 'execute()',
      callData: ''
    });

    uint40 payloadId = IMandateProvider(MANDATE_PROVIDER).createPayload(
      actions,
      ipfsHash
    );

    console.log('payload id:', payloadId);

    vm.stopBroadcast();
  }
}