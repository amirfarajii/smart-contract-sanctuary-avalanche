pragma solidity >0.8.0 <0.9.0;

import "../utils/structs/EnumerableSet.sol";
import "../access/Ownable.sol";
import "../interfaces/IWitnetRandomness.sol";
import "hardhat/console.sol";

contract WitnetTest is Ownable {
    IWitnetRandomness private _witnet;
    uint256 private _latestRandomizingBlock;

    EnumerableSet.UintSet private _winningNumbers;

    using EnumerableSet for EnumerableSet.UintSet;

    receive() external payable {}
    
    constructor(address witnetAddress) {
        _witnet = IWitnetRandomness(witnetAddress);
    }

    function deleteNumbers() external onlyOwner() {
        delete _winningNumbers;
    }

    function getWinningNumbers() external view returns(uint[] memory) {
        return _winningNumbers.values();
    }

    function createRequest() public payable {
        _latestRandomizingBlock = block.number;
        
        uint _usedFunds = _witnet.randomize{ value: msg.value }();
        console.log("_usedFunds", _usedFunds);
        if (_usedFunds < msg.value) {
            payable(msg.sender).transfer(msg.value - _usedFunds);
        }
        console.log("latestRandomizingBlock", _latestRandomizingBlock);
    }

    function createNumbers() public {
        uint randomNonce = _latestRandomizingBlock;
        while (_winningNumbers.length() <= 5) {
            uint randomNumber = _witnet.random(45 + 1, uint(keccak256(abi.encodePacked(randomNonce))), _latestRandomizingBlock);
            _winningNumbers.add(randomNumber);
            console.log("randomNonce", randomNonce);
            randomNonce++;
        }       
    }
}

