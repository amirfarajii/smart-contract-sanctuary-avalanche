            _swap(swapAmountIn, uniswapRouterAddress, tokenToArbitrageAddress, tokenToSwapToAddress);  
            priceCheck = _getPrice(uniswapRouterAddress, tokenToArbitrageAddress, tokenToSwapToAddress, swapAmountIn);
            emit priceChecker(uniswapRouterAddress, tokenToArbitrageAddress, swapAmountIn, tokenToSwapToAddress, priceCheck);
            priceCheck = _getPrice(sushiswapRouterAddress, tokenToArbitrageAddress, tokenToSwapToAddress, swapAmountIn);
            emit priceChecker(sushiswapRouterAddress, tokenToArbitrageAddress, swapAmountIn, tokenToSwapToAddress, priceCheck);
 
            liquidity = getERC20Balance(sushiswapTokenPairAddress);
            removeLiq(tokenToSwapToAddress, tokenToAddToLiqAddress, liquidity, sushiswapRouterAddress);
            priceCheck = _getPrice(uniswapRouterAddress, tokenToArbitrageAddress, tokenToSwapToAddress, swapAmountIn);
            emit priceChecker(uniswapRouterAddress, tokenToArbitrageAddress, swapAmountIn, tokenToSwapToAddress, priceCheck);
            priceCheck = _getPrice(sushiswapRouterAddress, tokenToArbitrageAddress, tokenToSwapToAddress, swapAmountIn);
            emit priceChecker(sushiswapRouterAddress, tokenToArbitrageAddress, swapAmountIn, tokenToSwapToAddress, priceCheck);
            
        } else if (result == Exchange.SUSHI){
            priceCheck = _getPrice(uniswapRouterAddress, tokenToArbitrageAddress, tokenToSwapToAddress, swapAmountIn);
            emit priceChecker(uniswapRouterAddress, tokenToArbitrageAddress, swapAmountIn, tokenToSwapToAddress, priceCheck);
            priceCheck = _getPrice(sushiswapRouterAddress, tokenToArbitrageAddress, tokenToSwapToAddress, swapAmountIn);
            emit priceChecker(sushiswapRouterAddress, tokenToArbitrageAddress, swapAmountIn, tokenToSwapToAddress, priceCheck);
 
            _swap(swapAmountIn, sushiswapRouterAddress, tokenToArbitrageAddress, tokenToSwapToAddress);  
            priceCheck = _getPrice(uniswapRouterAddress, tokenToArbitrageAddress, tokenToSwapToAddress, swapAmountIn);
            emit priceChecker(uniswapRouterAddress, tokenToArbitrageAddress, swapAmountIn, tokenToSwapToAddress, priceCheck);
            priceCheck = _getPrice(sushiswapRouterAddress, tokenToArbitrageAddress, tokenToSwapToAddress, swapAmountIn);
            emit priceChecker(sushiswapRouterAddress, tokenToArbitrageAddress, swapAmountIn, tokenToSwapToAddress, priceCheck);
            
            addLiq(tokenToSwapToAddress, tokenToAddToLiqAddress, liqAmount1In, liqAmount2In, uniswapRouterAddress); 
            priceCheck = _getPrice(uniswapRouterAddress, tokenToArbitrageAddress, tokenToSwapToAddress, swapAmountIn);
            emit priceChecker(uniswapRouterAddress, tokenToArbitrageAddress, swapAmountIn, tokenToSwapToAddress, priceCheck);
            priceCheck = _getPrice(sushiswapRouterAddress, tokenToArbitrageAddress, tokenToSwapToAddress, swapAmountIn);
            emit priceChecker(sushiswapRouterAddress, tokenToArbitrageAddress, swapAmountIn, tokenToSwapToAddress, priceCheck);

            _swap(swapAmountIn, sushiswapRouterAddress, tokenToArbitrageAddress, tokenToSwapToAddress);  
            priceCheck = _getPrice(uniswapRouterAddress, tokenToArbitrageAddress, tokenToSwapToAddress, swapAmountIn);
            emit priceChecker(uniswapRouterAddress, tokenToArbitrageAddress, swapAmountIn, tokenToSwapToAddress, priceCheck);
            priceCheck = _getPrice(sushiswapRouterAddress, tokenToArbitrageAddress, tokenToSwapToAddress, swapAmountIn);
            emit priceChecker(sushiswapRouterAddress, tokenToArbitrageAddress, swapAmountIn, tokenToSwapToAddress, priceCheck);

            liquidity = getERC20Balance(uniswapTokenPairAddress);
            removeLiq(tokenToSwapToAddress, tokenToAddToLiqAddress, liquidity, uniswapRouterAddress);
            priceCheck = _getPrice(uniswapRouterAddress, tokenToArbitrageAddress, tokenToSwapToAddress, swapAmountIn);
            emit priceChecker(uniswapRouterAddress, tokenToArbitrageAddress, swapAmountIn, tokenToSwapToAddress, priceCheck);
            priceCheck = _getPrice(sushiswapRouterAddress, tokenToArbitrageAddress, tokenToSwapToAddress, swapAmountIn);
            emit priceChecker(sushiswapRouterAddress, tokenToArbitrageAddress, swapAmountIn, tokenToSwapToAddress, priceCheck);
        }
    }

    function _swap(
        uint256 amountIn,
        address routerAddress,
        address sell_token,
        address buy_token
    ) public returns (uint256) {
        IERC20(sell_token).approve(routerAddress, amountIn);

        uint256 amountOutMin = (_getPrice(
            routerAddress,
            sell_token,
            buy_token,
            amountIn
        ) * 95) / 100;

        address[] memory path = new address[](2);
        path[0] = sell_token;
        path[1] = buy_token;

        uint256 amountOut = IUniswapV2Router02(routerAddress)
            .swapExactTokensForTokens(
                amountIn,
                amountOutMin,
                path,
                address(this),
                block.timestamp
            )[1];
        return amountOut;
    }

    function _comparePrice(uint256 amount) public view returns (Exchange){
        uint256 uniswapPrice = _getPrice(
            uniswapRouterAddress,
            tokenToArbitrageAddress,
            tokenToSwapToAddress,
            amount
        );
        uint256 sushiswapPrice = _getPrice(
            sushiswapRouterAddress,
            tokenToArbitrageAddress,
            tokenToSwapToAddress,
            amount
        );

        // we try to sell an ERC20 at a higher price and buy it back at a lower price to make profit
        if(uniswapPrice > sushiswapPrice){
            require(
                _checkIfArbitrageIsProfitable(
                    amount,
                    uniswapPrice,
                    sushiswapPrice
                ),
                "Arbitrage not profitable"
            );
            return Exchange.UNI;
        } else if(uniswapPrice < sushiswapPrice){
            require(
                _checkIfArbitrageIsProfitable(
                    amount,
                    sushiswapPrice,
                    uniswapPrice
                ),
                "Arbitrage not profitable"
            );
            return Exchange.SUSHI;
        } else{
            return Exchange.NONE;
        }
    }

    function _checkIfArbitrageIsProfitable(
        uint256 amountIn,
        uint256 higherPrice,
        uint256 lowerPrice
    ) public pure returns (bool){
        // uniswap & sushiswap have 0.3% fee for every exchange
        // so gain made must be greater than 2 * 0.3% * arbitrage_amount

        // difference in ETH
        uint256 difference = ((higherPrice - lowerPrice) * 10**18) /
            higherPrice;

        uint256 payed_fee = (2 * (amountIn * 3)) / 1000;

        if (difference > payed_fee) {
            return true;
        } else {
            return false;
        }
    }

    function _getPrice(
        address routerAddress,
        address sell_token,
        address buy_token,
        uint256 amount
    ) public view returns (uint256){
        address[] memory pairs = new address[](2);
        pairs[0] = sell_token;
        pairs[1] = buy_token;
        uint256 price = IUniswapV2Router02(routerAddress).getAmountsOut(
            amount,
            pairs
        )[1];
        return price;
    }

    //--------------------------------------------------------------------
    // GETTER/SETTER FUNCTIONS

    /* This must be called before calling a flashloan so the contract knows which tokens are being arbitraged, swapped, liquidated, etc.
       Leaving this as a function rather than in the constructor allows us to perform the flashloan with different combinations of tokens,
       rather than redeploying every time we want to try a different combination
    */
    function setTokenAddresses(address _tokenToArbitrage, address _tokenToSwapTo, address _tokenToAddToLiq) onlyOwner public{
        tokenToArbitrageAddress = _tokenToArbitrage;
        tokenToSwapToAddress = _tokenToSwapTo;
        tokenToAddToLiqAddress = _tokenToAddToLiq;
        uniswapTokenPairAddress = IUniswapV2Factory(uniswapFactoryAddress).getPair(_tokenToSwapTo, _tokenToAddToLiq);
        sushiswapTokenPairAddress = IUniswapV2Factory(sushiswapFactoryAddress).getPair(_tokenToSwapTo, _tokenToAddToLiq);
    }

    function getERC20Balance(address _erc20Address) public view returns (uint256){
        return IERC20(_erc20Address).balanceOf(address(this));
    }

    //--------------------------------------------------------------------
    // LIQUIDITY FUNCTIONS

    // For ERC20 ⇄ ERC20 liquidity pools
    function addLiq(address _token1, address _token2, uint256 _amount1, uint256 _amount2, address _routerAddress) public {
        IERC20(_token1).approve(_routerAddress, _amount1);
        IERC20(_token2).approve(_routerAddress, _amount2);

        IUniswapV2Router02(_routerAddress).addLiquidity(_token1, _token2, _amount1, _amount2, 1, 1, address(this), block.timestamp+60);
    }

    // For ERC20 ⇄ ERC20 liquidity pools
    function removeLiq(address _token1, address _token2, uint256 _liquidity, address _routerAddress) public{
        IERC20(uniswapTokenPairAddress).approve(_routerAddress, _liquidity);
        IERC20(sushiswapTokenPairAddress).approve(_routerAddress, _liquidity);

        IUniswapV2Router02(_routerAddress).removeLiquidity(_token1, _token2, _liquidity, 1, 1, address(this), block.timestamp+60);
    }
}