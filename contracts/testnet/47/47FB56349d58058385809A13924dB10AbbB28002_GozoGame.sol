import "../VRFConsumerBaseV2.sol";
import "hardhat/console.sol";
import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "@chainlink/contracts/src/v0.8/interfaces/KeeperCompatibleInterface.sol";
import "@chainlink/contracts/src/v0.8/interfaces/LinkTokenInterface.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";

contract GozoGame is VRFConsumerBaseV2, Initializable, AccessControlUpgradeable, PausableUpgradeable {
    bytes32 public constant BOT_ROLE = keccak256("BOT_ROLE");
    mapping(address => Prize[]) public playersWinnings;
    mapping(uint256 => spinRequestData) private spinData;
    mapping(uint256 => Prize[]) private prizesListing;

    VRFCoordinatorV2Interface internal vrfCoordinator;
    LinkTokenInterface linkToken;
    bytes32 internal gasLane;
    uint64 internal subscriptionId;
    uint32 internal callbackGasLimit;
    uint32 internal constant NO_OF_WORDS = 1;
    uint16 internal constant REQUEST_CONFIRMATIONS = 3;

    struct Prize {
        string id;
        uint256 amount;
        uint256 increment;
        uint256 repeat;
        uint256 timestamp;
    }

    struct spinRequestData {
        address player;
        string gameToken;
        uint256 totalPrizes;
    }

    event prizeSelected(
        string id,
        uint amount,
        address playerAddress,
        string gameToken
    );
    event RequestedRandomness(uint256 reqId, address invokerW);
    event RandomnessSelected(uint256 reqId, uint256 randomWord, string gameToken);

    //Initialize function for contract to be upgradable
    function initializes(
        address vrfCoordinatorV2,
        bytes32 _gasLane,
        uint32 _callbackGasLimit,
        uint64 _subscriptionId
    ) public {
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(BOT_ROLE, msg.sender);
        VRFConsumerBaseV2.initialize(vrfCoordinatorV2);
        vrfCoordinator = VRFCoordinatorV2Interface(vrfCoordinatorV2);
        gasLane = _gasLane;
        subscriptionId = _subscriptionId;
        callbackGasLimit = _callbackGasLimit;
        linkToken = LinkTokenInterface(msg.sender);
    }

    //Play
    function play(
        address player,
        Prize[] memory _prizes,
        uint256 totalPrizes,
        string memory gameToken
    )
    public
    payable
    virtual
    onlyRole(BOT_ROLE)
    whenNotPaused
    returns (bool result)
    {
        //Get and filter prizes here
        uint256 _requestID = vrfCoordinator.requestRandomWords(
            gasLane,
            subscriptionId,
            REQUEST_CONFIRMATIONS,
            callbackGasLimit,
            NO_OF_WORDS
        );
        spinData[_requestID] = spinRequestData({
        gameToken : gameToken,
        player : player,
        totalPrizes : totalPrizes
        });
        concatenatePrizes(_prizes, _requestID);
        emit RequestedRandomness(_requestID, msg.sender);
        return true;
    }

    //Fulfill Random Words
    function fulfillRandomWords(uint256 requestID, uint256[] memory randomWords)
    internal
    override
    {
        require(spinData[requestID].totalPrizes > 0, "No prizes fetched");
        uint256 randomNumber = (randomWords[0] % spinData[requestID].totalPrizes) + 1;
        emit RandomnessSelected(requestID, randomNumber, spinData[requestID].gameToken);
        Prize memory _selectedPrize = selectPrize(randomNumber, requestID);
        addPrizeToPlayer(requestID, _selectedPrize);
    }

    //Concatenate Prizes
    function selectPrize(uint256 randomNumber, uint256 requestID)
    internal
    view
    returns (Prize memory)
    {
        for (uint i = 0; i < prizesListing[requestID].length; i++) {
            uint256 total = prizesListing[requestID][i].increment == 0 ?
            (prizesListing[requestID][i].repeat) :
            (prizesListing[requestID][i].amount / prizesListing[requestID][i].increment);
            if (randomNumber <= total) {
                Prize memory prize = prizesListing[requestID][i];
                prize.amount = prizesListing[requestID][i].increment == 0 ?
                prizesListing[requestID][i].amount :
                (randomNumber * prize.increment);
                return prize;
            }
            randomNumber = randomNumber - total;
        }
        return prizesListing[requestID][0];
    }

    //Concatenate Prizes
    function concatenatePrizes(Prize[] memory _prizes, uint256 requestID)
    internal
    {
        for (uint i = 0; i < _prizes.length; i++) {
            prizesListing[requestID].push(_prizes[i]);
        }
    }

    // Add Prize to Player
    function addPrizeToPlayer(uint256 requestID, Prize memory prize) internal {
        prize.timestamp = block.timestamp;
        playersWinnings[spinData[requestID].player].push(prize);
        emit prizeSelected(
            prize.id,
            prize.amount,
            spinData[requestID].player,
            spinData[requestID].gameToken
        );
        delete spinData[requestID];
    }

    //Get Player Winnings
    function getPlayerWinnings(address player)
    public
    whenNotPaused
    view
    returns (Prize[] memory prizes)
    {
        return playersWinnings[player];
    }

    //Pause Contract
    function pause()
    public
    onlyRole(BOT_ROLE) {
        _pause();
    }

    //Unpause Contract
    function unpause()
    public
    onlyRole(BOT_ROLE) {
        _unpause();
    }

}

