import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "hardhat/console.sol";

/*  This contract is for paired token staking. 
    There is a primary and secondary token.
    The primary token can be an ERC20 or Native.
    The seconday token can only be ERC20.
    A ratio for seconday tokens to primary tokens must be set.
    Example: 
        secondaryTokenPerPrimary = 10
        means 10 seconday tokens will be staked for every 1 primary token
    The ratio and tokens cannot be changed after the first deposit.
*/

contract IceBox is Ownable {
    struct UserInfo {
        uint256 primaryAmount; // How many Staking Tokens the user has provided.
        uint256 rewardDebt; // Reward debt.
        uint256 lastRewardBlock;
    }

    bool public immutable isPrimaryStakeNative;
    bool public stakingEnabled;

    address public immutable primaryToken; // main paired token, if not using native
    address public immutable secondaryToken; // staking ERC20 token
    address public treasuryAddress;

    uint256 public blocksPerDay = 42000;
    uint256 public endBlock;
    uint256 public immutable secondaryTokenPerPrimary; // number of staking tokens required per primary token
    uint256 public maxPrimaryStake; // max total amount that can be staked
    uint256 public maxPrimaryStakePerUser; // max staked per user
    uint256 public minPrimaryStakePerUser; // min stake per user
    uint256 public rewardPerBlockPerPrimary; // tokens distributed per block * 1e12
    uint256 public totalPrimaryStaked;

    mapping(address => UserInfo) public userInfos; // user stake

    event Claim(address indexed user, uint256 indexed pending);
    event Deposit(address indexed user, uint256 indexed primaryAmount);
    event EmergencyWithdraw(
        address indexed user,
        uint256 indexed primaryAmount
    );
    event Withdraw(address indexed user, uint256 indexed primaryAmount);
    event WithdrawNative(address indexed sender, uint256 indexed amount);
    event WithdrawToken(
        address indexed user,
        address indexed token,
        uint256 indexed amount
    );

    error AddressIsZero();
    error AfterEndblock();
    error BelowMinimumDeposit();
    error ExceedsMaxStake();
    error ExceedsMaxUserStake();
    error InsufficientRewards();
    error InvalidAmount();
    error NoStake();
    error OutOfRange(string range);
    error PrimaryStakeIsNative();
    error PrimaryStakeIsNotNative();
    error PrimaryTokenIsZero();
    error StakingDisabled();
    error StakingStarted();

    modifier notZeroAddress(address _address) {
        if (_address == address(0)) revert AddressIsZero();
        _;
    }

    modifier stakingNotStarted() {
        if (totalPrimaryStaked > 0) revert StakingStarted();
        _;
    }

    constructor(
        bool _isPrimaryStakeNative,
        address _primaryToken,
        address _secondaryToken,
        address _treasuryAddress,
        uint256 _rewardPerDayPerPrimary,
        uint256 _maxPrimaryStake,
        uint256 _maxPrimaryStakePerUser,
        uint256 _secondaryTokenPerPrimary,
        uint256 _poolLengthInDays
    ) notZeroAddress(_secondaryToken) notZeroAddress(_treasuryAddress) {
        if (!_isPrimaryStakeNative && _primaryToken == address(0))
            revert PrimaryTokenIsZero();
        isPrimaryStakeNative = _isPrimaryStakeNative;
        primaryToken = _primaryToken;
        secondaryToken = _secondaryToken;
        treasuryAddress = _treasuryAddress;
        maxPrimaryStake = _maxPrimaryStake;
        maxPrimaryStakePerUser = _maxPrimaryStakePerUser;
        rewardPerBlockPerPrimary =
            (_rewardPerDayPerPrimary * 1e12) /
            blocksPerDay;
        secondaryTokenPerPrimary = _secondaryTokenPerPrimary;
        endBlock = block.number + blocksPerDay * (_poolLengthInDays * 1 days);
    }

    receive() external payable {
        if (!isPrimaryStakeNative) {
            payable(treasuryAddress).transfer(msg.value);
        }
    }

    function deposit(uint256 amount) external {
        if (isPrimaryStakeNative) revert PrimaryStakeIsNative();
        _deposit(amount);
    }

    function depositNative() external payable {
        if (!isPrimaryStakeNative) revert PrimaryStakeIsNotNative();
        _deposit(msg.value);
    }

    // Withdraw without caring about rewards. EMERGENCY ONLY.
    function emergencyWithdraw() external {
        UserInfo storage user = userInfos[_msgSender()];
        uint256 userPrimaryAmount = user.primaryAmount;
        if (userPrimaryAmount == 0) revert NoStake();

        user.primaryAmount = 0;
        totalPrimaryStaked -= userPrimaryAmount;
        uint256 tokenAmount = userPrimaryAmount * secondaryTokenPerPrimary;

        if (isPrimaryStakeNative) {
            payable(_msgSender()).transfer(userPrimaryAmount);
        } else {
            IERC20(primaryToken).transfer(
                address(_msgSender()),
                userPrimaryAmount
            );
        }
        IERC20(secondaryToken).transfer(address(_msgSender()), tokenAmount);
        emit EmergencyWithdraw(_msgSender(), userPrimaryAmount);
    }

    function emergencyWithdrawByAdmin(address _user) external onlyOwner {
        UserInfo storage user = userInfos[_user];
        uint256 userPrimaryAmount = user.primaryAmount;
        if (userPrimaryAmount == 0) revert NoStake();
        user.primaryAmount = 0;
        totalPrimaryStaked -= userPrimaryAmount;
        if (isPrimaryStakeNative) {
            payable(_user).transfer(userPrimaryAmount);
        } else {
            IERC20(primaryToken).transfer(address(_user), userPrimaryAmount);
        }
        uint256 tokenAmount = userPrimaryAmount * secondaryTokenPerPrimary;
        IERC20(secondaryToken).transfer(address(_user), tokenAmount);
        emit EmergencyWithdraw(_user, userPrimaryAmount);
    }

    function setBlocksPerDay(uint256 _blocksPerDay)
        external
        onlyOwner
        stakingNotStarted
    {
        if (_blocksPerDay < 1000 || _blocksPerDay > 50000)
            revert OutOfRange("1000-50000");
        blocksPerDay = _blocksPerDay;
    }

    function setEndBlock(uint value) external onlyOwner {
        endBlock = value;
    }

    function setMaxPrimaryStake(uint256 amount) external onlyOwner {
        maxPrimaryStake = amount;
    }

    function setMaxPrimaryStakePerUser(uint256 amount) external onlyOwner {
        maxPrimaryStakePerUser = amount;
    }

    function setMinPrimaryStakePerUser(uint256 amount) external onlyOwner {
        minPrimaryStakePerUser = amount;
    }

    function setRewardPerDayPerPrimary(uint256 value)
        external
        onlyOwner
        stakingNotStarted
    {
        rewardPerBlockPerPrimary = (value * 1e12) / blocksPerDay;
    }

    function setStakingEnabled(bool enabled) external onlyOwner {
        stakingEnabled = enabled;
    }

    function setTreasuryAddress(address addr) external onlyOwner {
        treasuryAddress = addr;
    }

    function withdraw(uint256 primaryAmount) external {
        UserInfo storage user = userInfos[_msgSender()];
        uint256 userPrimaryAmount = user.primaryAmount;
        if (userPrimaryAmount == 0) revert NoStake();
        if (primaryAmount > userPrimaryAmount) revert InvalidAmount();

        claim();
        user.primaryAmount -= primaryAmount;
        totalPrimaryStaked -= primaryAmount;

        if (isPrimaryStakeNative) {
            payable(_msgSender()).transfer(primaryAmount);
        } else {
            IERC20(primaryToken).transfer(_msgSender(), primaryAmount);
        }
        uint256 tokenAmount = primaryAmount * secondaryTokenPerPrimary;
        IERC20(secondaryToken).transfer(address(_msgSender()), tokenAmount);

        emit Withdraw(_msgSender(), primaryAmount);
    }

    function getRewardPerDayPerPrimary() external view returns (uint) {
        return (rewardPerBlockPerPrimary * blocksPerDay) / 1e12 / 1e18;
    }

    function withdrawNativeToTreasury() external onlyOwner {
        uint256 amount = address(this).balance;
        if (amount == 0) return;
        amount -= isPrimaryStakeNative ? totalPrimaryStaked : 0;
        payable(treasuryAddress).transfer(amount);
        emit WithdrawNative(_msgSender(), amount);
    }

    function withdrawTokensToTreasury(address tokenAddress)
        external
        onlyOwner
        notZeroAddress(tokenAddress)
    {
        uint256 amount = IERC20(tokenAddress).balanceOf(address(this));
        if (amount == 0) return;
        amount -= !isPrimaryStakeNative && tokenAddress == primaryToken
            ? totalPrimaryStaked
            : 0;
        amount -= tokenAddress == secondaryToken
            ? totalPrimaryStaked * secondaryTokenPerPrimary
            : 0;
        IERC20(tokenAddress).transfer(treasuryAddress, amount);
        emit WithdrawToken(_msgSender(), tokenAddress, amount);
    }

    function claim() public {
        UserInfo storage user = userInfos[_msgSender()];
        uint256 userPrimaryAmount = user.primaryAmount;
        if (userPrimaryAmount == 0) revert NoStake();

        uint256 pending = getPending(_msgSender());
        if (pending > 0) {
            uint256 bal = isPrimaryStakeNative
                ? address(this).balance
                : IERC20(primaryToken).balanceOf(address(this));
            if (bal - pending < totalPrimaryStaked)
                revert InsufficientRewards();
            user.rewardDebt += pending;
            user.lastRewardBlock = block.number;
            if (isPrimaryStakeNative) {
                payable(_msgSender()).transfer(pending);
            } else {
                IERC20(primaryToken).transfer(_msgSender(), pending);
            }
        }
        emit Claim(_msgSender(), pending);
    }

    function getPending(address _user) public view returns (uint256) {
        UserInfo memory user = userInfos[_user];
        uint256 userPrimaryAmount = user.primaryAmount;
        if (userPrimaryAmount > 0 && block.number > user.lastRewardBlock) {
            uint multiplier = _getMultiplier(
                user.lastRewardBlock,
                block.number
            );
            if (multiplier == 0) return 0;
            return
                (userPrimaryAmount * rewardPerBlockPerPrimary * multiplier) /
                1e12 /
                1e18;
        }
        return 0;
    }

    function _deposit(uint256 amount) private {
        if (!stakingEnabled) revert StakingDisabled();
        if (block.number > endBlock) revert AfterEndblock();
        if (totalPrimaryStaked + amount > maxPrimaryStake)
            revert ExceedsMaxStake();
        UserInfo storage user = userInfos[_msgSender()];
        uint256 userPrimaryAmount = user.primaryAmount;
        if (userPrimaryAmount > 0) claim();
        if (amount == 0) return;
        if (userPrimaryAmount + amount > maxPrimaryStakePerUser)
            revert ExceedsMaxUserStake();
        if (userPrimaryAmount + amount < minPrimaryStakePerUser)
            revert BelowMinimumDeposit();

        if (!isPrimaryStakeNative) {
            IERC20(primaryToken).transferFrom(
                address(_msgSender()),
                address(this),
                amount
            );
        }

        uint256 tokenAmount = amount * secondaryTokenPerPrimary;
        IERC20(secondaryToken).transferFrom(
            address(_msgSender()),
            address(this),
            tokenAmount
        );

        user.primaryAmount += amount;
        totalPrimaryStaked += amount;
        user.lastRewardBlock = block.number;
        emit Deposit(_msgSender(), amount);
    }

    function _getMultiplier(uint from, uint to) private view returns (uint) {
        if (from < endBlock && to < endBlock) {
            return to - from;
        } else if (from < endBlock && to > endBlock) {
            return endBlock - from;
        }
        return 0;
    }
}

