contract EdgyEggs is ERC721Enumerable, Ownable, ERC2981 {
    using Strings for uint256;

    string baseURI;
    string public baseExtension = "";
    uint256 public maxSupply = 10000;
    uint256 public maxMintAmount = 12;
    uint256 public whitelistMintAmount = 3;
    uint256 public heldFarmId;
    bool public paused = false;
    bool public revealed = false;
    string public notRevealedUri;
    bool public whitelistEnforced = true;
    bool public whitelistOver = false;
    mapping (address => bool) public isWhitelisted;
    mapping (address => uint256) public whitelistedMintAmount;

    DividendTracker public dividendTracker;

    IERC20 public constant usdcToken = IERC20(0xB97EF9Ef8734C71904D8002F8b6Bc66Dd9c48a6E);
    
    uint256 public cost = 88 * 1e6; // 88 USDC
    IERC20 public constant eggToken = IERC20(0x7761E2338B35bCEB6BdA6ce477EF012bde7aE611);
    IERC20 public constant wormToken= IERC20(0x79BA10485ae46a9436D560D9664369176EC2eB2b);
    IERC20 public constant feedToken= IERC20(0xab592d197ACc575D16C3346f4EB70C703F308D1E);
    IERC20 public constant fertToken = IERC20(0x9C846D808A41328A209e235B5e3c4E626DAb169E);
    IERC20 public constant lpToken = IERC20(0x3052a75dfD7A9D9B0F81E510E01d3Fe80A9e7ec7);
    IERC721 public constant farmlandToken = IERC721(0x00f5D01D86008D14d04E29EFe88DffC75a9cAc47);
    IEggBurnPartner public constant burnContract = IEggBurnPartner(0x81ADaa2b115c4921f3Cc412A05F9Caa521b4e1AC);
    
    IChiknLpStaking public constant stakingContract = IChiknLpStaking(0x85fdcA3741395B7F9faa70Bf7cc40f4AC89127FF);
    IChiknLpStaking public constant fertBurnContract = IChiknLpStaking(0x160F701AB5Bc84Dd65CE1174D2022EdC1b128C5F);

    IJoeRouter public constant joeRouter = IJoeRouter(0x60aE616a2155Ee3d9A68541Ba4544862310933d4);

    address public operationsAddress;
    address public immutable liquidityAddress;
    address public eggVaultAddress;
    uint256 public operationsPercent = 60; // 6%
    uint256 public constant liquidityPercent = 450; // 45% from mint
    uint256 public wormEggVaultPercent = 0; 
    uint256 public eggBurnPercent = 500; // 50% of level-up eggs burned
    uint256 public constant divisor = 1000; // 10 = 1%
    uint256 public amountStaked;

    mapping (uint256 => uint256) public tokenLevel;
    mapping (uint256 => uint256) public tokenLastLevelUpTimestamp;

    bool public compoundEnabled;
    bool public levelingEnabled;

    constructor() ERC721("EdgyEggs", "EDGYEGG"){
        operationsAddress = 0x23973F233c1863aC56076872F382C4EdD5973ea8;
        liquidityAddress = 0x15e51B3BeD300C92BC92b2A990Dae790f702Dc98;
        eggVaultAddress = 0xed133a27a3FdDD1Ff8835eF1193eEAfD49f57DAA;
        setBaseURI("");
        setNotRevealedURI("");
        try burnContract.subscribe(name()){} catch {}
        dividendTracker = new DividendTracker(address(wormToken));
        _setDefaultRoyalty(operationsAddress, 600);
    }

    receive() external payable {}

    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(ERC721Enumerable, ERC2981)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    // internal
    function _baseURI() internal view virtual override returns (string memory) {
        return baseURI;
    }

    // anyone/thing can call harvest to automate it.

    function harvest() public {
        // first harvest any pending Worm from staking
        stakingContract.claimReward();

        if(feedToken.balanceOf(address(this)) > 0){
            feedToken.transfer(address(eggVaultAddress), feedToken.balanceOf(address(this)));
        }

        // if any Worm is claimed then determine if compound is enabled     
        if(wormToken.balanceOf(address(this)) > 0){
            if(operationsPercent > 0){
                wormToken.transfer(operationsAddress, wormToken.balanceOf(address(this)) * operationsPercent / divisor);
            }
            
            if(!compoundEnabled){

                // in normal circumstances Compound will not be enabled, 
                // and Worm gets distributed to holders via a standard dividend tracking system.
                if(wormEggVaultPercent > 0){
                    wormToken.transfer(eggVaultAddress, wormToken.balanceOf(address(this)) * wormEggVaultPercent / divisor);
                }
                uint256 balanceForDividends = wormToken.balanceOf(address(this));
                wormToken.transfer(address(dividendTracker), balanceForDividends);
                dividendTracker.distributeTokenDividends();

            } else {

                // in the event the community has voted to compound, it will sell half the worm for Egg, half the Worm for Avax
                // and pair it together and stake again to compound the results.

                uint256 amountForBuy = wormToken.balanceOf(address(this)) / 2;
                uint256 amountForLp = wormToken.balanceOf(address(this)) - amountForBuy;
                uint256 initialBalance = eggToken.balanceOf(address(this));

                swapWormForEgg(amountForBuy);

                uint256 deltaBalance = eggToken.balanceOf(address(this)) - initialBalance;
                uint256 initialAvaxBalance = address(this).balance;

                swapWormForAvax(amountForLp);

                uint256 deltaAvaxBalance = address(this).balance - initialAvaxBalance;
                
                if(deltaBalance > 0){
                    addEggAvaxLiquidity(deltaBalance, deltaAvaxBalance);
                    uint256 lpAmount = lpToken.balanceOf(address(this));
                    lpToken.approve(address(stakingContract), lpAmount);
                    stakingContract.staking(lpAmount);
                    amountStaked += lpAmount;
                }
            }
        }
        dividendTracker.processAccount(payable(msg.sender), false);
    }

    function levelUpEdgyEggs(uint256[] memory tokenIds) external {
        require(levelingEnabled, "Disabled");
        uint256 currentLevel;
        uint256 lastLevelUp;
        uint256 amountEggForLevel;
        for(uint256 i = 0; i < tokenIds.length; i++){
            currentLevel = tokenLevel[tokenIds[i]];
            amountEggForLevel += currentLevel >= 36 ? 36 * (10**eggToken.decimals()): currentLevel * (10**eggToken.decimals()); 
            lastLevelUp = tokenLastLevelUpTimestamp[tokenIds[i]];
            require(lastLevelUp + 1 days <= block.timestamp, "Too Soon");
            tokenLastLevelUpTimestamp[tokenIds[i]] = block.timestamp;
            tokenLevel[tokenIds[i]] += 1;
        }
        require(eggToken.balanceOf(msg.sender) >= amountEggForLevel, "No Eggs");
        eggToken.transferFrom(msg.sender, address(this), amountEggForLevel);

        if(operationsPercent > 0){
            eggToken.transfer(operationsAddress, amountEggForLevel * operationsPercent / divisor);
        }
        if(eggBurnPercent > 0){
            string memory message = "burn";
            try burnContract.burnEgg(name(), amountEggForLevel * eggBurnPercent / divisor, message) {} catch {revert("burn fail");}
        }
        if(operationsPercent + eggBurnPercent < 1000){
            eggToken.transfer(eggVaultAddress, amountEggForLevel * (1000-operationsPercent-eggBurnPercent) / divisor);
        }
    }

    // public
    function mint(uint256 _mintAmount) public {
        uint256 supply = totalSupply();
        require(!paused);
        uint256 totalCost = cost * _mintAmount;

        if(whitelistEnforced){
            require(isWhitelisted[msg.sender]);
            require(whitelistedMintAmount[msg.sender] + _mintAmount <= whitelistMintAmount);
            whitelistedMintAmount[msg.sender] += _mintAmount;
        }

        require(_mintAmount > 0);
        require(_mintAmount <= maxMintAmount);
        require(supply + _mintAmount <= maxSupply);

        require(usdcToken.balanceOf(msg.sender) >= totalCost);
        
        for (uint256 i = 1; i <= _mintAmount; i++) {
            _safeMint(msg.sender, supply + i);
            tokenLevel[supply + i] = 1;
        }

        dividendTracker.setBalance(payable(msg.sender),  balanceOf(msg.sender));

        if(totalCost > 0){
            usdcToken.transferFrom(msg.sender, liquidityAddress, totalCost * liquidityPercent / divisor);
            usdcToken.transferFrom(msg.sender, operationsAddress, totalCost - (totalCost * liquidityPercent / divisor));
        }
    }
    
