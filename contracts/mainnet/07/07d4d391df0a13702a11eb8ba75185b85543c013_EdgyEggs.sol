            address(this),
            block.timestamp
        );
    }

    function swapWormForEgg(uint256 wormAmount) private {
        
        address[] memory path = new address[](3);
        path[0] = address(wormToken);
        path[1] = joeRouter.WAVAX();
        path[2] = address(eggToken);

        wormToken.approve(address(joeRouter), wormAmount);

        // make the swap
        joeRouter.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            wormAmount,
            0,
            path,
            address(this),
            block.timestamp
        );
    }

