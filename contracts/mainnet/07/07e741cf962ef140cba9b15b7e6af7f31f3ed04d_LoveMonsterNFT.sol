        // AiWhiskyBartendersCollection = IERC721(partnershipAddress[5]);
        // AiWhiskyCollection = IERC721(partnershipAddress[6]);
        // PumpskinCollection = IERC721(partnershipAddress[7]);
        // AIMutantsCollection = IERC721(partnershipAddress[8]);
    }

