    // function mintNextAvailable() external onlyRole(MINTER_ROLE) nonReentrant {
    //     require(totalSupply() < MAX_TOTAL_SUPPLY, "ERR-29");
    //     uint256 tokenId = getTokenAvailable();
    //     require(tokenId > 0, "ERR-28");
    //     _mint(msg.sender, tokenId);
    //     RegisterMonster(tokenId);
