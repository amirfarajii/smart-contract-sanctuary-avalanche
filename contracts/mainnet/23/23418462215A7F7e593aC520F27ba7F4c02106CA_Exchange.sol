pragma solidity 0.8.10;
pragma experimental ABIEncoderV2;

import {AggregatorV2V3Interface} from "@chainlink/contracts/src/v0.8/interfaces/AggregatorV2V3Interface.sol";

interface IExchange {

    event SupportedTokenAdded(address indexed token, address indexed priceFeed);

    event Deposit(address indexed account, address indexed receiver, uint256 cTokenAmount, address depositedTokenAddress, uint256 depositedTokenAmount);

    event Withdraw(address indexed account, address indexed receiver, uint256 cTokenAmount, address indexed tokenAddress, uint256 tokenAmount);

    error NotEnoughERC20Balance(address erc20Address);

    error ApprovedFailed(address erc20Address);

    error TransferFromFailed(address erc20Address);

    error DepositDisabled();

    error InvalidSignature();

    error TokenIsNotSupported(address erc20Address);

    error ZeroCtokenCalculated();

    error InsufficientFunds();


    struct DepositRequest {
        address from;
        address token;
        uint256 amount;
        address receiver;
        uint256 validAfter; // The time after which this is valid (unix time)
        uint256 validBefore;// The time before which this is valid (unix time)
        bytes32 nonce;      // Unique nonce
    }

    struct WithdrawRequest {
        address from;
        uint256 amount;
        address tokenAddress;
        address receiver;
        uint256 validAfter; // The time after which this is valid (unix time)
        uint256 validBefore;// The time before which this is valid (unix time)
        bytes32 nonce;      // Unique nonce
    }

    struct WithdrawExactTokenRequest {
        address from;
        uint256 tokenAmount;
        address tokenAddress;
        address receiver;
        uint256 validAfter; // The time after which this is valid (unix time)
        uint256 validBefore;// The time before which this is valid (unix time)
        bytes32 nonce;      // Unique nonce
    }

    struct SupportedToken {
        AggregatorV2V3Interface priceFeed;
        uint256 minimumReserve;
        uint256 reservePercentage;
    }


}

