pragma solidity 0.8.10;
pragma experimental ABIEncoderV2;

import {IExchange} from "./interfaces/IExchange.sol";
import {AuthorizedActionsUpgradeable} from "./abstracts/AuthorizedActionsUpgradeable.sol";
import {InterestCalculator} from "./lib/InterestCalculator.sol";

import {AccessControlUpgradeable} from "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import {AggregatorV2V3Interface} from "@chainlink/contracts/src/v0.8/interfaces/AggregatorV2V3Interface.sol";
import {IERC20Metadata} from "@openzeppelin/contracts/interfaces/IERC20Metadata.sol";
import {ERC20Upgradeable} from "@openzeppelin/contracts-upgradeable/token/ERC20/ERC20Upgradeable.sol";
import {ECDSAUpgradeable} from "@openzeppelin/contracts-upgradeable/utils/cryptography/ECDSAUpgradeable.sol";
import {PausableUpgradeable} from "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import {IPangolinRouter} from "@pangolindex/exchange-contracts/contracts/pangolin-periphery/interfaces/IPangolinRouter.sol";
import {IPool} from "@aave/core-v3/contracts/interfaces/IPool.sol";
import {DataTypes} from '@aave/core-v3/contracts/protocol/libraries/types/DataTypes.sol';


contract Exchange is IExchange, ERC20Upgradeable, AccessControlUpgradeable, AuthorizedActionsUpgradeable, PausableUpgradeable {
    using ECDSAUpgradeable for bytes32;
    using InterestCalculator for uint256;

    bytes32 public constant ADMIN_ROLE = keccak256("ADMIN_ROLE");
    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");
    bytes32 public constant WITHDRAW_ROLE = keccak256("WITHDRAW_ROLE");
    bytes32 public constant DEPOSIT_ROLE = keccak256("DEPOSIT_ROLE");


    IERC20Metadata public usdc;
    AggregatorV2V3Interface public usdtoCTokenPriceFeed;
    mapping(address => SupportedToken) supportedTokens;

    bytes32 private constant DEPOSIT_REQUEST_TYPE_HASH = keccak256("DepositRequest(address from,address token,uint256 amount,address receiver,uint256 validAfter,uint256 validBefore,bytes32 nonce)");
    bytes32 private constant WITHDRAW_REQUEST_TYPE_HASH = keccak256("WithdrawRequest(address from,uint256 amount,address tokenAddress,address receiver,uint256 validAfter,uint256 validBefore,bytes32 nonce)");
    bytes32 private constant WITHDRAW_EXACT_TOKEN_REQUEST_TYPE_HASH = keccak256("WithdrawExactTokenRequest(address from,uint256 tokenAmount,address tokenAddress,address receiver,uint256 validAfter,uint256 validBefore,bytes32 nonce)");


    IPangolinRouter public pangolinRouter;
    IPool public aavePool;
    uint256 public cTokenUsdcBasePrice;
    uint256 public cTokenUsdcPriceDecimals;
    uint256 public interestPeriodStart;
    uint256 public interestPeriodEnd;
    uint256 public interestPerSecondWay;
    uint256 public minimumUsdcReserve;
    uint16 public usdcReservePercentage;
    uint16 slippagePercentage;
    bool public depositEnabled;

    uint16 constant public PERCENTAGE = 10000;

    function initialize(
        address usdcAddress,
        address pangolinRouterAddress,
        address aavePoolAddress,
        uint256 interestPerYearWad,
        uint256 minimumUsdcReserve_,
        uint256 cTokenUsdcBasePrice_,
        uint256 cTokenUsdcPriceDecimals_,
        uint256 interestPeriodStart_,
        uint256 interestPeriodEnd_,
        uint16 usdcReservePercentage_,
        uint16 slippagePercentage_
    ) external initializer {
        __ERC20_init_unchained("CToken", "CT1");
        __Pausable_init();
        __AccessControl_init_unchained();
        __AuthorizedActions_init("CToken", "1.0");

        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _setupRole(ADMIN_ROLE, _msgSender());
        _setupRole(PAUSER_ROLE, _msgSender());
        _setupRole(WITHDRAW_ROLE, _msgSender());
        _setupRole(DEPOSIT_ROLE, _msgSender());

        pangolinRouter = IPangolinRouter(pangolinRouterAddress);
        minimumUsdcReserve = minimumUsdcReserve_;
        aavePool = IPool(aavePoolAddress);
        cTokenUsdcBasePrice = cTokenUsdcBasePrice_;
        interestPerSecondWay = interestPerYearWad.yearlyRateToRay();
        usdc = IERC20Metadata(usdcAddress);
        cTokenUsdcPriceDecimals = cTokenUsdcPriceDecimals_;

        usdcReservePercentage = usdcReservePercentage_;
        slippagePercentage = slippagePercentage_;
        interestPeriodStart = interestPeriodStart_;
        interestPeriodEnd = interestPeriodEnd_;
    }

    /**
    * @notice Maintained USDC balance in the contract around the desired amount, Invest test to AAVE
    */
    function maintainUsdcReserve() external whenNotPaused() {

        uint256 contractBalance = usdc.balanceOf(address(this));
        DataTypes.ReserveData memory reserveData = aavePool.getReserveData(address(usdc));
        uint256 investedReserve = IERC20Metadata(reserveData.aTokenAddress).balanceOf(address(this));
        uint256 desiredSupply;
        if (minimumUsdcReserve > (contractBalance + investedReserve) * usdcReservePercentage / PERCENTAGE) {
            desiredSupply = minimumUsdcReserve;
        } else {
            desiredSupply = (contractBalance + investedReserve) * usdcReservePercentage / PERCENTAGE;
        }

        if (desiredSupply > contractBalance) {
            //withdraw
            _withdrawFromAave(address(usdc), desiredSupply - contractBalance);
        } else if (desiredSupply < contractBalance) {
            //supply
            _depositToAave(address(usdc), contractBalance - desiredSupply);
        }
    }

    /**
    * @notice Maintained token balance in the contract around the desired amount, convert rest to USDC and invest
    */
    function maintainErc20Reserve(address erc20Address) external whenNotPaused() {

        if (!isTokenSupported(erc20Address)) {
            revert TokenIsNotSupported(erc20Address);
        }

        uint256 contractTokenBalance = IERC20Metadata(erc20Address).balanceOf(erc20Address);
        uint256 contractTokenBalanceInUsdc = _convertToken(supportedTokens[erc20Address].priceFeed, erc20Address, address(usdc), contractTokenBalance);
        uint256 contractUsdcBalance = usdc.balanceOf(address(this));
        DataTypes.ReserveData memory reserveData = aavePool.getReserveData(address(usdc));
        uint256 investedUsdcReserve = IERC20Metadata(reserveData.aTokenAddress).balanceOf(address(this));
        uint256 desiredSupply;
        if (supportedTokens[erc20Address].minimumReserve > (contractUsdcBalance + investedUsdcReserve + contractTokenBalanceInUsdc) * supportedTokens[erc20Address].reservePercentage / PERCENTAGE) {
            desiredSupply = supportedTokens[erc20Address].minimumReserve;
        } else {
            desiredSupply = (contractUsdcBalance + investedUsdcReserve + contractTokenBalanceInUsdc) * supportedTokens[erc20Address].reservePercentage / PERCENTAGE;
        }

        if (desiredSupply > contractTokenBalanceInUsdc) {
            //withdraw and sell usdc
            _withdrawFromAave(address(usdc), desiredSupply - contractTokenBalanceInUsdc);
            _swapUsdcToERC20(erc20Address, desiredSupply - contractTokenBalanceInUsdc);

        } else if (desiredSupply < contractTokenBalanceInUsdc) {
            //buy usdc and supply
            _swapERC20ToUSDC(erc20Address, contractTokenBalanceInUsdc - desiredSupply);
            _depositToAave(address(usdc), usdc.balanceOf(address(this)) - contractUsdcBalance);
        }
    }

    /**
     * @notice Deposits ERC20 in exchange of CToken
     * @param tokenAddress    Address of ERC20 Contract
     * @param amount   amount of ERC20
     * @param receiver   receiver address for CToken
     */
    function deposit(address tokenAddress, uint256 amount, address receiver) external whenNotPaused() {
        if (!depositEnabled) {
            revert DepositDisabled();
        }
        _deposit(_msgSender(), tokenAddress, amount, receiver);
    }

    /**
     * @notice Deposits ERC20 in exchange of CToken as an Authorization function
     * @param req    DepositRequest
     * @param signature   typed signature of req by the req.from
     */
    function depositWithAuthorization(DepositRequest calldata req, bytes calldata signature) external whenNotPaused() onlyRole(DEPOSIT_ROLE) {
        _requireValidAuthorization(req.from, req.nonce, req.validAfter, req.validBefore);

        address signer = _hashTypedDataV4(keccak256(abi.encode(
                DEPOSIT_REQUEST_TYPE_HASH,
                req.from,
                req.token,
                req.amount,
                req.receiver,
                req.validAfter,
                req.validBefore,
                req.nonce
            ))).recover(signature);

        if (signer != req.from) {
            revert InvalidSignature();
        }

        _markAuthorizationAsUsed(req.from, req.nonce);
        _deposit(req.from, req.token, req.amount, req.receiver);
    }

    /**
     * @notice withdraw ERC20 in exchange of CToken
     * @param amount   amount of Ctoken
     * @param tokenAddress    Address of ERC20 Contract
     * @param receiver   receiver address for ERC20
     */
    function withdraw(uint256 amount, address tokenAddress, address receiver) external whenNotPaused() {
        _withdraw(_msgSender(), amount, tokenAddress, receiver);
    }


    /**
     * @notice withdraw ERC20 in exchange of CToken as an Authorization function
     * @param req    WithdrawRequest
     * @param signature   typed signature of req by the req.from
     */
    function withdrawWithAuthorization(WithdrawRequest calldata req, bytes calldata signature) external whenNotPaused() {
        _requireValidAuthorization(req.from, req.nonce, req.validAfter, req.validBefore);

        address signer = _hashTypedDataV4(keccak256(abi.encode(
                WITHDRAW_REQUEST_TYPE_HASH,
                req.from,
                req.amount,
                req.tokenAddress,
                req.receiver,
                req.validAfter,
                req.validBefore,
                req.nonce
            ))).recover(signature);

        if (signer != req.from) {
            revert InvalidSignature();
        }
        _markAuthorizationAsUsed(req.from, req.nonce);
        _withdraw(req.from, req.amount, req.tokenAddress, req.receiver);
    }

    /**
     * @notice withdraw exact amount of ERC20 in exchange of CToken
     * @param tokenAmount   amount of ERC20 token requested
     * @param tokenAddress    Address of ERC20 Contract
     * @param receiver   receiver address for ERC20
     */
    function withdrawExactToken(uint256 tokenAmount, address tokenAddress, address receiver) external whenNotPaused() {
        _withdrawExactToken(_msgSender(), tokenAmount, tokenAddress, receiver);
    }


    /**
     * @notice withdraw exact amount of ERC20 in exchange of CToken as an Authorization function
     * @param req    WithdrawExactTokenRequest
     * @param signature   typed signature of req by the req.from
     */
    function withdrawExactTokenWithAuthorization(WithdrawExactTokenRequest calldata req, bytes calldata signature) external whenNotPaused() {
        _requireValidAuthorization(req.from, req.nonce, req.validAfter, req.validBefore);

        address signer = _hashTypedDataV4(keccak256(abi.encode(
                WITHDRAW_EXACT_TOKEN_REQUEST_TYPE_HASH,
                req.from,
                req.tokenAmount,
                req.tokenAddress,
                req.receiver,
                req.validAfter,
                req.validBefore,
                req.nonce
            ))).recover(signature);

        if (signer != req.from) {
            revert InvalidSignature();
        }

        _markAuthorizationAsUsed(req.from, req.nonce);
        _withdrawExactToken(req.from, req.tokenAmount, req.tokenAddress, req.receiver);
    }

    /**
     * @notice admin function to add an ERC2ß token address and PriceFeed
     * @param tokenAddress    Address of ERC20 Contract
     * @param priceFeedAddress   Address of AggregatorV2V3Interface Contract ERC20 / USD
     */
    function addSupportedToken(address tokenAddress, address priceFeedAddress, uint256 minimumReserve, uint256 reservePercentage) external onlyRole(ADMIN_ROLE) {
        supportedTokens[tokenAddress].priceFeed = AggregatorV2V3Interface(priceFeedAddress);
        supportedTokens[tokenAddress].minimumReserve = minimumReserve;
        supportedTokens[tokenAddress].reservePercentage = reservePercentage;

        emit SupportedTokenAdded(tokenAddress, priceFeedAddress);
    }

    /**
     * @notice returns if token is supported
     * @param tokenAddress    Address of ERC20 Contract
     * @return true if token is supported
     */
    function isTokenSupported(address tokenAddress) public view returns (bool) {
        return tokenAddress == address(usdc) || address(supportedTokens[tokenAddress].priceFeed) != address(0);
    }


    /**
    *
    */
    function _convertToken(AggregatorV2V3Interface priceFeed, address token1, address token2, uint256 amount) view internal returns (uint256) {
        (, int256 price,,,) = priceFeed.latestRoundData();
        if (token1 == address(usdc)) {
            return (amount * (10 ** priceFeed.decimals()) / uint256(price) * 10 ** IERC20Metadata(token2).decimals() / 10 ** IERC20Metadata(token1).decimals());
        }
        return (amount * uint256(price) / (10 ** priceFeed.decimals()) * 10 ** IERC20Metadata(token2).decimals() / 10 ** IERC20Metadata(token1).decimals());
    }

    function _depositToAave(address erc20Address, uint256 amount) internal {

        if (!IERC20Metadata(erc20Address).approve(address(aavePool), amount)) {
            revert ApprovedFailed(erc20Address);
        }
        aavePool.supply(erc20Address, amount, address(this), 0);
    }

    function _withdrawFromAave(address erc20Address, uint256 amount) internal {
        aavePool.withdraw(erc20Address, amount, address(this));
    }

    function _swapUsdcToERC20(address erc20Address, uint256 usdcValue) internal {

        uint256 tokenValue = _convertToken(supportedTokens[erc20Address].priceFeed, address(usdc), erc20Address, usdcValue);
        usdc.approve(address(pangolinRouter), usdcValue);

        address[] memory path = new address[](2);
        path[0] = address(usdc);
        path[1] = erc20Address;

        pangolinRouter.swapExactTokensForTokens(usdcValue, (tokenValue * slippagePercentage / PERCENTAGE), path, address(this), block.timestamp);

    }

    function _swapERC20ToUSDC(address erc20Address, uint256 usdcValue) internal {
        uint256 tokenValue = _convertToken(supportedTokens[erc20Address].priceFeed, address(usdc), erc20Address, usdcValue);

        IERC20Metadata(erc20Address).approve(address(pangolinRouter), tokenValue);
        address[] memory path = new address[](2);
        path[0] = erc20Address;
        path[1] = address(usdc);

        pangolinRouter.swapExactTokensForTokens(tokenValue, (usdcValue * slippagePercentage / PERCENTAGE), path, address(this), block.timestamp);

    }

    function _deposit(address from, address tokenAddress, uint256 amount, address receiver) internal {
        if (!isTokenSupported(tokenAddress)) {
            revert TokenIsNotSupported(tokenAddress);
        }

        uint256 amountToDeposit = amount < IERC20Metadata(tokenAddress).balanceOf(from) ? amount : IERC20Metadata(tokenAddress).balanceOf(from);
        uint256 cAmount = calculateCTokenValue(tokenAddress, amountToDeposit);
        if (cAmount == 0) {
            revert ZeroCtokenCalculated();
        }

        if (!IERC20Metadata(tokenAddress).transferFrom(from, address(this), amountToDeposit)) {
            revert  TransferFromFailed(tokenAddress);
        }

        _mint(receiver, cAmount);
        emit Deposit(from, receiver, cAmount, tokenAddress, amountToDeposit);

    }

    function _withdrawExactToken(address from, uint256 tokenAmount, address tokenAddress, address receiver) internal {

        if (!isTokenSupported(tokenAddress)) {
            revert TokenIsNotSupported(tokenAddress);
        }
        uint256 cTokenAmount = calculateCTokenValue(tokenAddress, tokenAmount);
        if (balanceOf(from) < cTokenAmount) {
            revert InsufficientFunds();
        }

        if (!IERC20Metadata(tokenAddress).transfer(receiver, tokenAmount)) {
            revert  TransferFromFailed(tokenAddress);
        }

        _burn(from, cTokenAmount);
        emit Withdraw(from, receiver, cTokenAmount, tokenAddress, tokenAmount);

    }

    function _withdraw(address from, uint256 amount, address tokenAddress, address receiver) internal {
        if (!isTokenSupported(tokenAddress)) {
            revert TokenIsNotSupported(tokenAddress);
        }

        if (balanceOf(from) < amount) {
            revert InsufficientFunds();
        }

        uint256 returnTokenAmount = 0;
        if (tokenAddress == address(usdc)) {
            returnTokenAmount = convertCTokenToUsdc(amount);
        } else {
            returnTokenAmount = _convertToken(supportedTokens[tokenAddress].priceFeed, address(usdc), tokenAddress, convertCTokenToUsdc(amount));
        }

        if (!IERC20Metadata(tokenAddress).transfer(receiver, returnTokenAmount)) {
            revert  TransferFromFailed(tokenAddress);
        }

        _burn(from, amount);
        emit Withdraw(from, receiver, amount, tokenAddress, returnTokenAmount);

    }

    function updateUsdcReservePercentage(uint16 _usdcReservePercentage) public onlyRole(ADMIN_ROLE) {
        usdcReservePercentage = _usdcReservePercentage;
    }

    function getCurrentPrice() public view returns (uint256) {
        if (block.timestamp < interestPeriodStart) {
            return cTokenUsdcBasePrice;
        }
        uint256 currentTimestamp;
        if (block.timestamp > interestPeriodEnd) {
            currentTimestamp = interestPeriodEnd;
        } else {
            currentTimestamp = block.timestamp;
        }

        uint256 currentPrice = cTokenUsdcBasePrice.accrueInterest(interestPerSecondWay, currentTimestamp - interestPeriodStart);
        return currentPrice;
    }

    function convertCTokenToUsdc(uint256 cTokenAmount) public view returns (uint256 usdcAmount) {
        usdcAmount = cTokenAmount * getCurrentPrice() / 10 ** cTokenUsdcPriceDecimals;
    }

    function convertUsdcToCToken(uint256 usdcAmount) public view returns (uint256 cTokenAmount) {
        cTokenAmount = usdcAmount * 10 ** cTokenUsdcPriceDecimals / getCurrentPrice();
    }

    function decimals() public view virtual override returns (uint8) {
        return 6;
    }

    function calculateCTokenValue(address tokenAddress, uint256 amount) public view returns (uint256) {
        if (!isTokenSupported(tokenAddress)) {
            revert TokenIsNotSupported(tokenAddress);
        }
        if (tokenAddress == address(usdc)) {
            return convertUsdcToCToken(amount);
        }
        return convertUsdcToCToken(_convertToken(supportedTokens[tokenAddress].priceFeed, tokenAddress, address(usdc), amount));
    }




    /**
    * @dev Triggers stopped state.
     *
     * Requirements:
     *
     * - The contract must not be paused.
     */
    function pause() external whenNotPaused onlyRole(PAUSER_ROLE) {
        _pause();
    }

    /**
     * @dev Returns to normal state.
     *
     * Requirements:
     *
     * - The contract must be paused.
     */
    function unpause() external whenPaused onlyRole(PAUSER_ROLE) {
        _unpause();
    }


    function version() external view returns (string memory) {
        return _version;
    }


    function updateUsdcAddress(address usdc_) external onlyRole(ADMIN_ROLE) {
        usdc = IERC20Metadata(usdc_);
    }


    function updateDepositEnabled(bool depositEnabled_) external onlyRole(ADMIN_ROLE) {
        depositEnabled = depositEnabled_;
    }
}

