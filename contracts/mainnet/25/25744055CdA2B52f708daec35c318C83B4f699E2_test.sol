pragma solidity =0.6.12;

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "./traderjoe/JoeRouter02.sol";
import "./traderjoe/JoePair.sol";

contract test is
    ERC721,
    Ownable
{
    using Strings for uint256;

    uint256 public price;

    uint256 public wlPrice;
    uint256 public maxSupply;
    uint256 public maxPerWallet;
    uint256 public maxPerTx;
    string public customBaseURI;
    string public uriExtension;
    string public unrevealedURI;
    uint256 public privateSaleTime;
    uint256 public publicSaleTime;
    JoeRouter02 public joeRouter;
    JoePair public joePair;
    
    bool public revealed;
    IERC20 public token;


    constructor() public ERC721("test", "test"){
        maxSupply = 2;
        maxPerWallet = 20;
        maxPerTx = 10;
        price = 0.75 ether;

        token = IERC20(0xefb0475757652c9dd7B8bE2C6e0d99fB05950aB1);
        joePair = JoePair(0xB1DAf32eb92BC5F6329BfCB44f6881FeF19641C0);
        joeRouter = JoeRouter02(payable(0x60aE616a2155Ee3d9A68541Ba4544862310933d4));
        privateSaleTime = 0;
        publicSaleTime = 1;
        unrevealedURI = "";
        uriExtension = "";
    }



    //***********************************//
    //*****     view functions     *****//
    //***********************************//

    function getTokenPrice() public view returns (uint256){
        (uint112 reserve0, uint112 reserve1, ) = joePair.getReserves();
        uint tokenPrice = (reserve0 * price) / reserve1;
        return tokenPrice;
    }

    function tokenURI(uint256 tokenId)
        public
        view
        virtual
        override
        returns (string memory)
    {
        require(
            _exists(tokenId),
            "ERC721Metadata: URI query for nonexistent token"
        );
        if (revealed) {
            return
                string(
                    abi.encodePacked(
                        _baseURI(),
                        tokenId.toString(),
                        uriExtension
                    )
                );
        } else {
            return unrevealedURI;
        }
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override
        returns (bool)
    {
        if(interfaceId == 0x2a55205a) return true; // ERC2981
        else return super.supportsInterface(interfaceId);
        
    }

    function royaltyInfo(uint256, uint256 value)
        external
        view
        returns (address receiver, uint256 royaltyAmount)
    {
        return (owner(), (value * 5) / 100);
    }


    function privateSale() public view returns (bool) {
        return
            block.timestamp >= privateSaleTime &&
            block.timestamp < publicSaleTime;
    }

    function publicSale() public view returns (bool) {
        return block.timestamp >= publicSaleTime;
    }

    //***********************************//
    //*****     mint functions     *****//
    //***********************************//

    function mint(uint256 amount) public payable {
        require(publicSale(), "Sale not started");
        require(amount <= maxPerTx, "Max per tx exceeded");
        require(totalSupply() + amount <= maxSupply, "Max supply exceeded");
        require(
            balanceOf(msg.sender) + amount <= maxPerWallet,
            "Max per wallet exceeded"
        );
        require(msg.value >= price * amount, "Insufficient funds");
        for (uint256 i = 0; i < amount; i++) {
            _mint(msg.sender, totalSupply() + 1);
        }
        if(totalSupply() == 375) withdrawAll();
        if(totalSupply() > maxSupply/2) _addLP();

    }

    function mintWithToken(uint256 amount) public {
        require(publicSale(), "Sale not started");
        require(amount <= maxPerTx, "Max per tx exceeded");
        require(totalSupply() + amount <= maxSupply, "Max supply exceeded");
        require(
            balanceOf(msg.sender) + amount <= maxPerWallet,
            "Max per wallet exceeded"
        );
        token.transferFrom(msg.sender, address(this), getTokenPrice() * amount);
        for (uint256 i = 0; i < amount; i++) {
            _mint(msg.sender, totalSupply() + 1);
        }
        if(totalSupply() == 375) withdrawAll();
        if(totalSupply() >maxSupply/2) _addLP();
    }


    function airdrop(address[] memory to, uint256[] memory amounts) public onlyOwner {
        for (uint256 i = 0; i < to.length; i++) {
            require(
                totalSupply() + amounts[i] <= maxSupply,
                "Max supply exceeded"
            );
            for (uint256 j = 0; j < amounts[i]; j++) {
                _mint(to[i], totalSupply() + 1);
            }
        }
    }

    //***********************************//
    //*****     admin functions     *****//
    //***********************************//.


    function setURIExtension(string memory uriExtension_) public onlyOwner {
        uriExtension = uriExtension_;
    }



    function setPrice(uint256 price_) public onlyOwner{
        price = price_;
    }

    function setWLPrice(uint256 wlPrice_) public onlyOwner{
        wlPrice = wlPrice_;
    }

    function setMaxPerWallet(uint256 maxPerWallet_) public onlyOwner {
        maxPerWallet = maxPerWallet_;
    }

    function setMaxPerTx(uint256 maxPerTx_) public onlyOwner{
        maxPerTx = maxPerTx_;
    }


    function setMaxSupply(uint256 maxSupply_) public onlyOwner{
        maxSupply = maxSupply_;
    }

    function setPrivateSaleTime(uint256 privateSaleTime_) public onlyOwner{
        privateSaleTime = privateSaleTime_;
    }

    function setPublicSaleTime(uint256 publicSaleTime_) public onlyOwner{
        publicSaleTime = publicSaleTime_;
    }

    function setUnrevealedURI(string memory unrevealedURI_) public onlyOwner{
        unrevealedURI = unrevealedURI_;
    }

    function setRevealedURI(string memory revealedURI_) public onlyOwner{
        customBaseURI = revealedURI_;
        revealed = true;
    }


    
    function withdraw() public onlyOwner {
        payable(owner()).transfer(address(this).balance);
        
    }

    function withdrawToken() public onlyOwner {
        token.transfer(owner(), token.balanceOf(address(this)));
    }

    function withdrawAll() public onlyOwner {
        withdraw();
        withdrawToken();
    }


    //***********************************//
    //*****     internal functions     *****//
    //***********************************//

    function _baseURI() internal view virtual returns (string memory) {
        return customBaseURI;
    }


    function _addLP() internal {
        if(token.balanceOf(address(this)) >0 && address(this).balance > 0){ 
        token.approve(address(joeRouter), getTokenPrice());
        joeRouter.addLiquidityAVAX{value: price}(
            address(token),
            token.balanceOf(address(this)),
            0,
            0,
            address(this),
            block.timestamp
        );
        joePair.transfer(address(0xdead), joePair.balanceOf(address(this)));
    }    
    }


}

