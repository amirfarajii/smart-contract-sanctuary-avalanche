pragma solidity 0.8.14;

import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/StringsUpgradeable.sol";
import "./interfaces/IBRAINMaintenanceUpgradeable.sol";
import "./interfaces/IBRAINNFTUpgradeable.sol";
import "./interfaces/IBRAINRewardsUpgradeable.sol";
import "./interfaces/IMINDv2.sol";
import "./utils/ProxyableUpgradeable.sol";
import "./utils/WithdrawableUpgradeable.sol";

/*  */

contract BRAINManagementUpgradeable is
    Initializable,
    OwnableUpgradeable,
    ReentrancyGuardUpgradeable,
    ProxyableUpgradeable,
    WithdrawableUpgradeable
{
    using SafeERC20Upgradeable for IERC20Upgradeable;

    IBRAINMaintenanceUpgradeable public brainMaintenanceContract;
    IBRAINNFTUpgradeable public brainNFTContract;
    IBRAINRewardsUpgradeable public brainRewardsContract;
    address public maintenanceFeeToken;
    address public mindContract;
    address public paymentToken;
    address public treasury;

    uint256 public brainPrice;
    uint256 public maintenanceFee;
    uint256 public maxBRAINsPerUser;
    uint256 public maxBRAINs;
    uint256 public maxBRAINsPerMint;
    uint256 public maxBRAINSToProcess;
    uint256 public minTokensForReferral;
    uint256 public minBRAINsForReferral;
    uint256 public referralRateForBRAINCreation;

    event CreateBRAINsWithTokens(
        address indexed user,
        string[] indexed names,
        uint256 indexed amount
    );
    event Mint(
        address indexed user,
        string[] indexed names,
        uint256 indexed amount
    );

    error ExceedsMaxBRAINsPerUser();
    error ExceedsMaxBRAINsToProcess();
    error ExceedsMaxMint();
    error ExceedsMaxPerMint();
    error ExceedsMaxValue(uint256 value);
    error InconsistentArrayLengths(uint array1, uint array2);
    error NotApproved();
    error NotFeeMultiplier();
    error NotOwner(uint256 tokenID);

    function initialize(address _treasury, address _paymentToken)
        public
        checkZeroAddress(_treasury)
        initializer
    {
        OwnableUpgradeable.__Ownable_init();
        treasury = _treasury;
        maxBRAINSToProcess = 20;
        maxBRAINsPerUser = 500;
        maxBRAINsPerMint = 5;
        brainPrice = 125 ether;
        maintenanceFee = 500000000000000000; // 0.5 MIND+
        paymentToken = _paymentToken;
        minTokensForReferral = 100 ether;
        minBRAINsForReferral = 1;
        referralRateForBRAINCreation = 200; // 2%
    }

    modifier onlyApproved() {
        if (_msgSender() != owner() && !proxyToApproved[_msgSender()])
            revert NotApproved();
        _;
    }

    /** MANAGEMENT **/

    function setProxyState(address proxyAddress, bool value)
        public
        override
        onlyOwner
    {
        proxyToApproved[proxyAddress] = value;
        if (address(brainNFTContract) != address(0))
            brainNFTContract.setProxyState(proxyAddress, value);
        if (address(brainMaintenanceContract) != address(0))
            brainMaintenanceContract.setProxyState(proxyAddress, value);
        if (address(brainRewardsContract) != address(0))
            brainRewardsContract.setProxyState(proxyAddress, value);
    }

    function initializeReferences(
        address brainMaintenance,
        address brainNFT,
        address brainRewards,
        address mindToken
    )
        external
        checkZeroAddress(brainMaintenance)
        checkZeroAddress(brainNFT)
        checkZeroAddress(brainRewards)
        onlyApproved
    {
        brainMaintenanceContract = IBRAINMaintenanceUpgradeable(
            brainMaintenance
        );
        brainNFTContract = IBRAINNFTUpgradeable(brainNFT);
        brainRewardsContract = IBRAINRewardsUpgradeable(brainRewards);

        brainMaintenanceContract.setBRAINManagementContract(address(this));
        brainMaintenanceContract.setBRAINNFTContract(brainNFT);
        brainMaintenanceContract.setBRAINRewardsContract(brainRewards);

        brainNFTContract.setBRAINManagementContract(address(this));

        brainRewardsContract.setBRAINManagementContract(address(this));
        brainRewardsContract.setBRAINMaintenanceContract(brainMaintenance);
        brainRewardsContract.setBRAINNFTContract(brainNFT);

        brainRewardsContract.setProxyState(
            address(brainMaintenanceContract),
            true
        );
        mindContract = mindToken;
    }

    function setBRAINNFTContract(address value)
        external
        checkZeroAddress(value)
        onlyApproved
    {
        brainNFTContract = IBRAINNFTUpgradeable(value);

        brainNFTContract.setBRAINManagementContract(address(this));

        brainMaintenanceContract.setBRAINNFTContract(value);
        brainRewardsContract.setBRAINNFTContract(value);
    }

    function setBRAINRewardsContract(address value)
        external
        checkZeroAddress(value)
        onlyApproved
    {
        brainRewardsContract = IBRAINRewardsUpgradeable(value);

        brainMaintenanceContract.setBRAINRewardsContract(value);

        brainRewardsContract.setBRAINManagementContract(address(this));
        brainRewardsContract.setBRAINNFTContract(address(brainNFTContract));
        brainRewardsContract.setBRAINMaintenanceContract(
            address(brainMaintenanceContract)
        );

        brainRewardsContract.setProxyState(
            address(brainMaintenanceContract),
            true
        );
    }

    function setBRAINMaintenanceContract(address value)
        external
        checkZeroAddress(value)
        onlyApproved
    {
        brainMaintenanceContract = IBRAINMaintenanceUpgradeable(value);

        brainMaintenanceContract.setBRAINManagementContract(address(this));
        brainMaintenanceContract.setBRAINNFTContract(address(brainNFTContract));
        brainMaintenanceContract.setBRAINRewardsContract(
            address(brainRewardsContract)
        );

        brainRewardsContract.setBRAINMaintenanceContract(value);
        brainRewardsContract.setProxyState(
            address(brainMaintenanceContract),
            true
        );
    }

    function setMindTokenContract(address value)
        external
        checkZeroAddress(value)
        onlyApproved
    {
        mindContract = value;
    }

    /** BRAIN NFT CONTRACT */

    function balanceOf(address account) external view returns (uint256) {
        return brainNFTContract.balanceOf(account);
    }

    function createBRAINsWithTokens(string[] calldata names, uint256 amount)
        external
    {
        if (amount > maxBRAINsPerMint) revert ExceedsMaxPerMint();
        _mint(_msgSender(), names, amount, brainPrice * amount);
        emit CreateBRAINsWithTokens(_msgSender(), names, amount);
    }

    function getBRAINNames(uint256[] calldata brainIDs)
        external
        view
        returns (string[] memory)
    {
        return brainNFTContract.getBRAINNames(brainIDs);
    }

    function getNFTInfos(uint[] memory tokenIDs)
        external
        view
        returns (IBRAINNFTUpgradeable.NFTInfo[] memory)
    {
        return brainNFTContract.getNFTInfos(tokenIDs);
    }

    function isValidName(string memory name) external view returns (bool) {
        return brainNFTContract.isValidName(name);
    }

    function mint(
        address recipient,
        string[] calldata names,
        uint256 amount
    ) external onlyApproved {
        _mint(recipient, names, amount, 0);
        emit Mint(recipient, names, amount);
    }

    function renameBRAIN(uint256 tokenID, string calldata value) external {
        brainNFTContract.renameBRAIN(_msgSender(), tokenID, value);
    }

    function setBRAINInfo(
        uint256 brainID,
        uint256 propertyNum1,
        uint256 propertyNum2,
        uint256 propertyNum3,
        string memory propertyString1,
        string memory propertyString2
    ) external onlyApproved {
        brainNFTContract.setNFTInfo(
            brainID,
            propertyNum1,
            propertyNum2,
            propertyNum3,
            propertyString1,
            propertyString2
        );
    }

    function setBRAINNumericInfo(
        uint256 brainID,
        uint256 propertyNum,
        uint256 value
    ) external onlyApproved {
        brainNFTContract.setNFTNumericInfo(brainID, propertyNum, value);
    }

    function setBRAINStringInfo(
        uint256 brainID,
        uint256 propertyNum,
        string memory value
    ) external onlyApproved {
        brainNFTContract.setNFTStringInfo(brainID, propertyNum, value);
    }

    function totalSupply() external view returns (uint256) {
        return brainNFTContract.getOwnerCount();
    }

    function tokenExists(uint256 tokenID) external view returns (bool) {
        return brainNFTContract.tokenExists(tokenID);
    }

    /** BRAIN MAINTENANCE FEE CONTRACT */

    function brainsToFees(uint256 brainID)
        external
        view
        returns (IBRAINMaintenanceUpgradeable.BRAINInfo memory)
    {
        return brainMaintenanceContract.brainsToFees(brainID);
    }

    // returns arrays of intervalsActive and intervalsPaid, in order of BRAINIDs
    function getPendingFees(uint256[] calldata brainIDs)
        external
        view
        returns (uint256[] memory, uint256[] memory)
    {
        return brainMaintenanceContract.getPendingFees(brainIDs);
    }

    function maintenanceInterval() external view returns (uint256) {
        return brainMaintenanceContract.interval();
    }

    function payMaintenanceFees(
        uint256[] calldata brainIDs,
        uint256[] calldata intervals,
        bool usePendingRewards
    ) external {
        if (brainIDs.length > maxBRAINSToProcess)
            revert ExceedsMaxBRAINsToProcess();
        if (brainIDs.length != intervals.length)
            revert InconsistentArrayLengths(brainIDs.length, intervals.length);
        uint256 totalIntervals;
        for (uint256 x; x < intervals.length; x++) {
            if (brainNFTContract.ownerOf(brainIDs[x]) != _msgSender())
                revert NotOwner(brainIDs[x]);
            totalIntervals += intervals[x];
        }
        uint256 amount = totalIntervals * maintenanceFee;
        uint256 refund;
        if (usePendingRewards) {
            uint256 rewards = brainRewardsContract
                .payMaintenanceFeesWithRewards(brainIDs);
            if (rewards > amount) {
                refund = rewards - amount;
                brainRewardsContract.sendRewards(address(this), amount);
                amount = 0;
            } else {
                amount -= rewards;
                brainRewardsContract.sendRewards(address(this), rewards);
            }
        }
        if (amount > 0) {
            IERC20Upgradeable(maintenanceFeeToken).safeTransferFrom(
                _msgSender(),
                treasury,
                amount
            );
        }
        brainMaintenanceContract.payMaintenanceFees(
            _msgSender(),
            brainIDs,
            intervals
        );
        if (refund > 0) {
            brainRewardsContract.sendRewards(_msgSender(), refund);
        }
    }

    function setMaintenanceInterval(uint256 seconds_) external onlyApproved {
        brainMaintenanceContract.setInterval(seconds_);
    }

    function setMaintenanceFeeToken(address value)
        external
        checkZeroAddress(value)
        onlyApproved
    {
        maintenanceFeeToken = value;
    }

    function setMaintenanceFee(uint256 value) external onlyApproved {
        maintenanceFee = value;
    }

    /** BRAIN REWARDS CONTRACT **/

    function accRewardsPerShare() external view returns (uint256) {
        return brainRewardsContract.accRewardsPerShare();
    }

    function brainToRewardDebt(uint256 brainID)
        external
        view
        returns (IBRAINRewardsUpgradeable.BRAINRewardInfo memory)
    {
        return brainRewardsContract.brainToRewardDebt(brainID);
    }

    function cashoutBRAINRewards(uint256[] calldata brainIDs)
        external
        returns (uint256 rewards)
    {
        return brainRewardsContract.cashoutBRAINRewards(_msgSender(), brainIDs);
    }

    function cashoutPartialBRAINRewards(
        uint256[] calldata brainIDs,
        uint256[] calldata partialRewards
    ) external returns (uint256 rewards) {
        return
            brainRewardsContract.cashoutPartialBRAINRewards(
                _msgSender(),
                brainIDs,
                partialRewards
            );
    }

    function cashoutDisabled() external view returns (bool) {
        return brainRewardsContract.cashoutDisabled();
    }

    function endTimestamp() external view returns (uint256) {
        return brainRewardsContract.endTimestamp();
    }

    function minRewardsPerDay() external view returns (uint256) {
        return brainRewardsContract.minRewardsPerDay();
    }

    function maxRewardsPerDay() external view returns (uint256) {
        return brainRewardsContract.maxRewardsPerDay();
    }

    function pendingBRAINRewards(uint256[] calldata brainIDs)
        external
        view
        returns (uint256 rewards)
    {
        return brainRewardsContract.pendingBRAINRewards(brainIDs);
    }

    function rewardsPerSecond() external view returns (uint256) {
        return brainRewardsContract.rewardsPerSecond();
    }

    function rewardToken() external view returns (address) {
        return brainRewardsContract.rewardToken();
    }

    function setBRAINRewardToZero(uint256 brainID) external {
        brainRewardsContract.setBRAINRewardToZero(brainID);
    }

    function setBRAINRewardsToZero(uint256[] calldata brainIDs) external {
        brainRewardsContract.setBRAINRewardsToZero(brainIDs);
    }

    function setCashoutDisabled(bool value) external onlyApproved {
        brainRewardsContract.setCashoutDisabled(value);
    }

    function setEndTimestamp(uint256 value) external onlyApproved {
        brainRewardsContract.setEndTimestamp(value);
    }

    function setMaxRewardsPerDay(uint256 value) external onlyApproved {
        brainRewardsContract.setMaxRewardsPerDay(value);
    }

    function setMinRewardsPerDay(uint256 value) external onlyApproved {
        brainRewardsContract.setMinRewardsPerDay(value);
    }

    function setRewardsPerSecond(uint256 value) external onlyApproved {
        brainRewardsContract.setRewardsPerSecond(value);
    }

    function setRewardToken(address value)
        external
        checkZeroAddress(value)
        onlyApproved
    {
        brainRewardsContract.setRewardToken(value);
    }

    function totalRewardsClaimed() external view returns (uint256) {
        return brainRewardsContract.totalRewardsClaimed();
    }

    function updateRewards() external {
        brainRewardsContract.updateRewards();
    }

    /** ADMIN */

    function setBRAINPrice(uint256 value) external onlyApproved {
        brainPrice = value;
    }

    function setMaxBRAINsPerUser(uint256 value) external onlyApproved {
        maxBRAINsPerUser = value;
    }

    function setMaxBRAINs(uint256 value) external onlyApproved {
        maxBRAINs = value;
    }

    function setMaxBRAINsPerMint(uint256 value) external onlyApproved {
        maxBRAINsPerMint = value;
    }

    function setMaxBRAINsToProcess(uint256 value) external onlyApproved {
        maxBRAINSToProcess = value;
    }

    function setMinTokensForReferral(uint256 amount) external onlyApproved {
        minTokensForReferral = amount;
    }

    function setMinBRAINsForReferral(uint256 amount) external onlyApproved {
        minBRAINsForReferral = amount;
    }

    function setPaymentToken(address value)
        external
        checkZeroAddress(value)
        onlyApproved
    {
        paymentToken = value;
    }

    function setreferralRateForBRAINCreation(uint256 value) external onlyOwner {
        if (value > 1000) revert ExceedsMaxValue(1000);
        referralRateForBRAINCreation = value;
    }

    function setTreasury(address value)
        external
        checkZeroAddress(value)
        onlyApproved
    {
        treasury = value;
    }

    // withdraw native to treasury
    function withdrawNativeToTreasury() external onlyApproved {
        _withdrawNativeToTreasury(treasury);
    }

    // withdraw native to treasury
    function withdrawTokensToTreasury(address tokenAddress)
        external
        onlyApproved
    {
        _withdrawTokensToTreasury(treasury, tokenAddress);
    }

    function _mint(
        address recipient,
        string[] calldata names,
        uint256 amount,
        uint256 tokenAmount
    ) private {
        if (brainNFTContract.getOwnerCount() + amount > maxBRAINs)
            revert ExceedsMaxMint();
        if (names.length != amount)
            revert InconsistentArrayLengths(names.length, amount);
        if (brainNFTContract.balanceOf(recipient) + amount > maxBRAINsPerUser)
            revert ExceedsMaxBRAINsPerUser();
        if (tokenAmount > 0) {
            uint256 total = brainPrice * amount;
            total -= _processReferralFee(
                recipient,
                total,
                referralRateForBRAINCreation
            );
            IERC20Upgradeable(paymentToken).safeTransferFrom(
                recipient,
                address(paymentToken),
                total
            );
        }
        // mint
        uint256[] memory brainIDs = brainNFTContract.batchMint(
            recipient,
            names,
            amount
        );

        brainRewardsContract.initializeBRAINs(brainIDs);
        brainMaintenanceContract.initializeBRAINs(brainIDs);
    }

    function _processReferralFee(
        address user,
        uint256 amount,
        uint256 rate
    ) private returns (uint256 referralAmount) {
        if (rate == 0 || paymentToken != mindContract) return 0;
        address referral = _getReferral(user);
        referralAmount = (amount * rate) / 10000;
        if (referralAmount > 0) {
            IMINDv2(mindContract).updateReferralIncome(
                referral,
                referralAmount
            );
            IERC20Upgradeable(mindContract).safeTransferFrom(
                user,
                referral,
                referralAmount
            );
        }
    }

    function _getReferral(address user)
        private
        view
        returns (address referral)
    {
        referral = IMINDv2(mindContract).Referrals(user);
        referral = referral == address(0) ||
            IERC20Upgradeable(mindContract).balanceOf(referral) <
            minTokensForReferral ||
            brainNFTContract.balanceOf(referral) < minBRAINsForReferral
            ? address(this)
            : referral;
    }
}

